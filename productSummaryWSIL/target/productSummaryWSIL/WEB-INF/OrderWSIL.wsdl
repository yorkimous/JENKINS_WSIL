          <xsd:enumeration value="102">
            <xsd:annotation/>
          </xsd:enumeration>
          <xsd:enumeration value="103">
            <xsd:annotation/>
          </xsd:enumeration>
          <xsd:enumeration value="104">
            <xsd:annotation/>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
