<?xml version='1.0' encoding='UTF-8'?>
<wsdl11:definitions xmlns:wsdl11="http://schemas.xmlsoap.org/wsdl/" xmlns:soap11="http://schemas.xmlsoap.org/wsdl/soap/"
   xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   xmlns:whttp="http://schemas.xmlsoap.org/wsdl/http/" xmlns:mime="http:/schemas.xmlsoap.org/wsdl/mime/"
   xmlns:csom="http://services.tmobile.com/OrderManagement/OrderWSIL/V1"
   xmlns:wsp="http://www.w3.org/ns/ws-policy" targetNamespace="http://services.tmobile.com/OrderManagement/OrderWSIL/V1">
  <wsdl11:documentation>Name: OrderWSIL
Version: 1.18439103.1
TransactionObjects 1.9.3
CSOM 9.22.1
R1.6
Service SRs# 868 and 870 / CSOM CR#805: Included POSTVOID manually to avoid CSOM update. Solution suggested by TMO and agreed by Yoga, Meena and Mark Johnson.
</wsdl11:documentation>
  <!--BEGIN ABSTRACT DESCRIPTION-->
  <wsdl11:types>
    <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xml="http://www.w3.org/XML/1998/namespace"
       elementFormDefault="qualified" attributeFormDefault="unqualified" targetNamespace="http://services.tmobile.com/OrderManagement/OrderWSIL/V1"
       xmlns:csom="http://services.tmobile.com/OrderManagement/OrderWSIL/V1"
       version="1" xmlns:base="http://services.tmobile.com/base">  <!--Schema import TransactionObjects is actually inlined into the service definiton. Changing to namespace import.-->
      <xsd:import namespace="http://services.tmobile.com/base"/>
      <xsd:complexType name="PromotionReference">
        <xsd:annotation>
          <xsd:documentation>A proposition to entice a customer to purchase one or more offers.  The promotion can provide discounted pricing or additional products as the incentive to purchase the offer(s).  The discount or additional product is called the promotion benefit.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="promotionName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Commonly used name of the Promotion, as defined in the Product Catalog.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="promotionDescription" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded"/>
          <xsd:element name="promotionShortDescription" type="csom:Description"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="promotionEffectivePeriod" type="csom:EffectivePeriod"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Time period during which the promotion is offered.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="promotionType" type="csom:PromotionType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The type of promotion.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="promotionShortName" type="xsd:normalizedString"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The brief name of the promotion.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="promotionLongDescription" type="csom:ComplexDescription"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="promotionAlternateDescription" type="csom:ComplexDescription"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Alternative description for the promotion</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="customerFacingCodeType" type="csom:CustomerFacingPromotionCodeType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Valid promotion code type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="csom:ActionCode"/>
        <xsd:attribute name="promotionId" type="csom:PromotionId">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies a promotion.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>A URI that can be used to return information about the promotion.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="PromotionStatus">
        <xsd:annotation>
          <xsd:documentation>The status of promotion.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:annotation>
                <xsd:documentation>A short-hand code to identify the status.  For example, Available, Redeemed.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode">
              <xsd:annotation>
                <xsd:documentation>For future use</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="PromotionIDSystem">
        <xsd:sequence minOccurs="0">
          <xsd:element name="promotionID" type="csom:PromotionId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Pronmotion ID</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="promotionSystem" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The system from which the promotion is initiated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="SerializedPromotionCoupon">
        <xsd:annotation>
          <xsd:documentation>A promotion coupon is used to identify a Product Catalog Promotion. The inventory of serialized promotion codes is managed in the product catalog.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="serialNumber" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The unique id of the promotion code  in the system of record.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The time frame where the coupon is use (effective).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:PromotionStatus" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Status of promotion coupon</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="applicablePromotionId" type="csom:PromotionIDSystem"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Identfier of the Product Catalog Promotions that this serialized promotion is attached to.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="PromotionId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies a Promotion.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="CustomerFacingPromotionCodeType">
        <xsd:annotation>
          <xsd:documentation>Valid promotion code type</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="PromotionCode">
            <xsd:annotation>
              <xsd:documentation>In a purchase transaction, a multi-use promotionCode is required for this promotion.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="CouponCode">
            <xsd:annotation>
              <xsd:documentation>In a purchase transaction, a serializedPromotionCoupon is required for this promotion.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="PromotionOrCouponCode">
            <xsd:annotation>
              <xsd:documentation>In a purchase transaction, EITHER a serializedPromotionCoupon OR a promotionCode is required for this promotion.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="None">
            <xsd:annotation>
              <xsd:documentation>In a purchase transaction, no code is required for this promotion.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="PromotionType">
        <xsd:annotation>
          <xsd:documentation>The type of promotion.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="PromotionalActionType">
        <xsd:annotation>
          <xsd:documentation>Action to be applied as part of the promotion: Discount, Allowance, ProductAddition.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Discount"/>
          <xsd:enumeration value="Allowance"/>
          <xsd:enumeration value="ProductAddition"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="TaxExemptionIndicator">
        <xsd:annotation>
          <xsd:documentation>Indicates whether the customer is tax exempt and identifies the pertinent exemtption.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:token">
            <xsd:attribute name="id" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>Unique identifier for the specific tax exemption that applies to the tax.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:simpleType name="TaxTypeCode">
        <xsd:annotation>
          <xsd:documentation>Tax types: SALES, USE, VAT, etc.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="SALES">
            <xsd:annotation>
              <xsd:documentation>Tax calculated as a percentage of the sales price of taxable goods or services.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="VAT">
            <xsd:annotation>
              <xsd:documentation>Value Added Tax</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="USE">
            <xsd:annotation>
              <xsd:documentation>Flat tax assessed based on use of a resource.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="RESORT">
            <xsd:annotation>
              <xsd:documentation>Tax assessed within specail jurisdiction defined to be a resort.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="PROPERTY">
            <xsd:annotation>
              <xsd:documentation>Tax assessed based on property value</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="INCOME">
            <xsd:annotation>
              <xsd:documentation>Tax assessed based on income level.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="SERVICE"/>
          <xsd:enumeration value="E911"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="JurisdictionType">
        <xsd:annotation>
          <xsd:documentation>The type of tax jurisdiction that the customer has received an exemption for.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="State"/>
          <xsd:enumeration value="County"/>
          <xsd:enumeration value="City"/>
          <xsd:enumeration value="District">
            <xsd:annotation>
              <xsd:documentation>Specially defined district such as Conservation, Utility, or Resort.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="DebitCard">
        <xsd:annotation>
          <xsd:documentation>A debit card provides the cardholder electronic access to his or her bank account(s) at a financial institution.  Some cards may bear a stored value with which a payment is made, while most relay a message to the cardholder's bank to withdraw funds from a payer's designated bank account.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:PaymentCard">
            <xsd:sequence>
              <xsd:element name="PIN" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>PIN Code of the card</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="PaymentCard">
        <xsd:annotation>
          <xsd:documentation>The Payment Card provides the core elements that are shared in common between Credit Card, Debit Card, and Prepaid Card. It can be used when it is not necessary to differentiate between those three.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="typeCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Type of Credit Card. Examples are Visa, MasterCard etc</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="MASTERCARD"/>
                <xsd:enumeration value="VISA"/>
                <xsd:enumeration value="AMERICANEXPRESS"/>
                <xsd:enumeration value="DISCOVER"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="cardNumber" type="csom:CardNumber" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Payment card number</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="cardHolderName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name of the Card Holder</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="companyName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Business Name identified for the card if applicable</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="expirationMonthYear" type="csom:ExpirationMonthYear"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Expiration month and year of the card</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="cardHolderAddress" type="csom:Address" minOccurs="0"/>
          <xsd:element name="cardImage" type="csom:Attachment" minOccurs="0"
             maxOccurs="unbounded"/>
          <xsd:element name="cardHolderFirstName" type="xsd:normalizedString"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>First name of the card holder</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="cardHolderLastName" type="xsd:normalizedString"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Last name of the card holder</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="cardTypeIndicator" type="csom:CardTypeIndicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates which type of card data is being set.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="chargeAccountNumberIndicator" type="csom:ChargeAccountNumberIndicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates what form of card number is presented.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="PrepaidCard">
        <xsd:annotation>
          <xsd:documentation>A Prepaid Card is a type of payment card that accesses a limited funding source. Commonly used to provide banking services to those without access to a bank account or credit card.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:DebitCard"/>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="CreditCard">
        <xsd:annotation>
          <xsd:documentation>holders draw on a credit limit approved by the card-issuer such as a bank, store, or service provider. Cardholders normally must pay for credit card purchases at a later date as determined by their agreement with the card-issuer.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:PaymentCard">
            <xsd:sequence>
              <xsd:element name="cardHolderBillingAddress" type="csom:Address"
                 minOccurs="0"/>
              <xsd:element name="securityCode" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Security Code on the card</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:simpleType name="ChargeAccountNumberIndicator">
        <xsd:annotation>
          <xsd:documentation>Form of card number being presented</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="AccountNumber"/>
          <xsd:enumeration value="TemporaryToken"/>
          <xsd:enumeration value="PermanentToken"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="CardTypeIndicator">
        <xsd:annotation>
          <xsd:documentation>Indicates which type of card data is being set</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="CardData"/>
          <xsd:enumeration value="Track1Data"/>
          <xsd:enumeration value="Track2Data"/>
          <xsd:enumeration value="EncryptedPaymentData"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="CCMaskingType">
        <xsd:annotation>
          <xsd:documentation>The type of masking to be applied to the credit card number for security and privacy.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Last4"/>
          <xsd:enumeration value="Last6"/>
          <xsd:enumeration value="NA"/>
          <xsd:enumeration value="None"/>
          <xsd:enumeration value="First5"/>
          <xsd:enumeration value="First6andLast4"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ExpirationMonthYear">
        <xsd:annotation>
          <xsd:documentation>Expiration month and year for credit card or bank card.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:length value="4"/>
          <xsd:pattern value="[0-9]{4}"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="CreditRatingProvider">
        <xsd:annotation>
          <xsd:documentation>A credit rating agency that provides credit rating information.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="name" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The user-friendly label by which the credit rating provider is commonly known.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An explanation of the credit rating provider.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The time during which the reference data value is valid.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="creditRatingProviderCode" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Commonly used code identifying the credit rating provider.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="creditRatingProviderId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Enterprise-wide unique identifier of the provider of credit rating services.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="OrganizationEmploymentStatistics">
        <xsd:annotation>
          <xsd:documentation>Employment statistics of an organization    </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="totalEmployment" type="csom:TotalEmployment" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The total number of employees in the organization.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="primaryAddressEmployment" type="csom:PrimaryAddressEmployment"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Statistics about employment at the organization's primary location.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="Incorporation">
        <xsd:annotation>
          <xsd:documentation>Details that apply only to organizations that are legally incorporated.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="incorporationDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date on which the organization is legally incorporated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="incorporationLocation" type="xsd:normalizedString"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The incorporation place of the organization. E.g. Dingle,Bangalore</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="incorporationCountryCode" type="csom:CountryCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The country in which the orgnization is incorporated</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="OrganizationCreditRatingStatus">
        <xsd:annotation>
          <xsd:documentation>Condition of an OrganizationCreditRating, at the time expressed in the effectivePeriod.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the object, at the point in time specified by the effectivePeriod.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A finer division of code.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="TotalEmployment">
        <xsd:annotation>
          <xsd:documentation>The total number of employees in the organization.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="employeeCount" type="xsd:integer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total number of personnel employeed</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="subsidiaryCountIncludedIndicator" type="xsd:boolean"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether subsidiaries are included in the calculation of total employees</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="estimatedCountIndicator" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the employee total is estimated</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="employmentCountCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the number is a minimum, maximum, or average number of total employees</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="OrganizationCreditRating">
        <xsd:sequence minOccurs="0">
          <xsd:element name="description" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A textual description of the rating effort</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ratingCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The rating company's rating number or code</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ratingDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date that the rating was acquired     </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="debarmentIndicator" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the company is debarred from doing business with government agencies  </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="debarmentCount" type="xsd:integer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Count of filings by government agencies in the United States that prohibit the party from entering into contracts or transactions with those agencies  </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="lastDebarmentDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date of most recent filing to prohibit the party from entering into contracts or transactions with a government agency in the United States</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:OrganizationCreditRatingStatus"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The state of the customer's credit rating, at the time in effectivePeriod.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="creditRatingProvider" type="csom:CreditRatingProvider"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Credit Rating provider organization name</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="commercialCreditScore" type="xsd:integer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Commercial credit score of the credit rating               </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="failureRiskScore" type="xsd:integer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Failure risk score of the credit rating                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paydexScore" type="xsd:integer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Paydex score of the credit rating             </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="PrimaryAddressEmployment">
        <xsd:annotation>
          <xsd:documentation>Statistics about employment at the organization's primary location.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="employeeCount" type="xsd:integer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Number of employees at the referenced address</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="estimatedCountIndicator" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Qualifies calculation of employees at the primary address as estimated</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="minimumCountIndicator" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Qualifies calculation of employees at the primary address as minimum</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="FEIN">
        <xsd:annotation>
          <xsd:documentation>Federal Employer Identification Number</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:minLength value="4"/>
          <xsd:maxLength value="9"/>
          <xsd:pattern value="([0-9])*"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="BalanceSummaryGroup">
        <xsd:annotation>
          <xsd:documentation>A named group of account balances categorized by type of balance.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="expirationDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date on which the balances in the balance summary group expire. If no expiration date is given for the group, the expiration date may be specified on individual balance summaries contained within the group.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="balanceSummary" type="csom:BalanceSummary" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A summary balance, categorized by balance type. The balance may have a due date or an expiration date, depending on the type of balance. Balances may be either monetary amounts or other quantities.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="name">
          <xsd:annotation>
            <xsd:documentation>The name of the balance summary group.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:token">
              <xsd:enumeration value="VOICE">
                <xsd:annotation/>
              </xsd:enumeration>
              <xsd:enumeration value="DATA">
                <xsd:annotation/>
              </xsd:enumeration>
              <xsd:enumeration value="SMS">
                <xsd:annotation/>
              </xsd:enumeration>
              <xsd:enumeration value="MMS">
                <xsd:annotation/>
              </xsd:enumeration>
              <xsd:enumeration value="APP">
                <xsd:annotation/>
              </xsd:enumeration>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:simpleType name="BalanceType">
        <xsd:annotation>
          <xsd:documentation>The type of balance information given. For example, Initial, Transferred or Remaining.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Initial"/>
          <xsd:enumeration value="Total"/>
          <xsd:enumeration value="Remaining"/>
          <xsd:enumeration value="AvailableCredit"/>
          <xsd:enumeration value="Current"/>
          <xsd:enumeration value="RemainingFinanced"/>
          <xsd:enumeration value="TotalFinanced"/>
          <xsd:enumeration value="Net"/>
          <xsd:enumeration value="Gross"/>
          <xsd:enumeration value="Principal"/>
          <xsd:enumeration value="Interest"/>
          <xsd:enumeration value="Previous"/>
          <xsd:enumeration value="New"/>
          <xsd:enumeration value="PastDue"/>
          <xsd:enumeration value="Expired"/>
          <xsd:enumeration value="Transferred"/>
          <xsd:enumeration value="Prefunded"/>
          <xsd:enumeration value="Pooled"/>
          <xsd:enumeration value="Shared"/>
          <xsd:enumeration value="Recurring"/>
          <xsd:enumeration value="Reserved">
            <xsd:annotation>
              <xsd:documentation>Once a customer's AutoPay is processed a portion of their payment is held in reserve until their next renewal.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="Certification">
        <xsd:sequence minOccurs="0">
          <xsd:element name="name" type="xsd:normalizedString" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Name of the certification</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="issuedDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date on which the certification was issued
    					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="expirationDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Expiration Date for the certification if applicable</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="issuingAuthorityID" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Authority that issued the certification
    					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="qualifyingGrade" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Grade applicable for the certification - e.g. First Class, Distinction etc</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="NamedText">
        <xsd:sequence minOccurs="0">
          <xsd:element name="name" type="csom:Name" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="text" type="csom:Text" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="helpText" type="csom:Text" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="xsd:token"/>
        <xsd:attribute name="id" type="xsd:token"/>
      </xsd:complexType>
      <xsd:complexType name="RankedSpecificationValue">
        <xsd:sequence minOccurs="0">
          <xsd:element name="name" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the Specification value; a label by which the value is commonly known.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="value" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The value of a named property of an object.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ranking" type="csom:Ranking" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The rank or priority of the named value within the specification group.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="dataType" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The data type of the value.  The recognized data types are specified by the xml w3c standard, minus the namespace prefix.  For example xsd:boolean would be expressed as boolean.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="precision" type="xsd:nonNegativeInteger" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>For a numeric value, how many total digits are allowed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="scale" type="xsd:nonNegativeInteger" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>For a numeric value,the number of digits to the right of the decimal point.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="measurementUnit" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The unit of measure of the value, if applicable.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="currencyCode" type="csom:CurrencyCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The currency of the specification value, if the data type is currency.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="LineLimitCriteria">
        <xsd:annotation>
          <xsd:documentation>Criteria for number of lines</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="minimumNumberOfLines" type="xsd:integer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Minimum number of lines needed for eligibility</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="maximumNumberOfLines" type="xsd:positiveInteger"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Maximum number of lines allowed to be eligible</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="StateCriteria">
        <xsd:annotation>
          <xsd:documentation>The criteria requires the state  be in the eligible list or not in the excluded list.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
          <xsd:element name="eligibleState" type="csom:StateCode" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A state that is eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedState" type="csom:StateCode" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A state that is not eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
      <xsd:complexType name="CustomerSegmentCriteria">
        <xsd:annotation>
          <xsd:documentation>Customers in the included segments, but not in the excluded segments are eligible.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="eligibleSegment" type="csom:RankedSpecificationValue"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Specifies a customer segmentation attribute name and the value that is eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedSegment" type="csom:RankedSpecificationValue"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Specifies a customer segmentation attribute name and the value that is not eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="RankedSpecificationGroup">
        <xsd:annotation>
          <xsd:documentation>A ranked specification group captures name value pairs, but adds the ability to rank both the groups and the values in the list.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="groupName" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the group of specifications.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="groupRanking" type="csom:Ranking" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Rank or priority of the group of specifications.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="specificationValue" type="csom:RankedSpecificationValue"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A named value pair included in the specification group.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="DescriptionByChannel">
        <xsd:annotation>
          <xsd:documentation>Description with Sales Channel Code as an attribute.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="csom:Description">
            <xsd:attribute name="salesChannelCode" type="csom:SalesChannelCode">
              <xsd:annotation>
                <xsd:documentation>Sales channel that will be using the description.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="PostalCodeCriteria">
        <xsd:annotation>
          <xsd:documentation>The criteria requires the postal code be in the eligible list or not in the excluded list.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
          <xsd:element name="eligiblePostalCode" type="csom:PostalCode" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A postal code that is eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedPostalCode" type="csom:PostalCode" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A postal code that is not eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
      <xsd:complexType name="CreditWorthinessCriteria">
        <xsd:annotation>
          <xsd:documentation>The criteria requires the credit worthiness be in the eligible list or not in the excluded list.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
          <xsd:element name="eligibleCreditWorthiness" type="xsd:token" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A credit worthiness value that is eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedCreditWorthiness" type="xsd:token" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A credit worthiness value that is not eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
      <xsd:complexType name="OrderTypeCriteria">
        <xsd:annotation>
          <xsd:documentation>Criteria for order type</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="includedOrderType" type="csom:OrderType" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Order type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedOrderType" type="csom:OrderType" minOccurs="0"
             maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ChannelCriteria">
        <xsd:annotation>
          <xsd:documentation>The criteria requires the channel be in the eligible list or not in the excluded list.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
          <xsd:element name="eligibleChannel" type="csom:SalesChannelReference"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="excludedChannel" type="csom:SalesChannelReference"
             minOccurs="0" maxOccurs="unbounded"/>
        </xsd:choice>
      </xsd:complexType>
      <xsd:complexType name="UnitPrice">
        <xsd:annotation>
          <xsd:documentation>Unit price type.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="amount" type="csom:Amount" minOccurs="0"/>
          <xsd:element name="quantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The quantity of goods for which the unit price applies. Assumed to be one, unless specified otherwise.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="CountyCriteria">
        <xsd:annotation>
          <xsd:documentation>The criteria requires the county be in the eligible list or not in the excluded list.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
          <xsd:element name="eligibleCountyName" type="xsd:token" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A county that is eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedCountyName" type="xsd:token" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A county that is not eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
      <xsd:complexType name="CityCriteria">
        <xsd:annotation>
          <xsd:documentation>The criteria requires the city be in the eligible list or not in the excluded list.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
          <xsd:element name="eligibleCity" type="csom:CityName" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The name of a city that is eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedCity" type="csom:CityName" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The name of a city that is excluded.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
      <xsd:complexType name="Measure">
        <xsd:simpleContent>
          <xsd:extension base="xsd:decimal">
            <xsd:attribute name="measurementUnit" type="csom:MeasurementUnit">
              <xsd:annotation>
                <xsd:documentation>Unit of measure.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="Text">
        <xsd:annotation>
          <xsd:documentation>A character string.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:normalizedString">
            <xsd:attribute name="languageCode" type="csom:LanguageCode">
              <xsd:annotation/>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="StoreCriteria">
        <xsd:annotation>
          <xsd:documentation>The criteria requires the store ID to be in the eligible list, or not in the excluded list. If neither list is populated, all stores are eligible.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
          <xsd:element name="eligibleStore" type="csom:StoreId" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>If the store ID is in the eligible list, the store is eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedStore" type="csom:StoreId" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>If the store ID is in the excluded list, the store is excluded from eligibility.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
      <xsd:complexType name="ComplexDescription">
        <xsd:sequence minOccurs="0">
          <xsd:element name="descriptionCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A code identifying the description.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="salesChannelCode" type="csom:SalesChannelCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A code identifying the sales channel where the description will be used.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="languageCode" type="csom:LanguageCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A code identifying the language used in the decription text per ISO 639-1 Code: en for English, es for espagnol (Spanish). Reference http://www.iso.org/iso/home/standards/language_codes.htm</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="relativeSize" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The relative size of the description text, as in Short or Long.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="Short"/>
                <xsd:enumeration value="Medium"/>
                <xsd:enumeration value="Long"/>
                <xsd:enumeration value="Abbreviation"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="contentType" type="csom:ContentTypeCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The type of content in the description, as in XML, Text, HTML.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="descriptionText" type="xsd:string" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The textual content of the description.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="DealerCodeCriteria">
        <xsd:annotation>
          <xsd:documentation>Criteria for dealer code</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="includedDealerCode" type="csom:CommissionsCode"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Dealer identification</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedDealerCode" type="csom:CommissionsCode"
             minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ContractTypeCriteria">
        <xsd:annotation>
          <xsd:documentation>Criteria for contract type</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="includedContractType" type="csom:DealerContractType"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Contract type with the dealer</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedContractType" type="csom:DealerContractType"
             minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="LimitRange">
        <xsd:sequence minOccurs="0">
          <xsd:element name="maximumQuantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Maximum Quantity on the Limit Range</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="minimumQuantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Minimum Quantity on the Limit Range</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="thresholdQuantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Threshold Quantity on the Limit Range</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="Rate">
        <xsd:annotation>
          <xsd:documentation>Numeric information that is assigned or is determined by calculation, counting, or sequencing. It does not require a unit of quantity or unit of measure. Expressed as a quantity per quantity.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="csom:Quantity">
            <xsd:attribute name="baseMeasurementUnit" type="csom:MeasurementUnit">
              <xsd:annotation>
                <xsd:documentation>The measurement unit of the denominator of the definition of the rate.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="baseValue" type="xsd:decimal">
              <xsd:annotation>
                <xsd:documentation>The value of the denominator of the defintion of the rate.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="AccessoryOfferingReference">
        <xsd:annotation>
          <xsd:documentation>DEPRECATED</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:SimpleProductOfferingReference"/>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="TheftLockStatus">
        <xsd:annotation>
          <xsd:documentation>Status of theft lock</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="ON"/>
                  <xsd:enumeration value="OFF"/>
                  <xsd:enumeration value="LOST"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="DeviceBlock">
        <xsd:annotation>
          <xsd:documentation>Device block details</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="status" type="csom:DeviceBlockStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Status of device block</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="blockedByOtherServiceProvider" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if the device is blocked by other service provider</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="serviceProviderName" type="xsd:normalizedString"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of service provider.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="serviceProviderId" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identification of service provider</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="userId" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identification of user</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="blockTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>When the device is blocked</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="nationalBlockIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicate if the device is blocked nationaly</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="DeviceBlockStatus">
        <xsd:annotation>
          <xsd:documentation>Device block status</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="WarrantyStatus">
        <xsd:annotation>
          <xsd:documentation>Status of warranty</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="AssignedProductProcessingStatus">
        <xsd:annotation>
          <xsd:documentation>State of the processing of an AssignedProduct, at the time expressed in the effectivePeriod.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the object, at the point in time specified by the effectivePeriod.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="INSTALLED"/>
                  <xsd:enumeration value="IN_TRANSIT"/>
                  <xsd:enumeration value="UPGRADED"/>
                  <xsd:enumeration value="RETURNED_FOR_REPAIR"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A finer division of code.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="Warranty">
        <xsd:annotation>
          <xsd:documentation>Details of the warranty in effect for customer-owned equipment (device or accessory).</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="merchandiseProvider" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Service provider through which this device was sold </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="configurationDescription" type="csom:Description"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Describes the model and additional characteristics of the device.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="manufacturerCoveragePeriod" type="csom:EffectivePeriod"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Limited warranty coverage period by manufacturer based on purchase of handset</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="extendedCoveragePeriod" type="csom:EffectivePeriod"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Extended warranty coverage period as determined by the purchase of a warranty program. </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="warrantyExpirationDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Represents the warranty end date of the device or accessory.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="daysRemaining" type="xsd:int" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Number of days left under existing warranty. </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="estimatedPurchaseDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Estimated date of purchase for warrany purposes</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="purchaseCountry" type="csom:CountryCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Country code identifying where the device purchase was made. </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="registrationDate" type="xsd:date" minOccurs="0"/>
          <xsd:element name="premiumCoverageVendor" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the vendor providing premium coverage.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="tmoMerchandiseIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates that this device is sold by T-mobile, though not necessarily purchased from T-Mobile.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="warrantyAttachment" type="csom:Attachment" minOccurs="0"
             maxOccurs="unbounded"/>
          <xsd:element name="warrantySpecification" type="csom:SpecificationGroup"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional properties of the warranty not specified elsewhere.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:WarrantyStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Warranty status</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="parentURI" type="xsd:token"/>
        <xsd:attribute name="referenceURI" type="xsd:token"/>
      </xsd:complexType>
      <xsd:complexType name="TheftLock">
        <xsd:annotation>
          <xsd:documentation>Theft lock details</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="theftLockType" type="csom:TheftLockType" minOccurs="0"/>
          <xsd:element name="lastRetrievalDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The latest date when the theft lock code is retrieved</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:TheftLockStatus" minOccurs="0"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="DeviceRooting">
        <xsd:annotation>
          <xsd:documentation>Device rooting</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="isCurrentlyRooted" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if the device is currently rooted</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="currentRootDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Current root date</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="everRooted" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if any rooting is performed in the device </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="lastRootingDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The latest date when rooting is performed in the device</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ReturnChargeStatus">
        <xsd:annotation>
          <xsd:documentation>Charge status on the returned device.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="DepositRetained"/>
                  <xsd:enumeration value="ChargeApplied"/>
                  <xsd:enumeration value="NotCharged"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="AssignedProductStatus">
        <xsd:annotation>
          <xsd:documentation>Condition of an AssignedProduct, at the time expressed in the effectivePeriod.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the object, at the point in time specified by the effectivePeriod.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="ACTIVE"/>
                  <xsd:enumeration value="SUSPENDED"/>
                  <xsd:enumeration value="CANCELLED"/>
                  <xsd:enumeration value="PENDING"/>
                  <xsd:enumeration value="EXPIRED"/>
                  <xsd:enumeration value="CHARGEBACK"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode">
              <xsd:annotation>
                <xsd:documentation>A finer division of code.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="LOST"/>
                  <xsd:enumeration value="STOLEN"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:simpleType name="AssignedProductActionCode">
        <xsd:annotation>
          <xsd:documentation>Specifies the action to be taken with the AssignedProduct object as a result of an operation with the action code as an input.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="ADD"/>
          <xsd:enumeration value="REMOVE"/>
          <xsd:enumeration value="RETAIN"/>
          <xsd:enumeration value="UPDATE"/>
          <xsd:enumeration value="CANCEL"/>
          <xsd:enumeration value="SUSPEND"/>
          <xsd:enumeration value="RESTORE"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="SerialNumberType">
        <xsd:annotation>
          <xsd:documentation>Serial number type</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="IMEI"/>
          <xsd:enumeration value="ESN_HEX"/>
          <xsd:enumeration value="ESN_NUM"/>
          <xsd:enumeration value="MEID_HEX"/>
          <xsd:enumeration value="MEID_NUM"/>
          <xsd:enumeration value="IMEISV"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="TheftLockType">
        <xsd:annotation>
          <xsd:documentation>Type of theft lock</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="FMIP"/>
          <xsd:enumeration value="ANTITHEFT"/>
          <xsd:enumeration value="BLACKBERRY"/>
          <xsd:enumeration value="WINDOWS"/>
          <xsd:enumeration value="OTHER"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="AssignedProductId">
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:complexType name="SpecialTreatment">
        <xsd:annotation>
          <xsd:documentation>Special treatment as to the financial account from different financial department</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="departmentName" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name of the financial department</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="treatmentType" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Type of the special treatment</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="FinancialAccountBase">
        <xsd:annotation>
          <xsd:documentation>A formal business arrangement providing for regular dealings or services, and involving the establishment and maintenance of a record of debit and credit entries to cover transactions involving a particular Customer.  
    An invoice is prepared relative to a single financial account for a given bill cycle.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:FinancialAccountReference">
            <xsd:sequence>
              <xsd:element name="billingArrangement" type="csom:BillingArrangement"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Options for billing the customer or creating an invoice.  The billing arrangement contains information such as how payment is made (credit card, check, or bank account) and the whether the customer receives a paper and/or electronic bill. </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="currencyCode" type="csom:CurrencyCode" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation> 
    Default Billing Currency for this financial account
                                    </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="preferredLanguage" type="csom:PreferredLanguage"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The preferred language of communication in the specified usage context. ISO 639-1 Code (en-US,en-CA,en-GB)</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="accountHistory" type="csom:ActivityInstance"
                 minOccurs="0" maxOccurs="unbounded"/>
              <xsd:element name="accountBalanceSummaryGroup" type="csom:BalanceSummaryGroup"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation> 
    Capture Account Balance Information for the Customer Account.
                                   </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="accountAttachment" type="csom:Attachment" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation> 
    Attachment for document related to the account.
                                   </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="accountContact" type="csom:Contact" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation> 
    Contact persons on behalf of this customer Party 
                                   </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="creationTime" type="xsd:dateTime" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation> 
    Date when the customer account was originally established
                                    </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="terminateTime" type="xsd:dateTime" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation> 
    Identifies the date when services ordered by the customer will end. 
                                    </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="privacyProfile" type="csom:PrivacyProfile"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Default privacy profile for the contact usage context within the account.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="note" type="csom:Note" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation> 
    Note concerning the account.
                                   </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="accountTenure" type="csom:Tenure" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Duration of the life of the financial account.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
                 minOccurs="0" maxOccurs="unbounded"/>
              <xsd:element ref="csom:paymentProfile" minOccurs="0"/>
              <xsd:element name="spendingLimit" type="csom:Amount" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The spending limit is a threshold set by the customer for a single billing cycle. When total incidental charges on the account for that month exceed the monthly limit, the new charges would not be authorized.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="preferredCommunicationChannel" type="csom:PreferredCommunicationChannel"
                 minOccurs="0" maxOccurs="unbounded"/>
              <xsd:element name="fiServIndicator" type="csom:Indicator" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>FiServe subscription indicator</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="timeZone" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>It indicates the billing account time zone. It is used to adjust the balances reset to the desired time zone. </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="dunningIndicator" type="csom:Indicator" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Dunning Flag. Domain: X - Dunning is activated for this billing account; Null - Dunning is deactivated for this billing account.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="collectionHold" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>A flag is to be set upon putting a collectible collection workflow on Hold , and has to be unset again once hold is released.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="delinquentIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Delinquent flag is set on the first step of the collection workflow. The values for this attribute will be X  set, Or null  unset Also flag is being checked to suppress certain offers for customers having this flag set, this check to be done using OM. Also delinquent flag will be reset once:   the customer set an payment arrangement for the entire due balance OR the Account has been assigned to collection agency within the refer to the collection agency step. N.B.: Delinquency flag will remain set after write-off, till we reach the refer to collection agency step , then it will be unset
    </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="paymentPlanIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>This flag is used to identify the type of payment plan assigned for the account, either: PA: Payment Arrangement EPS: Extended Payment Schedule
    </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="legacyDepositIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>This indicator will be set when deposits are migrated from the legacy system.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="writeOffStatusEditIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Based on the value of this flag (Y/N), a user is allowed to modify the collection status when the account is in Write-off Status.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="fraudIndicator" type="csom:Indicator" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Fraud Indicator for financial account</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="fraudLock" type="csom:Indicator" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>This flag allows setting of fraud lock at billing account level. This must prevent any changes at the billing account level and all contracts under the billing account.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="maximumPrefundAmount" type="csom:Amount" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Specify the maximum prefund amount, only applicable for prepaid accounts (payment option = prepaid)</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="bankruptcyOpenIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Set when bankruptcy is filed by customer.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="bankruptcyDischargeIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Set when bankruptcy is accepted by the Judge. This information is viewable from the UI.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="paymentPlanEligibleIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>This billing account level attribute should be set via the northbound systems when the account is ineligible to enroll into a payment plan (e.g. payment arrangement or EPS). The attributes use to determine the eligibility.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="taxExemption" type="csom:TaxExemption" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>A copy of the tax exemption given to the Customer that is applicable to this billing account.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="programMembership" type="csom:ProgramMembership"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Program(s) in which the customer is participating, e.g. Advantage, Lifeline, TMUS Employee, Rural Healthcare, etc.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="specialTreatment" type="csom:SpecialTreatment"
                 minOccurs="0" maxOccurs="unbounded"/>
              <xsd:element name="fraudRefundBlockIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>To indicate if fraud user can get refunds. If the indicator is set by Fraud Analyst, CSR can not perform any refund activity.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="totalRecurringDueAmount" type="csom:Amount"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Total recurring due amount for all lines on the account</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="customerGroup" type="csom:CustomerGroup" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Customer group which the owner of the financial account is affiliated with.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="fraudLockEffectivePeriod" type="csom:EffectivePeriod"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Specifies the time period during which the account is (or was) locked for suspected fraud.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:simpleType name="FinancialAccountId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies the Customer's financial (billing) account within the billing system.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="FinancialAccountNumber">
        <xsd:annotation>
          <xsd:documentation>Customer billing account number</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:complexType name="RelatedOfferingGroup">
        <xsd:annotation>
          <xsd:documentation>The related offers</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="productOffering" type="csom:ProductOfferingReference"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The related offers</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="scope" type="csom:OfferLevel">
          <xsd:annotation>
            <xsd:documentation>Defines whether the relationship scope is within the same account, line, customer, etc...</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="ServiceCapacity">
        <xsd:annotation>
          <xsd:documentation>ServiceCapacity is a generic type that describe an amount of service (aka a bucket).</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="capacityType" type="csom:UsageType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Type of capacity, or usage type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="capacitySubType" type="csom:UsageSubType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Capacity sub type, or usage sub type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice minOccurs="0">
            <xsd:element name="unlimitedIndicator" type="csom:Indicator" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>indicate the capacity is unlimited</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="size" type="xsd:decimal" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Amount of the capacity</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="measurementUnit" type="csom:UsageMeasurementUnit"
             minOccurs="0"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ProductOfferingComponent">
        <xsd:annotation>
          <xsd:documentation>Product offering components are product offerings that are included within another product offering.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:OfferingComponentBase">
            <xsd:sequence minOccurs="0">
              <xsd:element name="specificationValue" type="base:SpecificationValue"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Named value pairs providing additional information about the product offering component.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="standAloneOfferIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Defines whether an offer can be purchased standalone or only as a part of a package.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="advertizedPrice" type="csom:Amount" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The advertized price is the lead-in price for a packaged offer component.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="packageComponentType" type="csom:PackageComponentType"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>To indicate whether the package component is a single item or a group of items from which the customer can choose</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:choice>
                <xsd:sequence>
                  <xsd:element name="productOffering" type="csom:ProductOfferingReference"
                     minOccurs="0"/>
                  <xsd:element name="offeringVariant" type="csom:OfferingVariant"
                     minOccurs="0"/>
                </xsd:sequence>
                <xsd:element name="optionGroupItem" type="csom:ProductComponentOptionItem"
                   minOccurs="0" maxOccurs="unbounded"/>
              </xsd:choice>
              <xsd:element name="lineGroupNumber" type="xsd:int" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Groups component offers according to line of service.  For example, a packaged offer with 2 plan offers and 2 phone offers could place plan offer A and phone offer B into lineGroupNumber 1, while plan offer C and phone offer D are placed into lineGroupNumber 2.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="MarketingMessage">
        <xsd:sequence minOccurs="0">
          <xsd:element name="salesChannelCode" type="csom:SalesChannelCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The sales channel in which these marketing messages should be used.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="relativeSize" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Describes the relative size of the marketing message. e.g. Small, Medium, Large</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="messagePart" type="csom:MarketingMessagePart" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A message part is one part of the entire marketing message that may be displayed in a list.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ProductOfferingVariant">
        <xsd:annotation>
          <xsd:documentation>Product offering with variant</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:ProductOfferingReference">
            <xsd:sequence>
              <xsd:element name="offeringVariant" type="csom:OfferingVariant"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Describes the detailed information for each variation of the offer.  For example, a device in various memory or color options.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="EquipmentCharacteristics">
        <xsd:annotation>
          <xsd:documentation>Chracterisitics of the product offering that are only used for equipment (device or accessory).</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="model" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Model name of the equipment in the product offering.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="manufacturer" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Maker of the equipment (device or accessory).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="color" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Comma separated list of colors available in the offer variants.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="memory" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Comma separated list of memory sizes available in the offer variants.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="tacCode" type="csom:TacCode" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The Type Allocation Code (TAC) is the initial eight-digit portion of the 15-digit IMEI and 16-digit IMEISV codes used to uniquely identify wireless devices.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="OfferingStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:annotation>
                <xsd:documentation>Code specifying the status of the product offering in the product catalog.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="FORSALE"/>
                  <xsd:enumeration value="NOTFORSALE"/>
                  <xsd:enumeration value="GRANDFATHERED"/>
                  <xsd:enumeration value="COMINGSOON"/>
                  <xsd:enumeration value="PREORDERAVAILABLE"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>Code specifying the sub-status of the product offering in the product catalog.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ScopedOfferingRelationship">
        <xsd:annotation>
          <xsd:documentation>Offer to offer relationships that are evaluated at a specific offer level.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="relatedOffers" type="csom:RelatedOfferingGroup"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The related offers.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="displayPriority" type="csom:Ranking" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Numeric ranking of the product offering relationship, to indicate which relationships are most important, implying that they should be presented to the Customer earlier.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="relationshipSpecificationValue" type="base:SpecificationValue"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional information about the product offering relationship, stored as named value pairs.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="RequiredOffering">
        <xsd:annotation>
          <xsd:documentation>Required offering list</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:ScopedOfferingRelationship">
            <xsd:sequence>
              <xsd:element name="minimumQuantity" type="xsd:positiveInteger"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The minimum number of offers that are required, from the set of offers listed in the productOffering element. Cannot be zero, or the offer would not be required.  For example, if minimumQuantity=1 and maximumQuantity=1 then 1 of the offers in productOffering is required with this offer AND no more than 1 is allowed.  If minimumQuantity=1 and maximumQuantity=2 then one of the offering in productOffering is required but 2 are allowed.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="maximumQuantity" type="xsd:positiveInteger"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The maximum number of offers that are allowed, from the set of offers listed in the productOffering element. For example, if minimumQuantity=1 and maximumQuantity=1 then 1 of the offers in productOffering is required with this offer AND no more than 1 is allowed.  If minimumQuantity=1 and maximumQuantity=2 then one of the offering in productOffering is required but 2 are allowed.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ProductOfferingImageAttachment">
        <xsd:complexContent>
          <xsd:extension base="csom:Attachment">
            <xsd:sequence minOccurs="0">
              <xsd:element name="sku" type="csom:SKU" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>A SKU for which this image can be used.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="imageDimensions" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The dimenions of the image in pixels. e.g. 400x320</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="color" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The color of the product in the image, as named in the official marketing material.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="searchableColor" type="xsd:token" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>A commonly used color name that can be reasonably associated with the appearance of the product, to be used for searches based on color.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="displayPurpose" type="xsd:token" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The purpose that the image will be used for in the user interface. e.g. full front, left side, right side, full back, swatch, etc.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="MarketingMessagePart">
        <xsd:annotation>
          <xsd:documentation>A message part is one part of the entire marketing message that may be displayed in a list.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="code" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An internally used name to identify the marketing message.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="label" type="csom:Name" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A short label to be displayed with the marketing message.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="messageText" type="csom:Text" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The descriptive text for the marketing message. Each message may be available in multiple languages as specified in the language attribute.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="messageSequence" type="csom:Ranking" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Used to sequence the marketing messages when they are displayed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ServiceCharacteristics">
        <xsd:annotation>
          <xsd:documentation>Chracterisitics of the product offering that are only used for network services.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="backDateAllowedIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the service can be back dated when added.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="futureDateAllowedIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the service can be future dated when added.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="backDateVisibleIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the service's back date should be visible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="futureDateVisibleIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the service's future date should be visible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="includedServiceCapacity" type="csom:ServiceCapacity"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The bucket types/amounts for the offer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="billEffectiveCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate when the bill will be effective</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="IMMEDIATE"/>
                <xsd:enumeration value="NEXTBILLCYCLE"/>
                <xsd:enumeration value="ANYFUTUREDATE"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="billableThirdPartyServiceIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if T-Mobile bills the customer for third-party service.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="prorateAllowedIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if prorate is allowed</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="prorateVisibleIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if prorate is visible</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="duration" type="xsd:duration" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The amount of time the customer has to use the includedServiceCapacity before it expires.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="maximumOrderDelay" type="xsd:duration" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Maximum allowed future dating for a particular offer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ProductComponentOptionItem">
        <xsd:annotation>
          <xsd:documentation>Option item of the product component</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="isPreSelected" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the offer component is preselected when displayed to a customer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="displayPriority" type="csom:Ranking" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The sort order of the offer component, relative to the other components of the offer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="advertizedPrice" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A manually configured display price of a component offer, when a part of the packaged offer. The advertised price is the lead-in price for a packaged offer component.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="offsetPrice" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>When the preselectedIndicator is false, this is the difference in price if the customer chooses this member instead of the preselected member. Example if this member is $20 more than thepreselected member, the value is +$20.00. If this member is $10 less than the Selected By Default member, the value is -$10.00.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="productOffering" type="csom:ProductOfferingReference"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The offer for this choice.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="offeringVariant" type="csom:OfferingVariant" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The offer variant for this choice.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="OrderBehavior">
        <xsd:annotation>
          <xsd:documentation>Parameters controlling the sale of an offer in a specifc channel. If no channel is specified, the parameters apply to all channels.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="salesChannelCode" type="csom:SalesChannelCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>DEPRECATED. Use salesChannel.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="preOrderAllowedIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether pre-ordering is allowed before the official launch date of the product in this Sales Channel.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="preOrderAvailableTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date and time when the product becomes available for pre-ordering in this Sales Channel. By definition, this time will always be before the product is available to ship.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="comingSoonDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date when we will beging accepting requests to notify customers of the availability of the product in this Sales Channel.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="backOrderAllowedIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Controls whether back ordering is permitted for this Product Offering in this Sales Channel.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="saleStartTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Time when product can be offered for sale in the specified channel.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="saleEndTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Time when product can no longer be offered for sale in the specified channel.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="retireTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Time when the product is retired.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="offeringStatus" type="csom:OfferingStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The status of the Product Offering in the product catalog in this sales channel.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="salesChannel" type="csom:SalesChannelReference"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Sales channel this order behavior affects.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="dealerCode" type="csom:CommissionsCode" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The master dealer code that this Order Behavior affects.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="contractType" type="csom:DealerContractType" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>the dealer's contract type that this Order Behavior affects.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="OfferingVariant">
        <xsd:annotation>
          <xsd:documentation>Describes the detailed information for each variation of the offer.  For example, a device in various memory or color options.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="sku" type="csom:SKU" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The identifier in the inventory system for the variant of the hard-good offer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional elements of an offer that determine the sku.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="offeringVariantPrice" type="csom:PriceListLine"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Offer prices that are applicable for this variant of the offer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="offeringVariantEligibility" type="csom:EligibilityCriteria"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The eligibility conditions for the variant that differ from the overall offer eligibility.  For example, a device offer may be available in all channels, but  the retail channel may have  only a few of the color/memory options, while web has all color/memory options.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="rankedSpecificationGroup" type="csom:RankedSpecificationGroup"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A ranked specification group captures name value pairs, but adds the ability to rank both the groups and the values in the list.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="productCondition" type="csom:ProductCondition"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The condtion of the product in this offering variant: New or Refurbished.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="color" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The color of the variant. </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="memory" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Comma separated list of memory sizes available in this offer variant.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="tacCode" type="csom:TacCode" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The Type Allocation Code (TAC) is the initial eight-digit portion of the 15-digit IMEI and 16-digit IMEISV codes used to uniquely identify wireless devices.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="displayPriority" type="csom:Ranking" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Determines the order in which the Offering Variants will be displayed within the UI.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="orderBehavior" type="csom:OrderBehavior" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Parameters controlling the sale of an offer variant in a specifc channel. If no channel is specified, the parameters apply to all channels.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="outOfStockIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates that the Offer Variant (SKU) should be marked as Out of Stock in the user interface. When this indicator is set, it implies that the inventory levels for this SKU have fallen below a defined threshold, although not necessarily at zero. The SKU may still be ordered, if backorder is allowed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="selectedSKUIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates the variant is the default selection for the offer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="searchableColor" type="xsd:token" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The color that can be used for search, in the case if variant image is not available. </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="inventoryStatus" type="csom:InventoryStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Inventory status</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="name" type="csom:Name" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name of the offering variant</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:DescriptionByChannel" minOccurs="0"
             maxOccurs="unbounded"/>
          <xsd:element name="shortDescription" type="csom:DescriptionByChannel"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="longDescription" type="csom:ComplexDescription"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The long description of the product offering variant to be used when space allows.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="offeringVariantId" type="csom:OfferingVariantId">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies an offering variant for hard goods.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:simpleType name="OfferingVariantId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies an offering variant for hard goods.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="TacCode">
        <xsd:annotation>
          <xsd:documentation>The Type Allocation Code (TAC) is the initial eight-digit portion of the 15-digit IMEI and 16-digit IMEISV codes used to uniquely identify wireless devices.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="OfferSubType">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="SoftGood"/>
          <xsd:enumeration value="Package"/>
          <xsd:enumeration value="HardGood"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ProductOfferingId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies a Product Offering in the Product Catalog.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="ProductCondition">
        <xsd:annotation>
          <xsd:documentation>The condtion of the product in this offering variant: New or Refurbished.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="New"/>
          <xsd:enumeration value="Refurbished"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="PackageComponentType">
        <xsd:annotation>
          <xsd:documentation>Describes whether the package component is a single item or a group of items from which the customer can choose.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Item"/>
          <xsd:enumeration value="OptionGroup"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="OfferingComponentId">
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="OfferType">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Base"/>
          <xsd:enumeration value="AddOn"/>
          <xsd:enumeration value="OnDemand"/>
          <xsd:enumeration value="TopOff"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="OfferingRelationshipType">
        <xsd:annotation>
          <xsd:documentation>Categorizes the type of relationship between the two product offerings.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="UpSell">
            <xsd:annotation>
              <xsd:documentation>Related offer is for superior product</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="DownSell">
            <xsd:annotation>
              <xsd:documentation>Related offer is for an inferior product</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="CrossSell">
            <xsd:annotation>
              <xsd:documentation>Related offer is for an additional product</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Upgrade">
            <xsd:annotation>
              <xsd:documentation>Related offer is for a better version of the same base product</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Downgrade">
            <xsd:annotation>
              <xsd:documentation>Related offer is for a lesser version of the same base product</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Refurbished">
            <xsd:annotation>
              <xsd:documentation>Related offer is for a refurbished version of the product</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="New">
            <xsd:annotation>
              <xsd:documentation>Related offer is for a new version of the product</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Warning"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="OfferLevel">
        <xsd:annotation>
          <xsd:documentation>Level in the customer hierarchy to which the offer should be applied.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Line">
            <xsd:annotation>
              <xsd:documentation>Offer applies to line of service level.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Account">
            <xsd:annotation>
              <xsd:documentation>Offer applies to account level.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Product">
            <xsd:annotation>
              <xsd:documentation>To be used for legacy offers only.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Transaction"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="ElectronicFundsTransfer">
        <xsd:annotation>
          <xsd:documentation>Electronic Funds Transfer (EFT) is a system of transferring money from one bank account directly to another without any paper money changing hands.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:FinancialInstrument"/>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="BankDraft">
        <xsd:annotation>
          <xsd:documentation>A bank draft is a bill of exchange drawn by a bank on itself, or on a correspondent bank in another city or country. Bank drafts are commonly used by banks in dealings with other banks, or when a creditor or seller is unwilling to accept an ordinary check from a debtor or buyer in another city or country.  Because a draft is certain to be paid, it is generally accepted as a cash equivalent.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:FinancialInstrument"/>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="FinancialInstrument">
        <xsd:annotation>
          <xsd:documentation>Financial Instrument provides the core elements used in common between Check, Bank Draft, and Electronic Funds Transfer. It can be used in situations where it is not necessary to differentiate between those three.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="typeCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Code Type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="accountHolderName" type="csom:PersonName" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Account Holder's name on the payment instrument</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="bankName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Bank on wnich the payment instrument is drawn</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="accountNumber" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Number of payment  instrument</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="routingNumber" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Routing Number of the bank on whom the payment instrument is drawn</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="memo" type="csom:Text" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Memo on the payment instrument</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="issuedDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Issued Date for the payment </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="accountHolderAddress" type="csom:Address" minOccurs="0"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="VerificationQuestion">
        <xsd:annotation>
          <xsd:documentation>A question and list of possible answers for the user to choose from.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="answerSelectionType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates if this is a single selection or multiple selection answer type</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="SINGLE"/>
                <xsd:enumeration value="MULTIPLE"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="questionText" type="csom:Text" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Text for the question that is to be presented to the end-user</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="verificationAnswer" type="csom:VerificationAnswer"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>One of the choices of an answer for a given question. It includes the id of the choice and the text to present to the end-user.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="helpText" type="csom:Text" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Help text that is associated with the question</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="questionId" type="xsd:token"/>
      </xsd:complexType>
      <xsd:complexType name="AuthenticationGroup">
        <xsd:annotation>
          <xsd:documentation>Assigns the user to a group for the purposes of establishing what type of authentication is available to the user.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="authenticationGroupName" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name of the group that is permitted to use this type of authentication.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="authenticationType" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The type of user authentication available to this group.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="QuestionAnswer">
        <xsd:annotation>
          <xsd:documentation>Answer detail</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="answerMessage" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Answer Message</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="answerURLTitle" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>URL title to the answer</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="answerURLLink" type="csom:ReferenceURI" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>URL link to the answer</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="SecurityQuestionSet">
        <xsd:annotation>
          <xsd:documentation>A group or set of questions (and optionally answers) that can be used for security validation either directly by the customer in a self care scenario, or by a customer care representatitve or retail sales representative doing assisted care. Different sets of questions are expected for self care and for assisted care.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="setName" type="csom:Name" minOccurs="0"/>
          <xsd:element name="securityQuestion" type="csom:SecurityQuestion"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A question and answer pair. The answer would only be sent in a message when the customer is initially setting up their answers.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="encryptionFormat" type="csom:EncryptionFormat"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Encryption format used, such as plaintext, encrypted or base64.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="isCaptchaRefRequired" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if CAPTCHA reference is required.  </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="languageCode" type="csom:LanguageCode">
          <xsd:annotation>
            <xsd:documentation>The language selected by the customer for the set of security questions.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>The URI that can be used to return this set of security questions.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="setId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>The name of the set of security questions. Different sets of questions are expected for self care and for assisted care.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="VerificationAnswer">
        <xsd:annotation>
          <xsd:documentation>One of the choices of an answer for a given question. It includes the id of the choice and the text to present to the end-user.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="answerText" type="csom:Text" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Answer text for this choice to be displayed to the user</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="answerId" type="xsd:token"/>
      </xsd:complexType>
      <xsd:complexType name="SecurityQuestion">
        <xsd:annotation>
          <xsd:documentation>A question and answer pair. The answer would only be sent in a message when the customer is initially setting up their answers.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="question" type="csom:Text" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The question to be answered by the customer in the chosen language.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="answer" type="csom:Text" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The answer to the security question would only be sent in a message when the customer is initially setting up their answers.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ranking" type="csom:Ranking" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Specifies the sequence in which the questions will be presented.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:SecurityQuestionStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Status of the security question</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="csom:auditTrail" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies a security question.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="name" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>A unique name identifying the question within the set. The name may be used for selecting the question from a list.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="SecurityProfileStatus">
        <xsd:annotation>
          <xsd:documentation>Status of the security profile</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>Code identifying the status of the security profile</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="VerificationQuestionSet">
        <xsd:annotation>
          <xsd:documentation>A list of questions with multiple choice responses.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="verificationQuestion" type="csom:VerificationQuestion"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A question and list of possible answers for the user to choose from.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="questionName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name of the question</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="answerName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name of the answer</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="reportingAnswer" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Reporting answer</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="customAnswer" type="csom:QuestionAnswer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Custom answer</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="systemAnswer" type="csom:QuestionAnswer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>System answer</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="languageCode" type="csom:LanguageCode"/>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
        <xsd:attribute name="setId" type="xsd:token"/>
      </xsd:complexType>
      <xsd:complexType name="SecurityLineOfService">
        <xsd:annotation>
          <xsd:documentation>Line of service extension</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:LineOfServiceReference">
            <xsd:sequence>
              <xsd:element name="blockedShortCode" type="xsd:normalizedString"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Blocked short code</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="blockedShortCodeType" type="csom:BlockedShortCodeType"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Short code block type.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ResourceEntitlement">
        <xsd:annotation>
          <xsd:documentation>Access permission of the user for the specified resource and action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="base:ResourceAction">
            <xsd:sequence minOccurs="0">
              <xsd:element name="isAccessAllowed" type="xsd:boolean" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Boolean value which says whether access allowed or not.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="responseAttribute" type="base:SpecificationValue"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The name and value of a response attribute.
    </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="SecurityProfile">
        <xsd:annotation>
          <xsd:documentation>Basic security information for user authenticaiton.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="userName" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>User name for logging into a software system.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="password" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Password for logging into a software system.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="loginLocked" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates that the user login is currently locked.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="lockType" type="csom:LockType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Type of locking currently in place for the user login.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="csom:emailAddress" minOccurs="0"/>
          <xsd:element name="securityQuestionSet" type="csom:SecurityQuestionSet"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A group or set of questions (and optionally answers) that can be used for security validation either directly by the customer in a self care scenario, or by a customer care representatitve or retail sales representative doing assisted care. Different sets of questions are expected for self care and for assisted care.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="recoveryPhone" type="csom:PhoneNumber" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The phone number that can be used to send unlock codes or temporary passwords to a customer in the event their account is locked.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="authenticationGroup" type="csom:AuthenticationGroup"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Assigns the user to a group for the purposes of establishing what type of authentication is available to the user.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="commissionsCode" type="csom:CommissionsCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The commissions code may be a dealer code or a rep id, depending on the context.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="passwordResetCount" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The number of times that the user has required a password reset.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="loginFailureCount" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The number of times that the user has attempted to login and failed, since the last successful attempt.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:SecurityProfileStatus" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Status of the security profile</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="loginTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date and time of the user's last successful login.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="activityHistory" type="csom:ActivityInstance" minOccurs="0"
             maxOccurs="unbounded"/>
          <xsd:element name="msisdn" type="csom:MSISDN" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The T-Mobile mobile phone number of the user.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="enhancementReminderDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date on which the user should be reminded about security enhancements.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="resourceEntitlement" type="csom:ResourceEntitlement"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="pin" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Personal identification number. The pin is normally associated to the MSISDN, not the user.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="personName" type="csom:PersonName" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The name of the user, parsed into its name elements.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="accountCreateDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date when the account was created.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="termsAcceptanceDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date when the terms of service were accepted.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="privacyProfile" type="csom:PrivacyProfile" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Privacy controls, indicating the circumstances in which communication is acceptable.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="preferredLanguage" type="csom:LanguageCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The preferred language of the user for communication.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="birthDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date of birth of the user, as reported by the user.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="birthDateLastUpdated" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date when the date of birth was last updated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="firstTimeLoginTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The time the user logged in the first time.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="lockTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date and time when the user's account was locked.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="loginCount" type="xsd:integer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The number of times the user logged in.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="loginLockReason" type="csom:NamedText" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The reason user account login is locked</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="oldEmailAddress" type="csom:EmailAddress" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Email Address used before.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="temporaryPin" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Temporary Personal identification number.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="temporaryPinExpirationTime" type="xsd:dateTime"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Expiration time for the temporaryPin.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="termsAcceptanceIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if the user has accepted terms and conditions.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="pinEncryptionFormat" type="csom:EncryptionFormat"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Encryption format for pin and termporary pin, such as plaintext, encrypted or base64.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="updateEmailScope" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Email address update scope of given security profile, such as updateOnlyProfile, updateLinkedAccounts</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="newEmailAddress" type="csom:EmailAddress" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>New email address of given security profile.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="updatePasswordMethod" type="csom:UpdatePasswordMethod"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To tell how password is updated, such as change_password,reset_password_via_email,reset_password_via_sms,reset_password_via_sq.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="newPassword" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>New password</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="preferredCommunicationChannel" type="csom:CommunicationChannelCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Preferred communication channel on security profile activity, such as Email, SMS</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="offerCommunicationChannel" type="csom:CommunicationChannelCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Communication channel for offer related activity.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="emailNewsLetterAllowed" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if customer allow for news letter email.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="annualEIPDisclosureAllowed" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if customer allow for annual EIP disclosure.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="is2ndFactorValidationRequired" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if second factor validation is required</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="isCaptchaRequiredFor2ndFactorValidation" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if CAPTCHA (Completely Automated Public Turing test to tell Computers and Humans Apart) is required for second factor validation  </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="lineOfService" type="csom:SecurityLineOfService"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Line of service</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="csom:ssn" minOccurs="0"/>
          <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Named value pair</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="userRole" type="xsd:token" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Role of user, such as RetailRep or CareRep.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="profileId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the security profile in the system of record.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>The URI that can be used to return information about this security profile.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="SecurityQuestionStatus">
        <xsd:annotation>
          <xsd:documentation>Status of security question</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:simpleType name="UpdatePasswordMethod">
        <xsd:annotation>
          <xsd:documentation>The method used to update password.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="ChangePassword"/>
          <xsd:enumeration value="ResetPasswordViaEmail"/>
          <xsd:enumeration value="ResetPasswordViaSMS"/>
          <xsd:enumeration value="ResetPasswordViaSQ"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="EncryptionFormat">
        <xsd:annotation>
          <xsd:documentation>Encryption format used, such as plaintext, encrypted or base64.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="PlainText"/>
          <xsd:enumeration value="Encrypted"/>
          <xsd:enumeration value="Base64"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="LockType">
        <xsd:annotation>
          <xsd:documentation>Type of locking currently in place for the user login.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="HARD">
            <xsd:annotation>
              <xsd:documentation>Fraud related</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="SOFT">
            <xsd:annotation>
              <xsd:documentation>Typically due to a number of unsuccessful login attempts</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="MANUALHARD"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="BlockedShortCodeType">
        <xsd:annotation>
          <xsd:documentation>Block type of short code</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="AllowAll"/>
          <xsd:enumeration value="BlockAll"/>
          <xsd:enumeration value="EnforceBlackList"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="MemberLineOfService">
        <xsd:annotation>
          <xsd:documentation>Identifies a Line of Service that participates in a sharing relationship with another line of service. Other Line of Service is assumed from the context.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:LineOfServiceReference">
            <xsd:sequence minOccurs="0">
              <xsd:element name="participationEffectivePeriod" type="csom:EffectivePeriod"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The time period during which the line of service participates as a member of the pool.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:element name="lineOfServiceId" type="csom:LineOfServiceId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies a line of service</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:simpleType name="LineOfServiceStatusCode">
        <xsd:annotation>
          <xsd:documentation>Code for the status of the Line Of Service.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="ACTIVE"/>
          <xsd:enumeration value="TENTATIVE"/>
          <xsd:enumeration value="CLOSED"/>
          <xsd:enumeration value="UNKNOWN"/>
          <xsd:enumeration value="RESERVED"/>
          <xsd:enumeration value="CANCELLED"/>
          <xsd:enumeration value="SUSPENDED"/>
          <xsd:enumeration value="PENDING"/>
          <xsd:enumeration value="RESTRICTED"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="EmployeeLineDesignation">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="DEPENDENT">
            <xsd:annotation>
              <xsd:documentation>Employee/Spouse/Dependent Child</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="NONDEPENDENT">
            <xsd:annotation>
              <xsd:documentation>Non-dependent</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="LineOfServiceActionCode">
        <xsd:annotation>
          <xsd:documentation>Action code for the line of service</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="ADD"/>
          <xsd:enumeration value="CANCEL"/>
          <xsd:enumeration value="REMOVE"/>
          <xsd:enumeration value="RESTORE"/>
          <xsd:enumeration value="RESUME"/>
          <xsd:enumeration value="SUSPEND"/>
          <xsd:enumeration value="RETAIN"/>
          <xsd:enumeration value="CHANGE"/>
          <xsd:enumeration value="MOVE"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="Score">
        <xsd:annotation>
          <xsd:documentation>Pair of label tag and score value used in credit decisioning process.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="scoreName" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name of the score, such as Application, RBC, Behavior, Accounting.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="scoreValue" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Value calculated for the named score.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="CustomerType">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Person"/>
          <xsd:enumeration value="Organization"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="CustomerId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies the customer party.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="CustomerGroup">
        <xsd:annotation>
          <xsd:documentation>The following customer groups are being considered for customer segmentation: Consumer, Business, Special/Other, Government.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Consumer"/>
          <xsd:enumeration value="Business"/>
          <xsd:enumeration value="SpecialOther"/>
          <xsd:enumeration value="Government"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="BillingArrangementStatus">
        <xsd:annotation>
          <xsd:documentation>Current condition of an BillingArrangement, at the time expressed in the effectivePeriod.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the object, at the point in time specified by the effectivePeriod.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A finer division of code.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ResourceUnit">
        <xsd:sequence minOccurs="0">
          <xsd:element name="unit" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Unit of Measure code</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="balanceQuantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Balance Quantity on the resource unit</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Description of the Resource Unit</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="creditLimitRange" type="csom:LimitRange" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Credit Limit Range information</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="resourceUnitBalance" type="csom:ResourceUnitBalance"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Resource Unit Balance information</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="name" type="xsd:normalizedString" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Name on the resource unit</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="PayChannelStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ResourceUnitBalance">
        <xsd:annotation>
          <xsd:documentation>Resource Unit Balance information</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="availableQuantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Available Quantity on the resource unit balance</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Description on the resource unit balance</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="BillingTypeCode">
        <xsd:annotation>
          <xsd:documentation>A shorthand identifying name for the type of billing settlement: Prepaid or Postpaid.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Prepaid"/>
          <xsd:enumeration value="Postpaid"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="BillingArrangementId">
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="AutomaticPaymentType">
        <xsd:annotation>
          <xsd:documentation>Categorizes the type of automatic payment configured by this Pay Channel.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="AutoPay"/>
          <xsd:enumeration value="EasyPay"/>
          <xsd:enumeration value="AutoRefill"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="BillingArrangementActionCode">
        <xsd:restriction base="csom:ActionCode"/>
      </xsd:simpleType>
      <xsd:simpleType name="PayChannelActionCode">
        <xsd:restriction base="csom:ActionCode"/>
      </xsd:simpleType>
      <xsd:simpleType name="MediaFormatCode">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="CD">
            <xsd:annotation>
              <xsd:documentation>Bill is delivered on compact disk.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="HTML">
            <xsd:annotation>
              <xsd:documentation>Bill is delivered in HTML format.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="PDF">
            <xsd:annotation>
              <xsd:documentation>Bill is delivered in PDF format.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="XML">
            <xsd:annotation>
              <xsd:documentation>Bill is delivered in XML format.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="SMS">
            <xsd:annotation>
              <xsd:documentation>Bill summary is sent by SMS.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="TEXT"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="MeasurementUnit">
        <xsd:annotation>
          <xsd:documentation>Unit of measure.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="ActionCode">
        <xsd:annotation>
          <xsd:documentation>Specifies the action to be taken</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="ADD"/>
          <xsd:enumeration value="REMOVE"/>
          <xsd:enumeration value="RETAIN"/>
          <xsd:enumeration value="UPDATE"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="CalendarFrequency">
        <xsd:annotation>
          <xsd:documentation>A code identifying the calendar frequency of a recurring event.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Monthly">
            <xsd:annotation>
              <xsd:documentation>Once a month.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Weekly">
            <xsd:annotation>
              <xsd:documentation>Once a week.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="SemiMonthly">
            <xsd:annotation>
              <xsd:documentation>Twice a month.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="BiWeekly">
            <xsd:annotation>
              <xsd:documentation>Every two weeks.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Yearly"/>
          <xsd:enumeration value="Quarterly"/>
          <xsd:enumeration value="OneTime"/>
          <xsd:enumeration value="Daily"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="MaskingType">
        <xsd:annotation>
          <xsd:documentation>The type of masking to be applied to the data element for security and privacy.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Last4"/>
          <xsd:enumeration value="Last6"/>
          <xsd:enumeration value="First5"/>
          <xsd:enumeration value="None"/>
          <xsd:enumeration value="NA"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="Ranking">
        <xsd:annotation>
          <xsd:documentation>Priority of ranking of the associated object or component. The lower the number, the higher the priority.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int"/>
      </xsd:simpleType>
      <xsd:simpleType name="CurrencyCode">
        <xsd:annotation>
          <xsd:documentation>For a monetary value, the currency code as defined by the iso 4217 Currency Code standard. For United States dollars, the code is USD. Reference http://www.iso.org/iso/home/standards/currency_codes.htm</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:length value="3"/>
          <xsd:pattern value="[A-Z][A-Z][A-Z]">
            <xsd:annotation/>
          </xsd:pattern>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="DayOfWeek">
        <xsd:annotation>
          <xsd:documentation>Identifies the day of the week: Monday, Tuesday, etc.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Monday"/>
          <xsd:enumeration value="Tuesday"/>
          <xsd:enumeration value="Wednesday"/>
          <xsd:enumeration value="Thursday"/>
          <xsd:enumeration value="Friday"/>
          <xsd:enumeration value="Saturday"/>
          <xsd:enumeration value="Sunday"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="AttachmentTypeCode">
        <xsd:annotation>
          <xsd:documentation>Type of attachment, e.g., URL, TEXT, JPG, PNG, HTML, PDF</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="URL"/>
          <xsd:enumeration value="TEXT"/>
          <xsd:enumeration value="JPG"/>
          <xsd:enumeration value="PDF"/>
          <xsd:enumeration value="HTML"/>
          <xsd:enumeration value="PNG"/>
          <xsd:enumeration value="XML"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="EligibilityStatusCode">
        <xsd:annotation>
          <xsd:documentation>Eligibility determination based on business rules for a specific activity.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="LanguageCode">
        <xsd:annotation>
          <xsd:documentation>The language per ISO 639-1 Code: en for English, es for espagnol (Spanish). Reference http://www.iso.org/iso/home/standards/language_codes.htm</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:length value="2"/>
          <xsd:pattern value="[a-z][a-z]">
            <xsd:annotation/>
          </xsd:pattern>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="DeviceFingerprintId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies the physical device or appliance that was used to interact with carrier systems.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:normalizedString"/>
      </xsd:simpleType>
      <xsd:simpleType name="IPAddress">
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="NPA">
        <xsd:annotation>
          <xsd:documentation>The first three digits of a ten digit phone number. Commonly called the area code in the United States.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:length value="3"/>
          <xsd:pattern value="[0-9]{3}"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ContentTypeCode">
        <xsd:annotation>
          <xsd:documentation>The type of content in a string, as in XML, Text, HTML.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="XML"/>
          <xsd:enumeration value="Text"/>
          <xsd:enumeration value="JSON"/>
          <xsd:enumeration value="Reference"/>
          <xsd:enumeration value="URL"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="CountryCode">
        <xsd:annotation>
          <xsd:documentation>The unique identifier of a country (ISO 3166-2 and UN/ECE Rec 3). For instance, the United States is USA, Canada is CAN, and Mexico is MEX. Reference http://www.iso.org/iso/home/standards/country_codes.htm</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:length value="3"/>
          <xsd:pattern value="[A-Z][A-Z][A-Z]">
            <xsd:annotation/>
          </xsd:pattern>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="NXX">
        <xsd:annotation>
          <xsd:documentation>The fourth, fifth and sixth digits of a ten digit phone number. Commonly called the exchange in the United States.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:length value="3"/>
          <xsd:pattern value="[0-9]{3}"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ReferenceURI">
        <xsd:annotation>
          <xsd:documentation>A URI that can be used to return information about an object.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:anyURI"/>
      </xsd:simpleType>
      <xsd:complexType name="Application">
        <xsd:annotation>
          <xsd:documentation>A software program in use at TMobile or one of its partners, dealers, etc...</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="name" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the software application (note:  the list of valid values for needs to be governed and is currently incomplete).</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="SIVR">
                  <xsd:annotation/>
                </xsd:enumeration>
                <xsd:enumeration value="TMOBILE.COM">
                  <xsd:annotation/>
                </xsd:enumeration>
                <xsd:enumeration value="VASA">
                  <xsd:annotation/>
                </xsd:enumeration>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The primary description of the application Note: only english description is needed here.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the business application.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:simpleType name="ReturnAuthorizationType">
        <xsd:annotation>
          <xsd:documentation>Return authorization type for cart item.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Warranty"/>
          <xsd:enumeration value="Remorse"/>
          <xsd:enumeration value="PHP"/>
          <xsd:enumeration value="JUMP"/>
          <xsd:enumeration value="Trade-In"/>
          <xsd:enumeration value="MHP"/>
          <xsd:enumeration value="SCORE"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="CartId">
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="CartItemId">
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="UsageSubType">
        <xsd:annotation>
          <xsd:documentation>The subtype of usage being measured: SMS, MMS, Roaming, Wifi, VoLTE, etc.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Wifi"/>
          <xsd:enumeration value="VoLTE"/>
          <xsd:enumeration value="Roaming"/>
          <xsd:enumeration value="WifiRoaming"/>
          <xsd:enumeration value="InternationalDiscountCall"/>
          <xsd:enumeration value="InternationalDiscountCallToMobile"/>
          <xsd:enumeration value="WPSCall"/>
          <xsd:enumeration value="SMS"/>
          <xsd:enumeration value="SMSRoaming"/>
          <xsd:enumeration value="MMS"/>
          <xsd:enumeration value="MMSRoaming"/>
          <xsd:enumeration value="OneTimeCharge"/>
          <xsd:enumeration value="MonthlySubscription"/>
          <xsd:enumeration value="Home"/>
          <xsd:enumeration value="DomesticRoaming"/>
          <xsd:enumeration value="InternationalRoaming"/>
          <xsd:enumeration value="Undefined"/>
          <xsd:enumeration value="NA"/>
          <xsd:enumeration value="SessionPassData"/>
          <xsd:enumeration value="SessionPassVoiceText"/>
          <xsd:enumeration value="PayPerMB"/>
          <xsd:enumeration value="Whitelisted"/>
          <xsd:enumeration value="MusicFreedom"/>
          <xsd:enumeration value="ContentCallerTune"/>
          <xsd:enumeration value="ContentApp"/>
          <xsd:enumeration value="ContentMusic"/>
          <xsd:enumeration value="SpecialNumbers"/>
          <xsd:enumeration value="DirectoryAssistance"/>
          <xsd:enumeration value="Toll"/>
          <xsd:enumeration value="DataStash"/>
          <xsd:enumeration value="StarterStash"/>
          <xsd:enumeration value="Promotion"/>
          <xsd:enumeration value="CareAdjustments"/>
          <xsd:enumeration value="CourtesyPass"/>
          <xsd:enumeration value="VideoFreedom"/>
          <xsd:enumeration value="CombinedStash"/>
          <xsd:enumeration value="TotalUsage">
            <xsd:annotation>
              <xsd:documentation>Total Volume Consumed</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="OnNetUsage">
            <xsd:annotation>
              <xsd:documentation>On network usage.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="ThrottlingUsage">
            <xsd:annotation>
              <xsd:documentation>On-DeviceThrottle and TetheringThrottle usage</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="TotalBasicUsage">
            <xsd:annotation>
              <xsd:documentation>Comprises of On-DeviceThrottle; On-DeviceCapped; TetheringThrottle; TetheringCapped; and PayDay</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="PayDayUsage">
            <xsd:annotation>
              <xsd:documentation>Comprises of only PayDayUsage</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="TotalTetheringUsage">
            <xsd:annotation>
              <xsd:documentation>Comprises of TetheringThrottle ; TetheringCapped</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="UsageType">
        <xsd:annotation>
          <xsd:documentation>Type of usage, such as Voice, Data</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="SMS">
            <xsd:annotation>
              <xsd:documentation>DEPRECATED </xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="MMS">
            <xsd:annotation>
              <xsd:documentation>DEPRECATED</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Voice"/>
          <xsd:enumeration value="Data"/>
          <xsd:enumeration value="Content"/>
          <xsd:enumeration value="Message">
            <xsd:annotation>
              <xsd:documentation>Combined SMS and MMS.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="VAS">
            <xsd:annotation>
              <xsd:documentation>Value Added Service</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="VoicePlusMessage"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="InventoryStatus">
        <xsd:annotation>
          <xsd:documentation>Status of the product in inventory.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:sequence>
              <xsd:element name="estimatedAvailable" type="csom:DatePeriod"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Estimated availabe date range when item is backordered or pre-ordered or being retired</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="statusCode">
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="OUTOFSTOCK"/>
                  <xsd:enumeration value="BACKORDER"/>
                  <xsd:enumeration value="PREORDER"/>
                  <xsd:enumeration value="AVAILABLE"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="CreditCardPayment">
        <xsd:complexContent>
          <xsd:extension base="csom:ChargeAuthorization">
            <xsd:sequence minOccurs="0">
              <xsd:element name="traceNumber" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Third party Payment (credit card system) system-provided trace number</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="voiceAuthorizationIndicator" type="xsd:boolean"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates whether Authorization was done manually over the phone</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="creditCard" type="csom:CreditCard" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Details about the credit card to which an amount is requsted to be charged</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="creditChargeAuthorizationResponse" type="csom:CreditChargeAuthorizationResponse"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Details about the authorization Response for an amount to be charged to a credit card</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ChargeAuthorizationResponse">
        <xsd:annotation>
          <xsd:documentation>Details about the authorization Response for an amount to be charged to a payment card</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="actionTypeCode" type="xsd:token" minOccurs="0"/>
          <xsd:element name="responseCode" type="csom:ChargeAuthorizationResponseCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Code that indicates the result of the authorization request. Code values will vary depending on the payment gateway vendor.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paymentSystemCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Payment system code returned during the initial authorization</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="ACCEPT"/>
                <xsd:enumeration value="REJECT"/>
                <xsd:enumeration value="ERROR"/>
                <xsd:enumeration value="REVIEW"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="authorizationTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Time at which the credit card payment request was authorized</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="authorizedAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Authorized  Charge Amount, in the  currency of the card issuer</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="additionalResponseData" type="xsd:normalizedString"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>This field contains additional data that may be returned in an authorization response message. This field could contain values that describe items such as the reason for a decline, the field in error, etc.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="errorCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A code returned in the response message that specifies an error condition encountered when processing the transaction.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="errorDescription" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A description returned in the response message that describes an error condition encountered when processing the transaction.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="Tokenization">
        <xsd:annotation>
          <xsd:documentation>This section contains fields used in TransArmor transactions in which the card number or track data are encrypted and/or tokenized.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="securityLevel" type="csom:SecurityLevel" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>This field indicates the security level used on a TransArmor transaction.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="tokenType" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>This field indicates the Multi-pay token type identifier used in a transaction.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="token" type="xsd:string" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>This field contains the Multi-pay token which can be used to replace the PAN. The Multi-pay token is the same length as the card number and contains the same last four digits. Multi-pay tokens can be used to initiate subsequent transactions that would normally be submitted using a PAN (e.g. recurring transactions).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="encryptionType" type="csom:EncryptionType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>This field indicates the type of encryption used when passing encrypted card data.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="encryptionTarget" type="csom:EncryptionTarget"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>This field indicates the type of data that is being encrypted.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="keyId" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>This is an identifier used to retrieve the private key, which is required for decryption.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="encryptedContent" type="xsd:string" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>This field contains the track data or card number provided in an encrypted block.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="securityKeyUpdateIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>This field applies only to RSA encryption and indicates that a key update is available. If present in a response, the POS/software should initiate a TAKeyRequest transaction.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="securityKey" type="xsd:string" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>This field applies to RSA only and specifies the public key used by the POS.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="DebitCardPayment">
        <xsd:complexContent>
          <xsd:extension base="csom:ChargeAuthorization">
            <xsd:sequence minOccurs="0">
              <xsd:element name="debitCard" type="csom:DebitCard" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Details about the debit card to which an amount is requsted to be charged</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="debitChargeAuthorizationResponse" type="csom:DebitChargeAuthorizationResponse"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Details about the authorization Response for an amount to be charged to a debit card</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="CreditChargeAuthorizationResponse">
        <xsd:complexContent>
          <xsd:extension base="csom:ChargeAuthorizationResponse">
            <xsd:sequence minOccurs="0">
              <xsd:element name="avsResponseCode" type="csom:AVSResponseCode"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Address Verification Service response code for swiped transactions where the zip code or address line information was provided as part of the request</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="securityCheckResponseCode" type="csom:SecurityCheckResponseCode"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Result of the CVV3/4 number verification</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ChargeAuthorization">
        <xsd:annotation>
          <xsd:documentation>Charge Card Payment and authorization Details</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="authorizationId" type="csom:ChargeAuthorizationId"
             minOccurs="0"/>
          <xsd:element name="chargeAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>In Payment transaction request currency</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="chargeTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Time of requesting the credit card authorization</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="DebitChargeAuthorizationResponse">
        <xsd:complexContent>
          <xsd:extension base="csom:ChargeAuthorizationResponse">
            <xsd:sequence minOccurs="0">
              <xsd:element name="traceNumber" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Trace Number</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="debitNetworkCode" type="csom:DebitNetworkCode"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Code of the Debit network processing the debit charge</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="settlementDate" type="xsd:date" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>This field contains the date the debit transaction will be settled.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="TrackData">
        <xsd:annotation>
          <xsd:documentation>Contains the unaltered track data from the magnetic stripe of the payment card.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="track1Data" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>This field contains the unaltered track 1 data from the magnetic stripe of the card.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:length value="76"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="track2Data" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>This field contains the unaltered track 2 data from the magnetic stripe of the card.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:length value="37"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="SecurityCheckResponseCode">
        <xsd:annotation>
          <xsd:documentation>Result of the CVV3/4 number verification</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="TerminalEntryMode">
        <xsd:annotation>
          <xsd:documentation>Indicates how the account number was entered on the transaction at Point of Sale (POS).</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Unspecified"/>
          <xsd:enumeration value="Manual">
            <xsd:annotation>
              <xsd:documentation>Key entered</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Barcode"/>
          <xsd:enumeration value="OCR">
            <xsd:annotation>
              <xsd:documentation>Optical Character Reader</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="ICR">
            <xsd:annotation>
              <xsd:documentation>Integrated Circuit Read (Reliable)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="CICR">
            <xsd:annotation>
              <xsd:documentation>Contactless Integrated Circuit Read (Reliable)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="ManualChip">
            <xsd:annotation>
              <xsd:documentation>Manual Entry at Chip Terminal</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="MagneticChip">
            <xsd:annotation>
              <xsd:documentation>Magnetic Stripe at Chip Terminal, Fallback (Reliable)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="CMC">
            <xsd:annotation>
              <xsd:documentation>Contactless Mobile Commerce</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="MagneticTrack">
            <xsd:annotation>
              <xsd:documentation>Magnetic Stripe - Track Read</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="ContactlessMagnetic">
            <xsd:annotation>
              <xsd:documentation>Contactless Magnetic Stripe Read</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="ICRUnreliable">
            <xsd:annotation>
              <xsd:documentation>Integrated Circuit Read (CVV data unreliable)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="TerminalEntryCapability">
        <xsd:annotation>
          <xsd:documentation>An identifier used to indicate the entry mode capability of the terminal.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Unspecified"/>
          <xsd:enumeration value="None">
            <xsd:annotation>
              <xsd:documentation>Terminal not used</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Magnetic">
            <xsd:annotation>
              <xsd:documentation>Magnetic stripe only</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="MageneticKey">
            <xsd:annotation>
              <xsd:documentation>Magnetic stripe and key entry</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="MagneticKeyChip">
            <xsd:annotation>
              <xsd:documentation>Magnetic stripe, key entry and chip</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Barcode">
            <xsd:annotation>
              <xsd:documentation>Bar code</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="ProximityChip">
            <xsd:annotation>
              <xsd:documentation>Proximity terminal - contactless chip / RFID</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="OCR"/>
          <xsd:enumeration value="Chip">
            <xsd:annotation>
              <xsd:documentation>Chip only</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="MagneticChip">
            <xsd:annotation>
              <xsd:documentation>Chip and magnetic stripe</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Manual">
            <xsd:annotation>
              <xsd:documentation>Manual entry only</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="ProximityMagnetic">
            <xsd:annotation>
              <xsd:documentation>Proximity terminal - contactless magnetic stripe</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="AVSResponseCode">
        <xsd:annotation>
          <xsd:documentation>Address Verification Service response code for swiped transactions where the zip code or address line information was provided as part </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="DebitNetworkCode">
        <xsd:annotation>
          <xsd:documentation>Code of the Debit network processing the debit charge</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="EncryptionTarget">
        <xsd:annotation>
          <xsd:documentation>This field indicates the type of data that is being encrypted.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Track1"/>
          <xsd:enumeration value="Track2"/>
          <xsd:enumeration value="PAN"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ElectronicAuthenticationCapability">
        <xsd:annotation>
          <xsd:documentation>A code indicating the electronic (PIN) authentication capability at the Point of Sale (POS).</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Unspecified"/>
          <xsd:enumeration value="PIN">
            <xsd:annotation>
              <xsd:documentation>PIN entry capability</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="NoPIN">
            <xsd:annotation>
              <xsd:documentation>No PIN entry capability</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="PINInoperative">
            <xsd:annotation>
              <xsd:documentation>PIN Pad Inoperative</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="PINVerified">
            <xsd:annotation>
              <xsd:documentation>PIN verified by terminal device</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ChargeAuthorizationResponseCode">
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="SecurityLevel">
        <xsd:annotation>
          <xsd:documentation>This field indicates the security level used on a TransArmor transaction.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Tknizatn">
            <xsd:annotation>
              <xsd:documentation>Tokenization</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="EncrptTknizatn">
            <xsd:annotation>
              <xsd:documentation>Encrypted Tokenization</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="CardCaptureCapability">
        <xsd:annotation>
          <xsd:documentation>This field represents the terminal capability to capture the card data.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Capable">
            <xsd:annotation>
              <xsd:documentation>terminal has card capture capability</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Incapable">
            <xsd:annotation>
              <xsd:documentation>terminal has no card capture capability or no terminal used</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="POSConditionCode">
        <xsd:annotation>
          <xsd:documentation>An identifier used to indicate the transaction condition at the Point-of-Sale (POS).</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="CardPresent">
            <xsd:annotation>
              <xsd:documentation>Cardholder Present, Card Present</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="UnattendedDevice">
            <xsd:annotation>
              <xsd:documentation>Cardholder Present, Unattended Device</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="SuspectFraud">
            <xsd:annotation>
              <xsd:documentation>Cardholder Present, Suspect Fraud</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Recurring">
            <xsd:annotation>
              <xsd:documentation>Cardholder Not Present  Recurring</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="CardNotPresent">
            <xsd:annotation>
              <xsd:documentation>Cardholder Present, Card Not Present</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="IdentityVerified">
            <xsd:annotation>
              <xsd:documentation>Cardholder Present, Identity Verified</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="MOTO">
            <xsd:annotation>
              <xsd:documentation>Cardholder Not Present, Mail Order or Telephone Order</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Ecommerce">
            <xsd:annotation>
              <xsd:documentation>Cardholder Not Present, Ecommerce</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="CouldNotRead">
            <xsd:annotation>
              <xsd:documentation>Cardholder Present, Magnetic Stripe Could Not Be Read</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="POSId">
        <xsd:annotation>
          <xsd:documentation>The point of sale (POS) id identifies the specific device or point of entry where the transaction originated.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="TerminalType">
        <xsd:annotation>
          <xsd:documentation>An identifier used to describe the type of terminal being used for the transaction.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Unspecified"/>
          <xsd:enumeration value="POS">
            <xsd:annotation>
              <xsd:documentation>Electronic Payment Terminal (POS)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="AFD">
            <xsd:annotation>
              <xsd:documentation>Automated Fuel Dispensing Machine (AFD)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Unattended">
            <xsd:annotation>
              <xsd:documentation>Unattended Customer Terminal (kiosk)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Mobile">
            <xsd:annotation>
              <xsd:documentation>Mobile Terminal (Transponder)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="NCR">
            <xsd:annotation>
              <xsd:documentation>Electronic Cash Register</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Ticket">
            <xsd:annotation>
              <xsd:documentation>Ticket Machine</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="CSR">
            <xsd:annotation>
              <xsd:documentation>Call Center Operator</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="MPOS">
            <xsd:annotation>
              <xsd:documentation>Mobile Point of Sale</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="EncryptionType">
        <xsd:annotation>
          <xsd:documentation>This field indicates the type of encryption used when passing encrypted card data.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="RSA"/>
          <xsd:enumeration value="Verifone"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="MerchantId">
        <xsd:annotation>
          <xsd:documentation>A unique ID used to identify the Merchant. The merchant must use the value assigned by payment gateway vendor. Each store is considered a separate merchant.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="ChargeAuthorizationId">
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="BillPaymentSchedulingType">
        <xsd:annotation>
          <xsd:documentation>The bill payment scheduling type notes whether the payment was made as part of an installment plan, as a recurring payment (like EasyPay), or a one time payment.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="OneTime"/>
          <xsd:enumeration value="Recurring"/>
          <xsd:enumeration value="Installment"/>
          <xsd:enumeration value="Deferred"/>
          <xsd:enumeration value="First"/>
          <xsd:enumeration value="Final"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="FinancialTransactionType">
        <xsd:annotation>
          <xsd:documentation>This field represents the type of financial transaction being performed.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Activation">
            <xsd:annotation>
              <xsd:documentation>A transaction that is performed when the purchase of a Prepaid Closed Loop card occurs. It adds value and changes the status of the Prepaid Closed Loop card to active.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Authorization">
            <xsd:annotation>
              <xsd:documentation>A transaction that is used to transmit an authorization request to First Data where it is not captured for settlement processing. A subsequent Completion transaction is then submitted to First Data to be captured for settlement processing.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="BalanceInquiry">
            <xsd:annotation>
              <xsd:documentation>A transaction that is used to return the current balance on an active account.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="BalanceLock">
            <xsd:annotation>
              <xsd:documentation>This transaction will lock the requested amount up to the current available balance for a preset time. One or more locks can be active on a Prepaid Closed Loop card at one time.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Cashout">
            <xsd:annotation>
              <xsd:documentation>A transaction that is used to remove the remaining balance from the card. The cashout transaction will bring the account balance to zero.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="CashoutActiveStatus">
            <xsd:annotation>
              <xsd:documentation>This transaction is used to remove the remaining balance from the card and the account status stays as active.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Completion">
            <xsd:annotation>
              <xsd:documentation>A transaction that is used to submit for settlement a previously approved Authorization or a transaction that was approved over the phone.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="FileDownload">
            <xsd:annotation>
              <xsd:documentation>A transaction submitted by the merchant/software to check the status of a file or download a file from payment gateway vendor.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="HostTotals">
            <xsd:annotation>
              <xsd:documentation>A transaction submitted to request a Host Totals Report for a particular day.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Load">
            <xsd:annotation>
              <xsd:documentation>This transaction will add value to a Prepaid Closed Loop card and change the status to active when purchasing a card. If the Prepaid Closed Loop card is already active, the value is added to the existing balance of the Prepaid Closed Loop card.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Redemption">
            <xsd:annotation>
              <xsd:documentation>A Redemption transaction reduces the balance of an active Prepaid Closed Loop card.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="RedemptionUnlock">
            <xsd:annotation>
              <xsd:documentation>This transaction is performed after a Balance Lock transaction to complete the purchase on a Prepaid Closed Loop card and unlock any remaining balance from the Balance Lock transaction.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Refund">
            <xsd:annotation>
              <xsd:documentation>A transaction that is used to credit funds to the account holder.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Reload">
            <xsd:annotation>
              <xsd:documentation>This transaction adds value to an already active Prepaid Closed Loop account.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Sale">
            <xsd:annotation>
              <xsd:documentation>A transaction that is used to authorize and capture the amount in one transaction.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="TAKeyRequest">
            <xsd:annotation>
              <xsd:documentation>A transaction submitted by the merchant/software to update the TransArmor key used for encryption of the card data.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="TATokenRequest">
            <xsd:annotation>
              <xsd:documentation>A transaction submitted by the merchant/software solely to obtain a TransArmor token.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Verification">
            <xsd:annotation>
              <xsd:documentation>A transaction that is used to determine if the card is active.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="DeviceOfferingReference">
        <xsd:annotation>
          <xsd:documentation>DEPRECATED</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:SimpleProductOfferingReference"/>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="VoucherRedemption">
        <xsd:annotation>
          <xsd:documentation>Payment is made by redeeming a voucher.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="serialNumber" type="csom:SerialNumber" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The seraila number uniquely identifies the Voucher document.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="issuerId" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the issuer of the Voucher document.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PIN" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Person identification number for the Voucher.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="voucherRedemptionType" type="csom:VoucherRedemptionType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Categorizes the type of voucher redemption: refill or activation.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="PaymentStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="Pending"/>
                  <xsd:enumeration value="Completed/Posted"/>
                  <xsd:enumeration value="Cancelled"/>
                  <xsd:enumeration value="Rejected"/>
                  <xsd:enumeration value="Scheduled"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="PaymentDirectDebitMandate">
        <xsd:sequence>
          <xsd:element name="authorizationMethodCode" type="csom:AuthorizationMethodCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Mandate authorization method.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="authorizedIndicator" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Flag indicating whether the payer has explicitly granted the mandate or not</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="signatureTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date on which the mandate has been signed by the payer party</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="electronicSignature" type="xsd:normalizedString"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Digital signature as provided by the payer party</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="frequencyCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Frequency of direct debit. Example values are :Daily, Weekly, Monthly etc.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="amendmentIndicator" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Flag indicating whether the underlying mandate has been amended or not</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectiveTimePeriod" type="csom:TimePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>First Collection Date and Final Collection Date as authorized by the Mandate</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="mandateSignerPartyReference" type="xsd:normalizedString"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Credit Party that signs the Direct Debit Mandate              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="originalDirectDebitMandateReference" type="xsd:normalizedString"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Original Mandate Reference for the case where amendments are made to the original mandate</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="PaymentActionCode">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="RELEASE"/>
          <xsd:enumeration value="STOP"/>
          <xsd:enumeration value="VOID"/>
          <xsd:enumeration value="HOLD"/>
          <xsd:enumeration value="POST"/>
          <xsd:enumeration value="ADD"/>
          <xsd:enumeration value="REMOVE"/>
          <xsd:enumeration value="RETAIN"/>
          <xsd:enumeration value="UPDATE"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="PaymentSource">
        <xsd:annotation>
          <xsd:documentation>Identifies the system that was the origin of the payment.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="VoucherRedemptionType">
        <xsd:annotation>
          <xsd:documentation>Categorizes the type of voucher redemption: refill or activation.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="AuthorizationMethodCode">
        <xsd:annotation>
          <xsd:documentation>Method by which the payer authorized the transaction.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="PHONE"/>
          <xsd:enumeration value="WEB"/>
          <xsd:enumeration value="WRITTEN"/>
          <xsd:enumeration value="EMAIL"/>
          <xsd:enumeration value="SMS"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="PaymentClassificationCode">
        <xsd:annotation>
          <xsd:documentation>Classifies the payment records on a customer's account as PAYMENT, REVERSAL, REFUND, or DEPOSIT.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="PAYMENT"/>
          <xsd:enumeration value="REFUND"/>
          <xsd:enumeration value="DEPOSIT"/>
          <xsd:enumeration value="VOUCHER_REFILL"/>
          <xsd:enumeration value="INVOICE_TRANSFER"/>
          <xsd:enumeration value="PAYMENT_TRANSFER"/>
          <xsd:enumeration value="CLEARING"/>
          <xsd:enumeration value="WRITE_OFF"/>
          <xsd:enumeration value="OUTPAYMENT"/>
          <xsd:enumeration value="TRANSFERFUND"/>
          <xsd:enumeration value="DEPOSIT2FUND"/>
          <xsd:enumeration value="OUTDEPOSIT"/>
          <xsd:enumeration value="CLR-FPA">
            <xsd:annotation>
              <xsd:documentation>Clear flexible payment arrangement</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="BACKOUT"/>
          <xsd:enumeration value="TRANSPARTPAYMENT">
            <xsd:annotation>
              <xsd:documentation>Transfer partial payment</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="PaymentChannelCode">
        <xsd:annotation>
          <xsd:documentation>The channel in which the payment was originally performed.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="PaymentMethodCode">
        <xsd:annotation>
          <xsd:documentation>This field specifies the payment method of the transaction.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Cash"/>
          <xsd:enumeration value="Credit">
            <xsd:annotation>
              <xsd:documentation>Credit card, including EMV and contactless</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Debit">
            <xsd:annotation>
              <xsd:documentation>DEBIT CARD with pin (ATM rail)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Check"/>
          <xsd:enumeration value="eCheck">
            <xsd:annotation>
              <xsd:documentation>Electronic check</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="EBT">
            <xsd:annotation>
              <xsd:documentation>Electronic Benefit Transfer</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Prepaid">
            <xsd:annotation>
              <xsd:documentation>PREPAID CARD</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="DelayPay"/>
          <xsd:enumeration value="PLDebit">
            <xsd:annotation>
              <xsd:documentation>PINLESS DEBIT CARD (ATM rail)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="DebitSignature">
            <xsd:annotation>
              <xsd:documentation>Debit with Signature (credit rail)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="PayPal"/>
          <xsd:enumeration value="GiftCard">
            <xsd:annotation>
              <xsd:documentation>Closed Loop Gift Card</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="GPRCard">
            <xsd:annotation>
              <xsd:documentation>General Purpose Reloadable (GPR) Card, open loop</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Voucher"/>
          <xsd:enumeration value="DeductionFromAccountBalance"/>
          <xsd:enumeration value="Hybrid"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="SubscriptionPromotionStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:simpleType name="BillingFrequencyCode">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="RECURRING"/>
          <xsd:enumeration value="INSTALLMENT"/>
          <xsd:enumeration value="REGULAR"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="OrderAccount">
        <xsd:annotation>
          <xsd:documentation>Information about the customer's business relationship with the company.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:CustomerAccountBase">
            <xsd:sequence minOccurs="0">
              <xsd:element name="financialAccount" type="csom:FinancialAccount"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Financial accounts associated with the customer</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="OrderAttachment">
        <xsd:complexContent>
          <xsd:extension base="csom:Attachment">
            <xsd:sequence minOccurs="0">
              <xsd:element name="signatureRequiredIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates whether a signature is required for the attached document.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="printDocumentIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates whether the attached document must be printed.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="documentType" type="csom:OrderDocumentType"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The type of document attached to the order.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="documentSubType" type="csom:OrderDocumentSubType"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Document SubType.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="language" type="csom:LanguageCode" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Language used</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="OriginalOrder">
        <xsd:sequence>
          <xsd:element name="orderId" type="csom:OrderId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Order identification</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="dealerCode" type="csom:CommissionsCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Dealer code</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="storeId" type="csom:StoreId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Store identification</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="OrderScheduleStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="NEW"/>
                  <xsd:enumeration value="INPROCESS"/>
                  <xsd:enumeration value="SHIPPED"/>
                  <xsd:enumeration value="BACKORDER"/>
                  <xsd:enumeration value="INSTALLED"/>
                  <xsd:enumeration value="PREORDER"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="OrderTypeSummary">
        <xsd:annotation>
          <xsd:documentation>Summary of mixed order items</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="orderType" type="csom:OrderType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Order type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="orderId" type="csom:OrderId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Order identification</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="orderLineId" type="csom:OrderLineId" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Order line identification for order lines under this order type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="scheduledTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Schedule date for future date items</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="cartItemId" type="csom:CartItemId" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Cart item identification for cart items under this order type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="OrderStatusHistory">
        <xsd:annotation>
          <xsd:documentation>A record of the status changes that have occurred during the life of the order.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="historicalStatus" type="csom:OrderStatus" minOccurs="0"
             maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="TermsAndConditionsDisposition">
        <xsd:annotation>
          <xsd:documentation>Disposition of the terms and conditions: whether they were accepted, how, and when.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="acceptanceIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the terms and conditions were accepted.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="acceptanceTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date and time that the terms and conditions were accepted.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="OrderSummary">
        <xsd:annotation>
          <xsd:documentation>An aggregation of order data according to the scope indicated in the summary scope.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element ref="csom:customerId" minOccurs="0"/>
          <xsd:element ref="csom:customerAccountId" minOccurs="0"/>
          <xsd:element ref="csom:financialAccountId" minOccurs="0"/>
          <xsd:element ref="csom:lineOfServiceId" minOccurs="0"/>
          <xsd:element name="summaryScope" type="csom:SummaryScope" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the scope of the aggregation presented in the summary: FinancialAccount, LineOfService, or GrandTotal.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalDueAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total amount due in the scope of the order (subset) specified in the summary scope.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalRecurringDueAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total recurring amount due in the scope of the order (subset) specified in the summary scope.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="charge" type="csom:Charge" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Total charge aggregated by charge type for the scope of the order (subset) specified in the summary scope.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="deduction" type="csom:Deduction" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Total deduction aggregated by deduction type for the scope of the order (subset) specified in the summary scope.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="tax" type="csom:Tax" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Total tax aggregated by tax type for the scope of the order (subset) specified in the summary scope.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="freightCharge" type="csom:Charge" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Total shipping charge aggregated by charge type for the scope of the order (subset) specified in the summary scope.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="quotationLineId" type="csom:QuotationLineId" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A list of related quotation lines that are relevant for the scope of the order (subset) specified in the summary scope.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="calculationType" type="csom:CalculationType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Price calculation type for summary.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalCurrentRecurringAmount" type="csom:Amount"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Sum of current recurring due</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalDeltaRecurringDueAmount" type="csom:Amount"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Difference between currentRecurringChargAmmount and totalRecurringChargeAmount</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalExtendedAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total extended amount of the order</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalSoftGoodDueNowAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>totalDueAmount portion for softgoods</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalHardGoodDueNowAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>totalDueAmount poriton for hardgoods</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalSoftGoodOneTimeDueNowAmount" type="csom:Amount"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total due now amount for one time soft godd charges, e.g. fees or data passes</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalSoftGoodRecurringDueNowAmount" type="csom:Amount"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total due now amount related to soft goods, e.g. due now amount on rate plan upgrade as well as prorated monthly amount due now.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalTaxAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total tax amount.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalFeeAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total due amount for fee only.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalExtendedRecurringAmount" type="csom:Amount"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total recurring amount excluding tax and fee</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalTaxRecurringAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total tax for recurring amount</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalNonTaxableAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total amount for non-taxable items.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="rootParentId" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Reference identification for LineOfService Summary on activations or adding a line, when there is no LineOFServiceId created yet.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalRefundAmountDueLater" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total refund amount due later after confirmation.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalRefundAmountDueNow" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total refund amount due now</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="finalRefundAmountDueNow" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Final refund amount due now</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="orderTypeSummary" type="csom:OrderTypeSummary"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Order type summary for mixed order types</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="RMAStatus">
        <xsd:annotation>
          <xsd:documentation>Status of the Return Merchandise Authorization (RMA).</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="RECEIVED"/>
                  <xsd:enumeration value="ADJUSTED"/>
                  <xsd:enumeration value="PAID"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:simpleType name="SummaryScope">
        <xsd:annotation>
          <xsd:documentation>Identifies the scope of the aggregation presented in the summary: FinancialAccount, LineOfService, or GrandTotal.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="GrandTotal"/>
          <xsd:enumeration value="FinancialAccount"/>
          <xsd:enumeration value="LineOfService"/>
          <xsd:enumeration value="Customer"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="OrderReason">
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="CalculationType">
        <xsd:annotation>
          <xsd:documentation>To distinguish the price schedule origin, such as Catalog, Quatation</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Catalog"/>
          <xsd:enumeration value="Quotation"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="OrderDocumentSubType">
        <xsd:annotation>
          <xsd:documentation>Order document subtype</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="ACTIVATION"/>
          <xsd:enumeration value="ADD_A_LINE"/>
          <xsd:enumeration value="RATE_PLAN_CHANGE"/>
          <xsd:enumeration value="LOST_STOLEN"/>
          <xsd:enumeration value="SIM_CHANGE"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ModeOfExchange">
        <xsd:annotation>
          <xsd:documentation>Mode of Exchange (Advance, Post, Immediate). Advance exchange means customer sends in their device before receiving the replacement. Post means new device is sent first and Customer sends in old one after receiving the new one. Immediate means the exchange is made in store with rep.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Advance"/>
          <xsd:enumeration value="Post"/>
          <xsd:enumeration value="Immediate"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="OrderLineId">
        <xsd:annotation>
          <xsd:documentation>Unique identifier for the order line within the scope of the given order.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="OrderActionCode">
        <xsd:annotation>
          <xsd:documentation>The action code defines what should be done with the order.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="SUBMIT"/>
          <xsd:enumeration value="COMPLETE"/>
          <xsd:enumeration value="CANCEL"/>
          <xsd:enumeration value="UPDATE"/>
          <xsd:enumeration value="CREATE">
            <xsd:annotation>
              <xsd:documentation>For orders that are created without a Cart.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="LOCKFORUPDATE"/>
          <xsd:enumeration value="UNLOCK"/>
          <xsd:enumeration value="LOCKFORCANCEL"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="OrderRelationshipType">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Return">
            <xsd:annotation>
              <xsd:documentation>The related order is a Return Order.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Sales"/>
          <xsd:enumeration value="Fulfillment"/>
          <xsd:enumeration value="Shipping"/>
          <xsd:enumeration value="ChangeOfResponsibility"/>
          <xsd:enumeration value="Parent"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="OrderLineActionCode">
        <xsd:annotation>
          <xsd:documentation>The action code defines what should be done with the order line. 
                </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="ADD"/>
          <xsd:enumeration value="REMOVE"/>
          <xsd:enumeration value="RETAIN"/>
          <xsd:enumeration value="UPDATE"/>
          <xsd:enumeration value="RESET"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="FundingGroupId">
        <xsd:annotation>
          <xsd:documentation>Identifier of funding group</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="OrderDocumentType">
        <xsd:annotation>
          <xsd:documentation>The type of document attached to the order.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="ACTIVATION_AGREEMENT"/>
          <xsd:enumeration value="SERVICE_AGREEMENT"/>
          <xsd:enumeration value="EIP_AGREEMENT"/>
          <xsd:enumeration value="EIP_CONFIRMATION"/>
          <xsd:enumeration value="DRP_AGREEMENT"/>
          <xsd:enumeration value="TRANSACTION_CONFIRMATION"/>
          <xsd:enumeration value="ESIGN_TERMS_AND_CONDITIONS"/>
          <xsd:enumeration value="SIGNATURE"/>
          <xsd:enumeration value="SERVICE_TERMS"/>
          <xsd:enumeration value="SIM_CHANGE_CONFIRMATION"/>
          <xsd:enumeration value="SUSPENSION_CONFIRMATION"/>
          <xsd:enumeration value="ADD_REMOVE_FEATURE"/>
          <xsd:enumeration value="RECEIPT"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="OrderSubType">
        <xsd:annotation>
          <xsd:documentation>Order sub type</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="ReceiptMethodCode">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Email"/>
          <xsd:enumeration value="Printed"/>
          <xsd:enumeration value="Mailed"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="OrderLineTransactionType">
        <xsd:annotation>
          <xsd:documentation>This field categorizes the type of transaction being performed on the order line.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="HandsetOrder"/>
          <xsd:enumeration value="Exchange"/>
          <xsd:enumeration value="Upgrade"/>
          <xsd:enumeration value="SimOnly"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="OrderLineTransactionSubType">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="FullPrice"/>
          <xsd:enumeration value="EIP"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="QuotationSchedule">
        <xsd:annotation>
          <xsd:documentation>A quotation schedule specifies pricing details for the quoted product offering.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="partialShipmentAllowedIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether partial shipment of the Sales Quote is allowed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total amount quoted for this line/schedule of the quotation.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="requestedDeliveryDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Delivery date requested by the customer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="modeOfTransportCode" type="csom:ModeOfTransportCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Shipping method such as air, sea, or ground.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="freightTermCode" type="csom:FreightTermCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the Freight Terms of the Quotation Line/Schedule.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="fobPointCode" type="csom:FOBPointCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Free On Board point code for shipping</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="serviceLevelCode" type="csom:ServiceLevelCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Shipping carrier priority code such as 2nd day, next day, priority, etc.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="note" type="csom:Note" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Notes about the quotation line schedule.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="dropShipmentAllowedIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether drop shipment is allowed for this quotation line/schedule.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="quantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Quantity of the quoted product offering from the quotation line that is addressed by this quotation schedule.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="unitListPrice" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The base price from which markups occur. Typically, this is the list price from the quote's price list.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="freightCharge" type="csom:Charge" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Charge associated with shipping for this quotation schedule.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="unitSalePrice" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Quoted selling price for each unit of the quantity.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="quotationScheduleStatus" type="csom:QuotationScheduleStatus"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Status of quote schedule such as pending, open, cancelled, closed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="freightCarrier" type="csom:FreightCarrier" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Freight Carrier responsible for the transporation of goods from the supplier to the customer for the purposes of the quotation.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="shipToParty" type="csom:Contact" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Details about the default Party to whom the line items on the Sales Quote are shipped or the services on the Sales Quote are provided.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="shipmentReference" type="xsd:string" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Refers to the shipment associated with this quotation schedule.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="priceListId" type="csom:PriceListId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the price list used for this quotation line schedule, if different from the default for this quotation.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="shipFromParty" type="csom:Contact" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Details about the default Party from whom the line items on the Sales Quote are shipped.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="billToParty" type="csom:QuotationBillToParty" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Details about the party to whom the sales quotation line schedule will be invoiced. </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="quotationScheduleTax" type="csom:Tax" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Details of Tax applicable to the Sales Quote Line Schedule. If tax is exempt, the Tax Exempt component in Tax will be populated with Exemption related information such as Number and reason.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="quotationScheduleSalesperson" type="csom:PersonReference"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Sales Persons associated with the quote schedule and the commission amount allocated to each sales person</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="quotationScheduleDeduction" type="csom:Deduction"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Details about the discount or allowance that the Customer is to receive for the Sales quote. Examples are Early Payment, Coupons etc.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="quotationScheduleCharge" type="csom:Charge" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Details about the Charges applicable on the sales quote schedule line.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="quotationScheduleId" type="csom:QuotationScheduleId"/>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="QuotationBillToParty">
        <xsd:complexContent>
          <xsd:extension base="csom:Contact">
            <xsd:sequence minOccurs="0">
              <xsd:element ref="csom:financialAccountNumber"/>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="QuotationScheduleStatus">
        <xsd:annotation>
          <xsd:documentation>Status of quote schedule such as pending, open, cancelled, closed.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="PENDING"/>
                  <xsd:enumeration value="OPEN"/>
                  <xsd:enumeration value="CANCELLED"/>
                  <xsd:enumeration value="CLOSED"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="QuotationLineStatus">
        <xsd:annotation>
          <xsd:documentation>Status of the quotation line.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:simpleType name="QuotationLineId">
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="QuotationScheduleId">
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="QuoteType">
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:complexType name="PartyRelationship">
        <xsd:annotation>
          <xsd:documentation>Specifies a relationship held between one party and one of more other parties.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="partyId" type="csom:PartyId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Uniquely identifies a Party. May be either a Person or an Organization.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="relatedPartyId" type="csom:PartyId" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Uniquely identifies a related Party. May be either a Person or an Organization.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="relationshipType" type="csom:PartyRelationshipType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Categorizes the type of relationship between the parties. Direction of relationship is from the partyId to the relatedPartyId.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Describes the party relationship.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Specifies the time period during which the relationship is in place.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="relationshipStatus" type="csom:PartyRelationshipStatus"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The status of the relationship between the parties during the specified time period.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="note" type="csom:Note" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Notes about the party relationship.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="partyRelationshipId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the relationship between the parties.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="PartyRelationshipStatus">
        <xsd:annotation>
          <xsd:documentation>The status of the relationship between the parties during the specified time period.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:simpleType name="PartyRelationshipType">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Spouse"/>
          <xsd:enumeration value="Parent"/>
          <xsd:enumeration value="Child"/>
          <xsd:enumeration value="Guardian"/>
          <xsd:enumeration value="Employee"/>
          <xsd:enumeration value="Employer"/>
          <xsd:enumeration value="Member"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="PartyId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies a Party. May be either a Person or an Organization.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:complexType name="RatePlanOfferingReference">
        <xsd:annotation>
          <xsd:documentation>DEPRECATED</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:ProductOfferingReference"/>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ResourceStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="csom:ResourceStatusCode">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the object, at the point in time specified by the effectivePeriod.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode" type="csom:ResourceSubStatusCode">
              <xsd:annotation>
                <xsd:documentation>A finer division of code.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="PairingStatus">
        <xsd:annotation>
          <xsd:documentation>The status of the network resource pairing.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the netwrok resource pairing, at the point in time specified by the effectivePeriod of the status.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="Pairing">
        <xsd:annotation>
          <xsd:documentation>Holds the network resource pairing and locking attributes.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="networkResource" type="csom:NetworkResource" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A network resource that is included in the pairing.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>An effective period of the pairing, with respect to the usage context.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:PairingStatus" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The status of the network resource pairing.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="financialAccountNumber" type="csom:FinancialAccountNumber"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Financial account number is the public identifier of the customer's financial account.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="brand" type="csom:Brand" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The business name under which a TMUS product is sold.  A Customer can have accounts in any Brand, within the same Business Unit. For example, a T-Mobile customer may also have a branded account with a national retailer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>The action to be taken with the network resource pairing data.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the network resource pairing.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>A URI that can be used to return information about the network resource pairing.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:element name="businessUnit" type="csom:BusinessUnit">
        <xsd:annotation>
          <xsd:documentation>The different T-Mobile business units require complete separation of assets. Examples are Magenta, Metro PCS, or business untis for specific national retailers or MVNOs.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:simpleType name="PINUnlockCode">
        <xsd:annotation>
          <xsd:documentation>Unique code that is linked to a specific SIM card, and is usually 8 digits long. Also know as PIN Unlock Key or Personal Unlock Key. If PIN number is entered incorrectly 3 times, SIM card is locked and this code is required to unlock it.
    </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:length value="8"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="IMEI">
        <xsd:annotation>
          <xsd:documentation>International Mobile Equipment Identifier.  Unique identifier for a mobile device on a network.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:normalizedString">
          <xsd:maxLength value="16"/>
          <xsd:minLength value="15"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ResourceStatusCode">
        <xsd:annotation>
          <xsd:documentation>Status of the network resource.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="RESERVED"/>
          <xsd:enumeration value="AVAILABLE"/>
          <xsd:enumeration value="INUSE"/>
          <xsd:enumeration value="AGING"/>
          <xsd:enumeration value="BLOCKED"/>
          <xsd:enumeration value="UNRECOGNIZED"/>
          <xsd:enumeration value="ERROR"/>
          <xsd:enumeration value="OUTOFUSE">
            <xsd:annotation>
              <xsd:documentation>This resource is no longer in use and will not be reused.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="PORTEDOUT">
            <xsd:annotation>
              <xsd:documentation>The resource is not in use in the T-Mobile network because it has been ported out to another carrier.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="BusinessUnit">
        <xsd:annotation>
          <xsd:documentation>The different T-Mobile business units require complete separation of assets. Separate companies that comprise TMUS corporation.  The system restricts a Customer to one Business Unit.  In other words, a Customer in one Business Unit is not visible in any of the other  Business Units of the corporation. For example, a Metro PCS customers billing address cannot be looked up by a Magenta representative.  There is no capability to override this in the system.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Magenta"/>
          <xsd:enumeration value="MetroPCS"/>
          <xsd:enumeration value="MVNO"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="IMSI">
        <xsd:annotation>
          <xsd:documentation>An International Mobile Subscriber Identifier of IMSI is a unique number associated with all GSM and UMTS network mobile phone users.  It is stored in the SIM card.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:normalizedString">
          <xsd:length value="15"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ResourceSubStatusCode">
        <xsd:annotation>
          <xsd:documentation>Sub-status of the network resource.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="LOSTSTOLEN"/>
          <xsd:enumeration value="EIP"/>
          <xsd:enumeration value="ACTIVE"/>
          <xsd:enumeration value="SUBSIDY"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="MSISDN">
        <xsd:annotation>
          <xsd:documentation>Mobile Subscriber Integrated Services Digital Network Number. A number uniquely identifying a subscription in a GSM or a UMTS mobile network. Simply put, it is the telephone number to the SIM card in a mobile/cellular phone.
    			</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:minLength value="10"/>
          <xsd:maxLength value="15"/>
          <xsd:pattern value="[0-9]{10,15}"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="SimType">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="GBA"/>
          <xsd:enumeration value="ISIS"/>
          <xsd:enumeration value="2G">
            <xsd:annotation>
              <xsd:documentation>Legacy 2G sim</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="UICC">
            <xsd:annotation>
              <xsd:documentation>3G sim</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="GSM"/>
          <xsd:enumeration value="MBB">
            <xsd:annotation>
              <xsd:documentation>Mobile Broadband</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Virtual"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="SIMnumber">
        <xsd:restriction base="xsd:token">
          <xsd:minLength value="19"/>
          <xsd:maxLength value="19"/>
          <xsd:pattern value="[0-9]{19}"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="CreditDecisionCode">
        <xsd:annotation>
          <xsd:documentation>Credit Decision Code (CDC)</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:token">
            <xsd:attribute name="scope" type="csom:CreditDecisionCodeScope">
              <xsd:annotation>
                <xsd:documentation>The scope of the credit decision code.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="source" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>Identifies the source system for this credit decision code.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:simpleType name="CreditDecisionCodeScope">
        <xsd:annotation>
          <xsd:documentation>The scope addressed by a particular Credit Decision Code (CDC)</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="EIP"/>
          <xsd:enumeration value="SERVICE"/>
          <xsd:enumeration value="OTHER"/>
          <xsd:enumeration value="LOAN"/>
          <xsd:enumeration value="VOICE"/>
          <xsd:enumeration value="DATA"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="SalesChannelTypeStatus">
        <xsd:annotation>
          <xsd:documentation>Current condition of a SalesChannelType, at the time expressed in the effectivePeriod.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the object, at the point in time specified by the effectivePeriod.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A finer division of code.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="SalesChannelStatus">
        <xsd:annotation>
          <xsd:documentation>Current condition of a SalesChannel, at the time expressed in the effectivePeriod.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the object, at the point in time specified by the effectivePeriod.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A finer division of code.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ServicingOrganization">
        <xsd:annotation>
          <xsd:documentation>(FOR FUTURE USE:  this should probably be a BusinessUnit when the B2B support is integrated into the model)   An organization that uses this Channel in order to service a customer. The organization could be TMobile,  partner, dealer, retailer, etc...  Example, organizations  are TMobile, BestBuy, T-Star...</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="organization" type="csom:Organization" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="servicingOrganizationId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Unique identifier assigned by the system of record.
    				</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="SalesChannelType">
        <xsd:annotation>
          <xsd:documentation>The category of the customer interaction that a sales channel is used for.  For example, sales or distribution.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="channelTypeName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The label by which the channelType is known. This is a user-friendly name, appropriate for display. This should always be populated if possible, as this is the place applications will look for a name to display to users.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="channelTypeCode" minOccurs="0">
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="DIRECT"/>
                <xsd:enumeration value="INDIRECT"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="channelTypeStatus" type="csom:SalesChannelTypeStatus"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>State of the channel type, during the time period defined in the status.  For example, active/inactive.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="InventoryChannelCode">
        <xsd:annotation>
          <xsd:documentation>Sales channel codes used by SAP.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="CARE"/>
          <xsd:enumeration value="DEALER"/>
          <xsd:enumeration value="IVR"/>
          <xsd:enumeration value="MOBILE"/>
          <xsd:enumeration value="NATIONALPARTNER"/>
          <xsd:enumeration value="RETAIL"/>
          <xsd:enumeration value="TELESALES"/>
          <xsd:enumeration value="WEB"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="SalesSubChannelCode">
        <xsd:annotation>
          <xsd:documentation>Sales Sub-Channel Code</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="MarketCode">
        <xsd:annotation>
          <xsd:documentation>The three-letter code that uniquely identifies the market.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:complexType name="PriceListLineEligibility">
        <xsd:annotation>
          <xsd:documentation>In order to be eligible for this price, ALL of the criteria in ONE of the price list line elibigibilities must be satisfied.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:EligibilityCriteria">
            <xsd:sequence minOccurs="0">
              <xsd:element name="beginningLineOfServiceNumber" type="xsd:nonNegativeInteger"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>For network service offers that have tiered pricing, the beginning line number in the tier.  For example if a service costs $10 for the first 2 lines, the "beginning" value is 1 and the "ending" value is 2.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="endingLineOfServiceNumber" type="xsd:nonNegativeInteger"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>For network service offers that have tiered pricing, the ending line number in the tier.  For example if a service costs $10 for the first 2 lines, the "beginning" value is 1 and the "ending" value is 2.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="groupOfLinesPriceIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates when the price is a combined price for a group of lines, instead of a per line price.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="EligibleOfferVariant">
        <xsd:annotation>
          <xsd:documentation>The variant(s) to which this price list line can be applied.  Applicable only to hard goods offers.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="offeringVariantId" type="csom:OfferingVariantId"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Uniquely identifies an offering variant for hard goods that is eligible for this price.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="sku" type="csom:SKU" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The stock keeping unti (SKU) of the hard good offering variant that is eligible for this price.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="ProductChargeType">
        <xsd:annotation>
          <xsd:documentation>Charge types that are used for product offerings.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="PayOffCharge"/>
          <xsd:enumeration value="PrePaymentPenalty"/>
          <xsd:enumeration value="Fee">
            <xsd:annotation>
              <xsd:documentation>The charge is a fee, such as an activation fee or change fee.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Usage">
            <xsd:annotation>
              <xsd:documentation>Charge for metered service.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Refund"/>
          <xsd:enumeration value="Periodic">
            <xsd:annotation>
              <xsd:documentation>Expected monthly payment for a device or monthly recurring charge for a service.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="DownPayment">
            <xsd:annotation>
              <xsd:documentation>Minimum down payment required for a device offering.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Initial">
            <xsd:annotation>
              <xsd:documentation>Required amount of first payment</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Final">
            <xsd:annotation>
              <xsd:documentation>Amount of final payment</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="FullPrice">
            <xsd:annotation>
              <xsd:documentation>The full price for the product, if the customer is not paying in installments.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Deposit">
            <xsd:annotation>
              <xsd:documentation>Amount paid by the customer and held as security against future payments.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Discount">
            <xsd:annotation>
              <xsd:documentation>A one time discount</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="PeriodicDiscount">
            <xsd:annotation>
              <xsd:documentation>A recurring discount</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ProductChargeIncurredType">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="IMMEDIATE"/>
          <xsd:enumeration value="ONUSAGE"/>
          <xsd:enumeration value="ONDEMAND"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="PeriodLengthMeasurementUnit">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="MINUTE"/>
          <xsd:enumeration value="HOUR"/>
          <xsd:enumeration value="DAY"/>
          <xsd:enumeration value="MONTH"/>
          <xsd:enumeration value="QUARTER"/>
          <xsd:enumeration value="YEAR"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="UsageMeasurementUnit">
        <xsd:annotation>
          <xsd:documentation>The unit to be used for measuring usage of this consumable product, usually a service.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="MINUTE"/>
          <xsd:enumeration value="MESSAGE"/>
          <xsd:enumeration value="MB"/>
          <xsd:enumeration value="GB"/>
          <xsd:enumeration value="TB"/>
          <xsd:enumeration value="KB"/>
          <xsd:enumeration value="DAY"/>
          <xsd:enumeration value="MONTH"/>
          <xsd:enumeration value="POINT"/>
          <xsd:enumeration value="UNIT"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="PriceListId">
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="BillingChargeCode">
        <xsd:annotation>
          <xsd:documentation>The billing system charge code that corresponds to this price component when it is added to the customer's bill.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="ServiceUsageChargeCode">
        <xsd:annotation>
          <xsd:documentation>The billing system charge code that identifies a price component when it is added to the customer's bill.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="DATA">
            <xsd:annotation>
              <xsd:documentation>Short-hand name for usage that includes only domestic data.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="ROAMING_DATA">
            <xsd:annotation>
              <xsd:documentation>Short-hand name for usage that includes only roaming data.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="SMS">
            <xsd:annotation>
              <xsd:documentation>Short-hand name for usage that includes only domestic text (SMS).</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="ROAMING_SMS">
            <xsd:annotation>
              <xsd:documentation>Short-hand name for usage that includes only roaming text (SMS).</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="MMS">
            <xsd:annotation>
              <xsd:documentation>Short-hand name for usage that includes only domestic picture text (MMS).</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="ROAMING_MMS">
            <xsd:annotation>
              <xsd:documentation>Short-hand name for usage that includes only roaming picture text (MMS).</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="TALK">
            <xsd:annotation>
              <xsd:documentation>Short-hand name for usage that includes only domestic voice.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="ROAMING_TALK">
            <xsd:annotation>
              <xsd:documentation>Short-hand name for usage that includes only roaming voice.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="TALK_TEXT">
            <xsd:annotation>
              <xsd:documentation>Short-hand name for usage that includes both domestic voice and domestic text (SMS) .</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="TALK_TEXT_WEB">
            <xsd:annotation>
              <xsd:documentation>Short-hand name for usage that includes domestic voice and domestic text (SMS) and domestic data.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="TEXT_ALL">
            <xsd:annotation>
              <xsd:documentation>Short-hand name for usage that includes both domestic text (SMS) and domestic picture text (MMS).</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="HIGH_SPEED_DATA">
            <xsd:annotation>
              <xsd:documentation>Short-hand name for usage that includes domestic data at the highest speed available (aka unthrottled data).</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="TALK_TEXTALL">
            <xsd:annotation>
              <xsd:documentation>Short-hand name for usage that includes both domestic voice and domestic text (MMS and SMS).</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="TALK_TEXTALL_WEB">
            <xsd:annotation>
              <xsd:documentation>Short-hand name for usage that includes domestic voice and domestic text (SMS and MMS) and domestic data.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="OfferingPriceType">
        <xsd:annotation>
          <xsd:documentation>Product offering price type. </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="WithService"/>
          <xsd:enumeration value="WithoutService"/>
          <xsd:enumeration value="EIP"/>
          <xsd:enumeration value="SCORE"/>
          <xsd:enumeration value="Lease"/>
          <xsd:enumeration value="Multiline"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="PriceListLineId">
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="ProductOfferingPriceId">
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:complexType name="AuditContext">
        <xsd:sequence minOccurs="0">
          <xsd:element name="userName" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>User name of the person (or system account) that initiated the event.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="eventTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date and time when the event occurred.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="applicationName" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name of the application where the event was initiated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="channelCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Code identifiying the channel within which the event was initiated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="AuditTrail">
        <xsd:annotation>
          <xsd:documentation>Provides the dates and users involved in creating and updating the object.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="creationContext" type="csom:AuditContext" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Record of when and how the object was created.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="updateContext" type="csom:AuditContext" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Record of when and how the object was last updated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="auditTrail" type="csom:AuditTrail">
        <xsd:annotation>
          <xsd:documentation>Provides the dates and users involved in creating and updating the object.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:complexType name="FinancialProductOfferingReference">
        <xsd:annotation>
          <xsd:documentation>DEPRECATED</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:SimpleProductOfferingReference"/>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:simpleType name="Brand">
        <xsd:annotation>
          <xsd:documentation>The business name under which a TMUS product is sold.  A Customer can have accounts in any Brand, within the same Business Unit. For example, a T-Mobile customer may also have a branded account with a national retailer.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="T-Mobile"/>
          <xsd:enumeration value="MetroPCS"/>
          <xsd:enumeration value="Brightspot"/>
          <xsd:enumeration value="UVM"/>
          <xsd:enumeration value="GoSmart"/>
          <xsd:enumeration value="WalmartFamilyMobile"/>
          <xsd:enumeration value="Solaveil"/>
          <xsd:enumeration value="INCOMM"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="DealerContractType">
        <xsd:annotation>
          <xsd:documentation>Identifies the type of contract that the dealer has with T-Mobile. A given dealer may have different stores operating under different contract types.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:complexType name="ManufacturerStatus">
        <xsd:annotation>
          <xsd:documentation>Current condition of a Manufacturer, at the time expressed in the effectivePeriod.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the Manufacturer, at the point in time specified by the effectivePeriod.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A finer division of statusCode.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ProductSpecificationRelationship">
        <xsd:annotation>
          <xsd:documentation>An object that is used to define different types of relationships between products.	</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="relatedProduct" type="csom:ProductSpecificationReference"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The ProductSpecification that this product is related to. For example, the parent feature ProductSpecification may conflict with another feature.  The other feature is the relatedProduct.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The time period where the relationship between the two products is effective.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="relationshipType" type="csom:ProductSpecificationRelationshipType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Categorizes the relationship between the products.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="relationshipSpecificationValue" type="base:SpecificationValue"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional attributes of the relationship between the Product Specifications.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="productRelationshipId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>The enterprise-wide unique identifier of the product-to-product relationship.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="reason">
          <xsd:annotation>
            <xsd:documentation>The reason for the relationship between the products.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:token">
              <xsd:enumeration value="UNSPECIFIED"/>
              <xsd:enumeration value="INCOMPATIBLE"/>
              <xsd:enumeration value="REQUIRED"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:simpleType name="ProductSpecificationActionCode">
        <xsd:annotation>
          <xsd:documentation>Action code for product specification</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="ADD"/>
          <xsd:enumeration value="REMOVE"/>
          <xsd:enumeration value="RETAIN"/>
          <xsd:enumeration value="ENABLE"/>
          <xsd:enumeration value="DISABLE"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ProductType">
        <xsd:annotation>
          <xsd:documentation>Categorizes the different types of product offered for sale: Service, Device, Accessory, or Financial.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Service"/>
          <xsd:enumeration value="Financial"/>
          <xsd:enumeration value="Device"/>
          <xsd:enumeration value="Accessory"/>
          <xsd:enumeration value="PremiumContent"/>
          <xsd:enumeration value="Plan"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ProductSpecificationRelationshipType">
        <xsd:annotation>
          <xsd:documentation>Categorizes the relationship between the products.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Dependent"/>
          <xsd:enumeration value="Conflicting"/>
          <xsd:enumeration value="Containing"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ProductSubType">
        <xsd:annotation>
          <xsd:documentation>Categorizes the different subtypes of product offered for sale.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Voice"/>
          <xsd:enumeration value="Text"/>
          <xsd:enumeration value="Data"/>
          <xsd:enumeration value="3rdPartyContent"/>
          <xsd:enumeration value="BillableContent"/>
          <xsd:enumeration value="Insurance"/>
          <xsd:enumeration value="Warranty"/>
          <xsd:enumeration value="EarlyUpgrade"/>
          <xsd:enumeration value="EquipmentInstallmentPlan"/>
          <xsd:enumeration value="DeviceTradeIn"/>
          <xsd:enumeration value="ElectronicPrepaidRefill"/>
          <xsd:enumeration value="CardPrepaidRefill"/>
          <xsd:enumeration value="Handset"/>
          <xsd:enumeration value="OtherAccessory"/>
          <xsd:enumeration value="ConnectedWearable"/>
          <xsd:enumeration value="Tablet"/>
          <xsd:enumeration value="SimCard"/>
          <xsd:enumeration value="Coverage"/>
          <xsd:enumeration value="Home"/>
          <xsd:enumeration value="MobileInternet"/>
          <xsd:enumeration value="Wearable"/>
          <xsd:enumeration value="Voucher"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ProductSpecificationId">
        <xsd:annotation>
          <xsd:documentation>The enterprise-wide unique identifier for the specification.  The id is mastered in the enterprise product catalog.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:complexType name="ValidationMessage">
        <xsd:annotation>
          <xsd:documentation>Validation message</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="messageType" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Categorizes the type of message</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="messageCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Code identifying the specific message</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="messageText" type="csom:Text" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Content of the message</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="messageSource" type="xsd:token" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Source of the message</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="GeoCodeId">
        <xsd:annotation>
          <xsd:documentation>A unique code that represents a geospatial coordinate measurement of an exact geographic location and time at, below, or above the surface of the earth.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:token">
            <xsd:attribute name="manualIndicator" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>Flag to indicate if GeoCodeID or TaxAreaID is entered manually by user.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="referenceLayer" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>The reference layer that the geocode id was calculated from.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="usageContext" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>The context within which the geocode id should be used.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="GeographicCoordinates">
        <xsd:annotation>
          <xsd:documentation>The latitude and longitude of the geographic location that corresponds to the address.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="latitude" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Latitude lines run horizontally on a map. Latitude lines are also known as parallels since they are parallel and are an equal distant from each other.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="longitude" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The vertical longitude lines are also known as meridians. They converge at the poles and are widest at the equator (about 69 miles or 111 km apart). Zero degrees longitude is located at Greenwich, England (0). The degrees continue 180 east and 180 west where they meet and form the International Date Line in the Pacific Ocean.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="PostalCode">
        <xsd:annotation>
          <xsd:documentation>A code specifying the postal code of the mailing address.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="CityName">
        <xsd:restriction base="xsd:normalizedString"/>
      </xsd:simpleType>
      <xsd:simpleType name="StateCode">
        <xsd:annotation>
          <xsd:documentation>Code of the state within the USA, where applicable (e.g. WA, TX, GA). Codes are defined by USPS.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:length value="2"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="Direction">
        <xsd:restriction base="xsd:token">
          <xsd:minLength value="1"/>
          <xsd:maxLength value="2"/>
          <xsd:enumeration value="N"/>
          <xsd:enumeration value="NE"/>
          <xsd:enumeration value="E"/>
          <xsd:enumeration value="SE"/>
          <xsd:enumeration value="S"/>
          <xsd:enumeration value="SW"/>
          <xsd:enumeration value="W"/>
          <xsd:enumeration value="NW"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="StoreStatus">
        <xsd:annotation>
          <xsd:documentation>Store status</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="StoreHours">
        <xsd:annotation>
          <xsd:documentation>Describes the days and times when the store is open or closed.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="openPeriod" type="csom:OpenPeriod" minOccurs="0"
             maxOccurs="unbounded"/>
          <xsd:element name="closedDays" type="csom:DayOfWeek" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Day of the week when the store is closed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="OpenPeriod">
        <xsd:sequence minOccurs="0">
          <xsd:element name="startingDayOfWeek" type="csom:DayOfWeek" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The starting day of the week when the store is open during the specified hours.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="endingDayOfWeek" type="csom:DayOfWeek" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The ending day of the week when the store is open during the specified hours. If this is blank, the specified hours apply to only the starting day of the week.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="openingTime" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The clock time when the store opens on the specified day(s).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="closingTime" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The clock time when the store closes on the specified day(s).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="Store">
        <xsd:sequence minOccurs="0">
          <xsd:element name="key" type="csom:DomainKey" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Alternate identifiers for the store.  For example a partner organization might have their own internal store location identifiers.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="location" type="csom:Location" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Information about where the store is physically located.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="dealerCode" type="csom:CommissionsCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Dealer that owns this store.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="partnerId" type="csom:NationalPartnerId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Partner that owns this store.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="relativeDistance" type="csom:Measure" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Distance from the store to the point of inquiry.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="parentStoreId" type="csom:StoreId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Uniquely identifies the primary store that this child store is associated with.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="storeType" type="csom:StoreType" minOccurs="0"/>
          <xsd:element name="marketCode" type="csom:MarketCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The three-letter code that uniquely identifies the market.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="storeHours" type="csom:StoreHours" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The days and times when the store is open or closed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="storeStatus" type="csom:StoreStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Status of the store.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="storeReportType" type="csom:StoreReportType" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Report type for the store</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="aggregateType" type="csom:AggregateType" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Aggregate type in the report</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="tillNumber" type="xsd:token" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Till number on the receipt</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="contractType" type="csom:DealerContractType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the type of contract that the dealer has with T-Mobile. A given dealer may have different stores operating under different contract types.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="licenseNumber" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Store license number</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="secondaryLicenseNumber" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Store's secondary license number</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="csom:StoreId">
          <xsd:annotation>
            <xsd:documentation>The unique  identifier of the store, as defined by the T-Mobile system of record.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:simpleType name="NationalPartnerId">
        <xsd:annotation>
          <xsd:documentation>National partner identification code</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="StoreReportType">
        <xsd:annotation>
          <xsd:documentation>Various report types for the store</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="DailyCashSummaryListingTillReport"/>
          <xsd:enumeration value="ClosingDetailsReport"/>
          <xsd:enumeration value="DepositReport"/>
          <xsd:enumeration value="CardReconciliationReport"/>
          <xsd:enumeration value="DailyTransactionAuditReport"/>
          <xsd:enumeration value="DailyPaymentListingReport"/>
          <xsd:enumeration value="DailyHandsetContractReport"/>
          <xsd:enumeration value="DailyRegisterSummaryListingTillReport"/>
          <xsd:enumeration value="DailyRegisterSummaryListingReport"/>
          <xsd:enumeration value="NoSaleReport"/>
          <xsd:enumeration value="DailyInventoryReport"/>
          <xsd:enumeration value="PriceOverrideReport"/>
          <xsd:enumeration value="PostVoidReport"/>
          <xsd:enumeration value="ReturnsReport"/>
          <xsd:enumeration value="DailyRegisterSummaryListingTillReportReprint"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="StoreType">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="RETAIL">
            <xsd:annotation>
              <xsd:documentation>Magenta Retail</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="TPR">
            <xsd:annotation>
              <xsd:documentation>T-Mobile Premium Retail (franchise)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="DEALER"/>
          <xsd:enumeration value="MPCS">
            <xsd:annotation>
              <xsd:documentation>Metro PCS</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="NationalRetail"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="AggregateType">
        <xsd:annotation>
          <xsd:documentation>Aggregate type in the report</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Sales"/>
          <xsd:enumeration value="Returns"/>
          <xsd:enumeration value="NetSales"/>
          <xsd:enumeration value="StoreCashTotal"/>
          <xsd:enumeration value="StoreAllTotal"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="Attachment">
        <xsd:annotation>
          <xsd:documentation>A file or file location containing information referenced by the owning business object.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="typeCode" type="csom:AttachmentTypeCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Type of attachment, e.g., URL, TEXT, JPG, PNG, HTML, PDF</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="creationTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date Time of creation of the document in the system of record</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice>
            <xsd:element name="characterData" type="xsd:token" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Character data such as embedded data, character data and URI.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="URI" type="xsd:anyURI" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>URI indicating where the attachment is located</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Specification group for named value pairs.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the attachment record.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="PersonName">
        <xsd:annotation>
          <xsd:documentation>The name of a person, parsed into its name elements.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="firstName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Is the Persons given name or first name</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="middleName" type="xsd:normalizedString" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Is the persons middle name or initial.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="familyName" type="xsd:normalizedString" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Is the persons family name</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="title" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Is the title of the person. For example Dr, Senator, Mr..</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="prefix" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Prefix to the last name that is not considered part of the last name, such as German von or French dela, etc.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="suffix" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Suffix to the last name, such as Senior, III, the Third etc. 
    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="csom:ActionCode"/>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the name being used by the person, with the knowledge that names may change over time.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="DeviceSpecification">
        <xsd:annotation>
          <xsd:documentation>DEPRECATED</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:ProductSpecificationBase">
            <xsd:sequence minOccurs="0"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="SpecificationValue">
        <xsd:annotation>
          <xsd:documentation>A specification is a property of an object that can be assigned a value.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:token">
            <xsd:attribute name="currencyCode">
              <xsd:annotation>
                <xsd:documentation>The currency of the specification value, if the data type is currency.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:length value="3"/>
                  <xsd:pattern value="[A-Z][A-Z][A-Z]">
                    <xsd:annotation>
                      <xsd:documentation>Upper case alpha</xsd:documentation>
                    </xsd:annotation>
                  </xsd:pattern>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="dataType" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>The data type of the value.  The recognized data types are specified by the xml w3c standard, minus the namespace prefix.  For example xsd:boolean would be expressed as boolean.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="measurementUnit" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>The unit of measure of the value, if applicable.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="name" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>The name of the Specification value; a label by which the value is commonly known.  For example a name = "LongDescription" for a value that contains a long description.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="precision" type="xsd:nonNegativeInteger">
              <xsd:annotation>
                <xsd:documentation>For a numeric value, how many total digits are allowed.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="scale" type="xsd:nonNegativeInteger">
              <xsd:annotation>
                <xsd:documentation>For a numeric value,the number of digits to the right of the decimal point.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="ServiceOfferingReference">
        <xsd:annotation>
          <xsd:documentation>DEPRECATED</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:ProductOfferingReference"/>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ProductOfferingRelationship">
        <xsd:sequence minOccurs="0">
          <xsd:element name="productOffering" type="csom:ProductOfferingVariant"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Reference information identifying the related product offering.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0"/>
          <xsd:element name="priority" type="csom:Ranking" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Numeric ranking of the product offering relationship, to indicate which relationships are most important, implying that they should be presented to the Customer earlier.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="relationshipType" type="csom:OfferingRelationshipType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Categorizes the type of relationship between the two product offerings.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="reason" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A reason why the additional offering is being presented to the customer. Used most often with the CrossSell offering relationship type.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="Compatible"/>
                <xsd:enumeration value="Recommended"/>
                <xsd:enumeration value="Popular"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="relationshipSpecificationValue" type="base:SpecificationValue"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional information about the product offering relationship, stored as named value pairs.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="offerLevel" type="csom:OfferLevel" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Scope of the offer</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="minimumQuantity" type="xsd:integer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Minimum required quantity to order. In some relationship types, such as required, the target of the relationship is a set of offers, and a min and max of the set is required.  For example if there are a set of 3 target offers, and only 1 of the 3 from the set is required, minimumQuantity =1 and maximumQuantity = 1.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="maximumQuantity" type="xsd:positiveInteger" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Maximum quantity allowed to order. In some relationship types, such as required, the target of the relationship is a set of offers, and a min and max of the set is required.  For example if there are a set of 3 target offers, and only 1 of the 3 from the set is required, minimumQuantity =1 and maximumQuantity = 1.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="offeringRelationshipId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the relationship between the two Product Offerings.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="DriversLicense">
        <xsd:sequence minOccurs="0">
          <xsd:element name="comment" type="xsd:normalizedString" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Comments about the driver license such as restrictions like glasses required or supporting notes for violations etc.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="birthDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date of birth as reported on the driver's license.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>The unique identifier found on the person's driver's license.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="Payment">
        <xsd:annotation>
          <xsd:documentation>A payment is a record of the information regarding the transfer of funds to settle an account, in full or in part.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="status" type="csom:PaymentStatus" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Status of the Payment. SAMPLE VALUES: Entered, Posted, Closed</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paymentVendorId" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Third party that processed the payment on behalf of T-Mobile.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="authorizationAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Amount that the payment method authorizes.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="requestAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Amount that the payer requests to pay.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="confirmationNumber" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Payment confirmation number generated by the payee party's system.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="processingTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Time when the payee's Payment system processed the payment
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paymentMethodCode" type="csom:PaymentMethodCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Payment method code. Values can be "Cash", "Credit Card", "Direct Debit" etc.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="allocationAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total amount allocated to document receivables such as Invoices.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="payFromParty" type="csom:Contact" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Refers to the identification and address of the PayFrom Party
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice>
            <xsd:element name="voucherRedemption" type="csom:VoucherRedemption"
               minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Payment is made by redeeming a voucher.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="bankPayment" type="csom:BankPayment" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Bank Account transaction details for the cases where payment method is : Direct Debit, Check or EFT</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="creditCardPayment" type="csom:CreditCardPayment"
               minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Credit Card Payment and authorization Details</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="debitCardPayment" type="csom:DebitCardPayment"
               minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Debit Card Payment and authorization Details</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="merchantId" type="csom:MerchantId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A unique ID used to identify the Merchant. The merchant must use the value assigned by payment gateway vendor. Each store is considered a separate merchant.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="preAuthorizedIndicator" type="csom:Indicator" minOccurs="0"/>
          <xsd:element name="paymentChannelCode" type="csom:PaymentChannelCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The channel in which the payment was originally performed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paymentSource" type="csom:PaymentSource" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the system that was the origin of the payment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paymentSchedulingType" type="csom:BillPaymentSchedulingType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The payment scheduling type notes whether the payment was made as part of an installment plan, as a recurring payment (like EasyPay), or a one time payment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="transactionType" type="csom:PaymentClassificationCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>This field represents the type of financial transaction being performed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paymentCharge" type="csom:Charge" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional charge added at the time of payment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="reversalIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates that the existing transaction is being or has been reversed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="authorizationChannel" type="csom:PaymentChannelCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Payment authourization channel code.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="pointOfSaleReceiptNumber" type="xsd:normalizedString"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Receipt number at point of sale.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="manualPaymentIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates if this is manually posted payment or not.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Specification group for name value pairs.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="csom:PaymentActionCode">
          <xsd:annotation>
            <xsd:documentation>The action to be taken with the Payment.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="paymentId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the payment.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>A URI to link back to the Payment resource.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="DatePeriod">
        <xsd:sequence minOccurs="0">
          <xsd:element name="startDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Start Date of the Date Period.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="endDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>End Date of the Date Period.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="DomainKey">
        <xsd:annotation>
          <xsd:documentation>The Domain Key structure is used to uniquely identifiy an object in a system (or domain) other than the system of record.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="keyName" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the key identifier, within the specified domain. For example, if an object can be identified by a field called code, then keyName = "code".</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="keyValue" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The value of the key identifier.  For example, if an object can be identified by a field called code, then keyName = "code".  The keyValue would be set to the value of the field called "code".</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="domainName" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>The system or business domain that is the source of the key. For example: PREPAIDBILLING, WEBCATALOG, SAMSON, SAP.			</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="PaymentProfile">
        <xsd:annotation>
          <xsd:documentation>Information about how the customer chooses to pay. May include saved payment methods: credit card details, bank accounts, etc.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="paymentMethodCode" type="csom:PaymentMethodCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Specifies the Payment Type. Example values can be: Card, Cash, Direct Debit, Draft, Letter of Credit, GIRO, Check ( Send me a bill)
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="currencyCode" type="csom:CurrencyCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Currency code for this Payment Profile
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paymentTerm" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The period allowed for a customer to pay off the amount due on charges assigned to this Pay Channel.  For example 0, 5, 30, 180 days.
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Code specifying the action to be taken with this Payment Profile in a given transaction, if any.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="paymentProfileId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Identifies the Payment Profile.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>The URI that can be used to access information from this payment profile.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="ServiceSpecification">
        <xsd:annotation>
          <xsd:documentation>DEPRECATED Product specification for a service.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:ProductSpecificationBase">
            <xsd:sequence minOccurs="0"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="Shipping">
        <xsd:annotation>
          <xsd:documentation>Shipping information includes all the attributes that are used to manage the shipping and transportation of the goods in an order.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="modeOfTransportCode" type="csom:ModeOfTransportCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the Shipping method of the order.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="freightCarrier" type="csom:FreightCarrier" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the freight carrier being used to ship the order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="freightTermCode" type="csom:FreightTermCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the Freight terms of the shipment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="requestedShipTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the default date by which the buyer or customer requests the order to be shipped. 
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="promisedShipTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The default date by which the order line was promised to be shipped from the supplier to the customer.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="requestedDeliveryTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the default date goods or services must be delivered to the customer, as specified by the customer. 
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="promisedDeliveryTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The default date by which the order line was promised to be delivered to the customer.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="shipTo" type="csom:ShipToContact" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name and address of the party to whom the order is to be shipped.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="shipFrom" type="csom:Contact" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name and address of the party from whom the order is to be shipped.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="note" type="csom:Note" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Shipping notes</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="serviceLevelCode" type="csom:ServiceLevelCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Shipping carrier priority code such as 2nd day, next day, priority, etc.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="fobPointCode" type="csom:FOBPointCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Free On Board point code for shipping.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="actualShippingTime" type="xsd:dateTime" minOccurs="0"/>
          <xsd:element name="actualDeliveryTime" type="xsd:dateTime" minOccurs="0"/>
          <xsd:element name="waiveShipFee" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Flags whether shipping fee has been waved for this order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="residentialShippingIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the shipping address is for a residence.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="xsd:token"/>
        <xsd:attribute name="id" type="csom:ShippingId">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the shipment. May be tracking info supplied by the freight carrier.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="SalesChannelReference">
        <xsd:annotation>
          <xsd:documentation>Describes the manner in which contact is made between T-Mobile and the customer.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="salesChannelCode" type="csom:SalesChannelCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A short-hand unique identifier of the channel, for a specific usage.  For example codeKey="CDC" usageContext = "WEBCATALOG"</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="name" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The label by which the channel is known. This is a user-friendly name, appropriate for display. This should always be populated if possible, as this is the place  	applications will look for a name to display to users.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The primary description of the channel.  Note: only english description is needed here.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="salesSubChannelCode" type="csom:SalesSubChannelCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Sales Sub-Channel Code</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="subChannelCategory" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Sub-Channel category, such as Frontline Support Teams, Business TeleSales, Small Business TeleSales, Web TeleSales Inside Sales, Small Business Mid-Market, Major Accounts</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="inventoryChannelCode" type="csom:InventoryChannelCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Sales channel code from SAP</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>A URI that can be used to retrieve information about the Sales Channel.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="salesChannelId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the Sales Channel.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="SpecificationGroup">
        <xsd:annotation>
          <xsd:documentation>A group of named value pairs.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="specificationValue" type="base:SpecificationValue"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A specification is a property of an object that can be assigned a value.  SpecificationValue provides a structure to contain   additional details about an object, beyond the attributes that are already defined as concrete, named elements.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>The name of the group of specifications.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="PlannedPayment">
        <xsd:annotation>
          <xsd:documentation>A Planned Payment defines a payment installment promised by the customer as part of a Payment Arrangement.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="plannedPaymentDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date on which the planned payment is to be made.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="plannedPaymentAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The monetary amount to be paid in this payment installment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="parentURI" type="csom:ReferenceURI"/>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="QuotationLineReference">
        <xsd:sequence minOccurs="0">
          <xsd:element name="description" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Description of the quotation line.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="quotationLineStatus" type="csom:QuotationLineStatus"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Status of the quotation line.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="quoteType" type="csom:QuoteType" minOccurs="0"/>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The time period during which the quotation line if effective, if different from the effective period of the quotation as a whole.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="quotationLineId" type="csom:QuotationLineId"/>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="CreditProfileStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="SubscriberContact">
        <xsd:annotation>
          <xsd:documentation>Contact information for the customer that is the primary user of this line of service.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:Contact">
            <xsd:attribute name="subscriberId" type="xsd:token"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="Party">
        <xsd:annotation>
          <xsd:documentation>A party is a legal entity when the entity can be either a person or an organization.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:choice>
            <xsd:element name="organization" type="csom:Organization" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Populated when the Party is an Organization.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="person" type="csom:Person" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Populated when the Party is a Person.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="partyRelationship" type="csom:PartyRelationship"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Specifies a relationship held between one party and one of more other parties.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="partyId" type="csom:PartyId">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies a Party. May be either a Person or an Organization.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="PrivacyProfile">
        <xsd:annotation>
          <xsd:documentation>Privacy controls, indicating the circumstances in which communication is acceptable.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="optOut" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates the opt out option.
    If true, this communication cannot be used for telemarketing calls, mails, emails...etc. </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="DEFAULT"/>
                <xsd:enumeration value="OPTIN"/>
                <xsd:enumeration value="OPTOUT"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="reason" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates the reason for applying the Opt Out, if any.
    					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="activityType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates the type of marketing activity that the customer has opted out from. If no value is provided, it is assumed that the customer has opted out from all marketing activity through this communication channel. </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="BILLING"/>
                <xsd:enumeration value="MARKETING"/>
                <xsd:enumeration value="MAILING"/>
                <xsd:enumeration value="DealsAndPromotions"/>
                <xsd:enumeration value="PersonalizedOffers"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="SIM">
        <xsd:sequence minOccurs="0">
          <xsd:element name="imsi" type="csom:IMSI" minOccurs="0"/>
          <xsd:element ref="csom:simNumber" minOccurs="0"/>
          <xsd:element name="simType" type="csom:SimType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Code identifying the type of SIM, such as GBA or ISIS. The type of SIM implies enabling a type of service. GBA sims are required to enable wifi calling.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="pinUnlockKey1" type="csom:PINUnlockCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The PIN Unlock Key is a unique code that is linked to a specific SIM card, and is usually 8 digits long. Also know as PIN Unlock Code or Personal Unlock Key. If PIN number is entered incorrectly 3 times, SIM card is locked and this code is required to unlock it.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="pinUnlockKey2" type="csom:PINUnlockCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Second PIN Unlock Key, used in the case of a dual SIM.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="virtualSim" type="csom:SIMnumber" minOccurs="0"
             maxOccurs="unbounded"/>
          <xsd:element name="embeddedSIMIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if the SIM is embedded in the device. See http://www.gsma.com/connectedliving/embedded-sim.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="OrganizationLocation">
        <xsd:complexContent>
          <xsd:extension base="csom:Location">
            <xsd:sequence minOccurs="0"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="Charge">
        <xsd:sequence minOccurs="0">
          <xsd:element name="typeCode" type="csom:ProductChargeType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Type of Charge</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="chargeFrequencyCode" type="csom:CalendarFrequency"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Frequency of Charge. Example monthly,yearly,quarterly</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice>
            <xsd:element name="basisAmount" type="csom:Amount" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>The amount on which the charge is calculated. This may be different from the line amount</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="amount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Charge Amount in the transactional currency</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice>
            <xsd:element name="calculationPercent" type="xsd:float" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>The percentage applied to the Basis Amount or Basis Quantity to calculate the charge. Either the CalculationPercent or the CalculationNumericRate can be specified</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:choice>
            <xsd:element name="discountAmount" type="csom:Amount" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>The Amount by which the price is discounted</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="chargeCode" type="csom:BillingChargeCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The billing system charge code that correspnods to this price component when it is added to the customer's bill.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="waiverIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the charge was waived.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="waiverReason" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The reason the charge was waived, if applicable.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="manuallyAddedCharge" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if the charge (fee) is manually added or not.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="promotion" type="csom:PromotionReference" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Promotion associated with the charge</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="feeType" type="csom:ServiceProductFeeType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Fee type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="productOffering" type="csom:ProductOfferingReference"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Product offering associated with the charge</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="taxInclusiveIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if tax is included</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="feeOverrideAllowed" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates if the fee is allowed for override</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice>
            <xsd:element name="overrideThresholdPercent" type="xsd:float" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Percentage threshold for override</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="overrideThresholdAmount" type="csom:Amount" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Amount threshold for override</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="supervisorID" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identification of supervisor for override</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="overrideAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Override amount</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="overrideReason" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Reason for override</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="productOfferingPriceId" type="csom:ProductOfferingPriceId"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>ProductOffering Price Identification</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="proratedIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if the charge is prorated</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="chargeId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the charge.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="TimePeriod">
        <xsd:annotation>
          <xsd:documentation>A span of time defined by any two of these three: a start time, a duration, and an end time.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="startTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Start DateTime of the Time Period</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="endTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>End Date Time of the Time Period</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="AddressCommunicationStatus">
        <xsd:annotation>
          <xsd:documentation>Condition of the AddressCommunication, at the time expressed in the effectivePeriod.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the object, at the point in time specified by the effectivePeriod.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="VALIDATED"/>
                  <xsd:enumeration value="NONVALIDATED"/>
                  <xsd:enumeration value="UNDELIVERABLE"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode">
              <xsd:annotation>
                <xsd:documentation>A finer division of code.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="OVERWRITTEN"/>
                  <xsd:enumeration value="Confirmed"/>
                  <xsd:enumeration value="UnitUnconfirmed"/>
                  <xsd:enumeration value="UnitMissing"/>
                  <xsd:enumeration value="Indistinct"/>
                  <xsd:enumeration value="ZipError"/>
                  <xsd:enumeration value="FalsePositive"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="OrderSchedule">
        <xsd:annotation>
          <xsd:documentation>Shipping, pricing and quantity details for the order line.
              </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="totalAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total value of the items on this sales order line schedule including taxes or charges.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="extendedAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Total value of the items on this sales order line schedule not including taxes or charges.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation> 
    Description of the sales order line schedule
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="orderQuantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    The quantity of the line item ordered.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="reason" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Reason for changing the order.For cancelling the order, the reason will be identified in Status.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="shippedQuantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Quantity of the line item shipped.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:OrderScheduleStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Status of the sales order schedule line. </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="unitListPrice" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    List Price Per Quantity for the line item on this schedule.
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="unitSalePrice" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Selling Price Per Quantity for the line item on this schedule.
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="servicePeriod" type="csom:TimePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Duration of the Service
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="customerPurchaseOrderLine" type="xsd:normalizedString"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Identifies the customer purchase order line and schedule associated with this Sales order line schedule, should also contain  information such as customer item net price, customer payment term, etc.
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="orderScheduleDeduction" type="csom:Deduction" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation> 
    Details about the Allowance,Discounts applicable on the sales order schedule line
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="orderScheduleCharge" type="csom:Charge" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation> 
    Details about the Charges applicable on the sales order schedule line
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="orderSchedulePayment" type="csom:OrderPayment"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation> 
    Details about all prepayments received for the sales order line schedule. 
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="orderScheduleTax" type="csom:Tax" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation> 
    Details of Tax applicable to this line schedule. If tax is exempt, then exemption component in Tax will be populated with Exemption related information such as Number and reason.
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paymentSchedule" type="csom:PaymentSchedule" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Installment payment schedule for a new payment contract.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="freightCharge" type="csom:Charge" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Freight charge associated with this cart schedule.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="calculationType" type="csom:CalculationType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Price calculation type for order schedule.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalTaxAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total tax amount</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="shippingId" type="csom:ShippingId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifier of shipping</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="csom:ActionCode">
          <xsd:annotation>
            <xsd:documentation>The action to be taken with the order schedule data.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Line Schedule Identification.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="Port">
        <xsd:annotation>
          <xsd:documentation>The information required to move or port a telephone number (MSISDN) from one service provider to another, or from one billing system to another.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="portRequestId" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The unique identifier generated by the Samson daemon for this port request.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="MSISDN" type="csom:MSISDN" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The MSISDN being ported.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="transitionalMSISDN" type="csom:MSISDN" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The temporary MSISDN used to give the subscriber service while waiting for the port to complete.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="requestorBillingSystem" type="csom:BillingSystemCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The billing system that initiated the port in request. This billing system is requesting the right to use the MSISDN.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="requestorAccountNumber" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The account number in the billing system that will be receiving the MSISDN.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="donorBillingSystem" type="csom:BillingSystemCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The billing system that is being asked to release the MSISDN for porting.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="donorAccountNumber" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The account number in the billing system that will be donating the MSISDN.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="donorAccountPassword" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The security password or PIN that will be used to validate the port request at the donor billing system.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="oldServiceProvider" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The service provider code of the tier 1 service provider being asked to release the MSISDN.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="oldVirtualServiceProvider" type="csom:VirtualServiceProvider"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>MVNO or other service partner previously using the MSISDN, if applicable.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="newServiceProvider" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The service provider code of the tier 1 service provider asking to receive the MSISDN.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="newVirtualServiceProvider" type="csom:VirtualServiceProvider"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>MVNO or other service partner requesting to use the MSISDN, if applicable.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="portDueTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The due date for completion of the port.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="portRequestedTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date requested  for completion of the port.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="portSubmittedTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date when the port was submitted.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="portCompletedTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date and time when the port was completed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="address" type="csom:Address" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The billing address of the porting MSISDN's account in the donor billing system.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:PortStatus" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="simType" type="csom:SimType" minOccurs="0"/>
          <xsd:element name="simPuk1" type="xsd:token" minOccurs="0"/>
          <xsd:element name="cityCode" type="xsd:token" minOccurs="0"/>
          <xsd:element name="npa" type="csom:NPA" minOccurs="0"/>
          <xsd:element name="nxx" type="csom:NXX" minOccurs="0"/>
          <xsd:element name="lastUpdateDate" type="xsd:dateTime" minOccurs="0"/>
          <xsd:element name="msisdnChangeIndicator" type="xsd:token" minOccurs="0"/>
          <xsd:element name="oldServiceProviderName" type="csom:Name" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the tier 1 service provider being asked to release the MSISDN.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="oldVirtualServiceProviderName" type="csom:Name"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name of the MVNO or other service partner previously using the MSISDN, if applicable.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="originalOrderId" type="csom:OrderId" minOccurs="0"/>
          <xsd:element name="oldVirtualServiceProviderId" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the MVNO or other service partner previously using the MSISDN, if applicable.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="portSpecification" type="csom:SpecificationGroup"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional information about the port saved as named value pairs.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice minOccurs="0">
            <xsd:element name="personProfile" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>The identifying attributes of the responsible person supplying the MSISDN from the donor billing system.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:complexContent>
                  <xsd:extension base="csom:Contact">
                    <xsd:sequence>
                      <xsd:element ref="csom:ssn" minOccurs="0"/>
                    </xsd:sequence>
                  </xsd:extension>
                </xsd:complexContent>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="organization" type="csom:Organization" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>The identifying attributes of the responsible organization supplying the MSISDN from the donor billing system.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="newServiceProviderName" type="csom:Name" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the tier 1 service provider being asked to receive the MSISDN.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="csom:PortActionCode">
          <xsd:annotation>
            <xsd:documentation>Specifies the action to be taken with the request.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="Activity">
        <xsd:annotation>
          <xsd:documentation>An occurrence of a business activity that causes a change in the status of the customer account. For example new service activation, feature addition, cancel service.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="activityName" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The unique name of the business activity.  For example: Activation, ChangeRatePlan</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Description of the processing activity.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="activityStatus" type="csom:ActivityStatus" minOccurs="0"
             maxOccurs="unbounded"/>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The time frame where the Activity is use (effective).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="activityId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the business activity definition.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="ActivityInstanceStatus">
        <xsd:annotation>
          <xsd:documentation>Condition of an ActivityInstance, at the time expressed in the effectivePeriod.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the object, at the point in time specified by the effectivePeriod.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A finer division of code.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="CreditProfile">
        <xsd:sequence>
          <xsd:element name="riskCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Specifies the Credit Risk
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="creditLimitAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The total amount that the customer is allowed to spend on purchases before the system sends a workflow message. Maximum value of total loans regardless of LCA or cdc_MPEC values  (effective credit limit is min of (cdc_MPEC, crp_MPEC,sum(LCA)))
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="nextCreditReviewDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    The next date that credit will be reviewed
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paymentGracePeriod" type="xsd:duration" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Maximum number of overdue days allowed before action is taken
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="creditScore" type="xsd:integer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates the credit score of the customer. Primary credit score used in credit decision for population in regulatory notices.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:CreditProfileStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Status as of Effective Date   
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="behaviorScore" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A score assessing credit worthiness based purely on the past performance of the customer in their business relationship with T-Mobile.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0"/>
          <xsd:element name="creditApplicationReference" type="xsd:normalizedString"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Reference number for cross reference to credit decisioning system</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalNumberOfVoiceLines" type="xsd:integer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The maximum number of voice lines that the customer's credit risk profile will support. This is a total across all accounts for which the customer has responsibility.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalNumberOfBroadbandLines" type="xsd:integer"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The maximum number of broadband lines that the customer's credit risk profile will support. This is a total across all accounts for which the customer has responsibility.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="scoreRange" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The scale or range of values within which the primary credit score used in credit decision falls, for population in regulatory notices.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="decisionSource" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The source of the credit decision.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="adverseActionReason" type="xsd:normalizedString"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A reason for an adverse action, stating why the applicant has been denied credit, for population in appropriate regulatory notices.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="creditDecisionCode" type="csom:CreditDecisionCode"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The Credit Decision Code (CDC) provides the result of the Credit Check and the following action.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional information to include with the credit profile, as a list of named values.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="scoreRank" type="csom:Ranking" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Score percentile assigned for population in regulatory notices.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="score" type="csom:Score" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Pair of label tag and score value used in credit decisioning process. One score element will occur for each score considered in the process.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="eipFCRAIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>If TRUE, then EIP related values cannot be viewed without triggering event;  if event trigger, then appropriate regulatory notice must be provided</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="eipFCRAConsentIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>If TRUE then customer has consented to appropriate regulatory notice and EIP values may be viewed</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="serviceFCRAIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>If TRUE then service related values cannot be viewed without triggering event;  if event trigger, then appropriate regulatory notice must be provided</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="serviceFCRAConsentIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>If TRUE then customer has consented to appropriate regulatory notice and Service values may be viewed</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the credit profile.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>The URI that can be used to access information from this credit profile.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="UsageUnitPrice">
        <xsd:sequence minOccurs="0">
          <xsd:element name="chargeCode" type="csom:ServiceUsageChargeCode"
             minOccurs="0"/>
          <xsd:element name="amountPerUse" type="csom:Amount" minOccurs="0"/>
          <xsd:element name="usageMeasurementUnit" minOccurs="0" maxOccurs="unbounded">
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="MINUTE"/>
                <xsd:enumeration value="MESSAGE"/>
                <xsd:enumeration value="MB"/>
                <xsd:enumeration value="GB"/>
                <xsd:enumeration value="TB"/>
                <xsd:enumeration value="KB"/>
                <xsd:enumeration value="DAY"/>
                <xsd:enumeration value="MONTH"/>
                <xsd:enumeration value="POINT"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="minimumUsageOccurrences" type="xsd:nonNegativeInteger"
             minOccurs="0"/>
          <xsd:element name="maximumUsageOccurrences" type="xsd:nonNegativeInteger"
             minOccurs="0"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="SimpleProductOfferingBase">
        <xsd:complexContent>
          <xsd:extension base="csom:ProductOfferingBase">
            <xsd:sequence minOccurs="0">
              <xsd:element name="productSpecification" type="csom:ProductSpecificationReference"
                 minOccurs="0"/>
            </xsd:sequence>
            <xsd:attribute name="sku"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="LineOfService">
        <xsd:annotation>
          <xsd:documentation>An arrangement for making use of services on a continuing or periodic basis. It serves as the collection point for the record of service offerings and resources that enable a line of service.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:LineOfServiceReference">
            <xsd:sequence minOccurs="0">
              <xsd:element name="note" type="csom:Note" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Note concerning the subscription.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="assignedProduct" type="csom:AssignedProduct"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation/>
              </xsd:element>
              <xsd:element name="currencyCode" type="csom:CurrencyCode" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation> 
    Default Billing Currency for this Subscription
                                    </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="preferredLanguage" type="csom:PreferredLanguage"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The preferred language of communication in the specified usage context. ISO 639-1 Code (en-US,en-CA,en-GB)</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="effectivePeriod" type="csom:EffectivePeriod"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The time period where the subscription is effective.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="market" type="csom:Market" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The T-Mobile market that owns the subscription.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="promotion" type="csom:SubscriptionPromotion"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Promotions associated with this subscription.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="balanceSummaryGroup" type="csom:BalanceSummaryGroup"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Balance information for the subscription</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="activityHistory" type="csom:ActivityInstance"
                 minOccurs="0" maxOccurs="unbounded"/>
              <xsd:element name="promotionCode" type="xsd:token" minOccurs="0"
                 maxOccurs="unbounded"/>
              <xsd:element name="memberLineOfService" type="csom:MemberLineOfService"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Identifies a Line of Service that participates in a sharing relationship with another line of service. Other Line of Service is assumed from the context.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="portPath" type="xsd:normalizedString" minOccurs="0"/>
              <xsd:element name="spendingLimit" type="csom:Amount" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The spending limit is a threshold set by the customer for a single billing cycle. When total incidental charges on the line for that month exceed the monthly limit, the new charges would not be authorized.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
                 minOccurs="0" maxOccurs="unbounded"/>
              <xsd:element name="employeeLineDesignation" type="csom:EmployeeLineDesignation"
                 minOccurs="0"/>
              <xsd:element name="depositRequiredIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Was a deposit required at the time of activation?</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="lineTenure" type="csom:Tenure" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Duration of the life of the line of service.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="resourceUnit" type="csom:ResourceUnit" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Balances and limits available for particular resources.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="fraudIndicator" type="csom:Indicator" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Fraud indicator for line of service.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="fraudLock" type="csom:Indicator" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>This flag allows setting of fraud lock at line of service level. This must prevent any changes at the line level.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="billingType" type="csom:BillingTypeCode" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>A shorthand identifying name for the type of billing settlement: Prepaid or Postpaid. This is a property of the Financial Account's Billing Arrangement, but is repeated within the Line of Service for convenience.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="privacyProfile" type="csom:PrivacyProfile"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Privacy profile associated with the line of service for marketing preference notification</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="fraudLockEffectivePeriod" type="csom:EffectivePeriod"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Specifies the time period during which the line of service is (or was) locked for suspected fraud.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="FinancialAccountReference">
        <xsd:annotation>
          <xsd:documentation>Contains the identifying information for a Customer's financial (billing) account.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element ref="csom:financialAccountNumber" minOccurs="0"/>
          <xsd:element name="accountType" type="csom:AccountType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Specific classification of account. </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="accountName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name of the account, normally assigned by the Customer to help them recognize the intended purpose of the account.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="billingMethod" type="csom:BillingMethod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Billing Method specifies the billing experience that the customer has qualified for and requested.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:FinancialAccountStatus" minOccurs="0"
             maxOccurs="unbounded"/>
          <xsd:element name="accountClassification" type="csom:AccountClassification"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Classifications that this Billing Account has been classified into. </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="marketCode" type="csom:MarketCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The three-letter code that uniquely identifies the market for this financial account.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="billCycle" type="csom:BillCycle" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The beginning and ending time period where a group of accounts are run through the billing process.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="key" type="csom:DomainKey" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Unique keys that identify the financial account in systems other than the system of record.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Code identifying the action to be taken with this financial account in a given transaction.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="financialAccountId" type="csom:FinancialAccountId">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the Customer's financial (billing) account within the billing system.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="parentURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>The URI which can be used to access the parent of this object. In the case of Financial Account, the parent is the Customer Account.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>The URI that can be used to access information from this financial account.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="CustomerAccountStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="BillCycle">
        <xsd:annotation>
          <xsd:documentation>Number of days between one invoicing period (during which invoices are prepared and dispatched) and the next.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="dayOfMonth" type="xsd:integer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The day of the month on which this bill cycle is typically run.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="code" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The bill cycle code is used to identify a group of accounts that will be processed together during the monthly billing. This code may be related to the day of the month when the account records are usually processed, but is not necessarily equal to it, since more than one bill cycle may be run during the same calendar day.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Start date and end date of bill cycle.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="nextBillingPeriod" type="csom:DatePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The next billing period is defined by the start and end dates for the upcoming bill cycle on the account. This data is calculated at time of message population, not stored values.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="uiAddedIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate that the entity is added from user interface.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Action to be taken with this bill cycle in the current transaction.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="billCycleId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>The unique identifier of a Bill Cycle.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="OrderCustomer">
        <xsd:annotation>
          <xsd:documentation>Information about the party that placed the order.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:CustomerReference">
            <xsd:sequence minOccurs="0">
              <xsd:element name="creditProfile" type="csom:CreditProfile" minOccurs="0"/>
              <xsd:element name="taxExemption" type="csom:TaxExemption" minOccurs="0"
                 maxOccurs="unbounded"/>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="BankPayment">
        <xsd:sequence minOccurs="0">
          <xsd:element name="actionTypeCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Type of EFT (Electronic Funds Transfer) transaction attempted.  Sample Values : DEBIT, CREDIT, VERIFY, VALIDATE</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="DEBIT"/>
                <xsd:enumeration value="CREDIT"/>
                <xsd:enumeration value="VERIFY"/>
                <xsd:enumeration value="VALIDATE"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="instructionCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Unique identification of the Bank payment Instruction that the PayFrom party instructs the bank</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="settlementPriorityCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The priority with which the financial institution or payment system should settle payment for this document</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="reason" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Payment Reason code determining the purpose of the payment. Usage: This code helps to process and fulfill the payment properly</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="authorizationMethodCode" type="csom:AuthorizationMethodCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Method by which the payer authorized the transaction.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="deliveryMethodCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Method by which the transaction is delivered.  Example of some values can be: ACH, FASCIMILE</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="transferTypeCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Bank account payment transfer type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="referenceCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Payment system-provided reference code for this payment</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="chargeBearerCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Bearer of bank account charges.  Specifies which party/parties will bear the charges associated with the processing of the payment transaction. Example of some values can be: Credit, Debit, Shared, Service Level</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="chargeAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Bank charge amount in payer's Bank account currency</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="payFromBankAccount" type="csom:BankAccount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Payer's Bank Account</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="payeeBankAccount" type="csom:BankAccount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Payee's Bank Account</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="chargeProcessingBankAccount" type="csom:BankAccount"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Bank Account that pays the Bank Charge amount, if different from the Payer's Bank Account              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paymentDebitContact" type="csom:Contact" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Contact information related to the Debit Notification        </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paymentRemittanceContact" type="csom:Contact" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Contact information related to the Remittance</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice>
            <xsd:element name="clearingChannelCode" type="xsd:token" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Clearing channel to be used for the transaction. Valid values are : Book Transfer, Mass Payment Net System, Real Time Gross Settlement System, Real Time Net Settlement System</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="serviceLevelCode" type="xsd:token" minOccurs="0">
              <xsd:annotation/>
            </xsd:element>
          </xsd:choice>
          <xsd:choice>
            <xsd:element name="check" type="csom:Check" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Details of the check in case of a check payment</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="paymentDirectDebitMandate" type="csom:PaymentDirectDebitMandate"
               minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Mandate for the Direct Debit in case of a Direct debit payment</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="approvalCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Approval code from the bank authorizing this payment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="chargeAccountNumberIndicator" type="csom:ChargeAccountNumberIndicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates what form of card number is presented.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="CustomerStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>Code that identifies the status of the Customer.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="SimpleProductOfferingReference">
        <xsd:complexContent>
          <xsd:extension base="csom:ProductOfferingReference">
            <xsd:attribute name="sku" type="csom:SKU"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="FraudCheckStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="100">
                    <xsd:annotation>
                      <xsd:documentation>Successful transaction</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                  <xsd:enumeration value="101">
                    <xsd:annotation>
                      <xsd:documentation>The request is missing one or more required fields.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                  <xsd:enumeration value="102">
                    <xsd:annotation>
                      <xsd:documentation>One or more fields in the request contains invalid data.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                  <xsd:enumeration value="150">
                    <xsd:annotation>
                      <xsd:documentation>Error: General system failure.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                  <xsd:enumeration value="151">
                    <xsd:annotation>
                      <xsd:documentation>Error: The request was received but there was a server time-out. This error does not include time-outs between the client and the server.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                  <xsd:enumeration value="152">
                    <xsd:annotation>
                      <xsd:documentation>Error: The request was received but there was a service time-out.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                  <xsd:enumeration value="202">
                    <xsd:annotation>
                      <xsd:documentation>Vendor declined the request because the card has expired. You may also receive this reason code if the expiration date that you provided does not match the date on file at the issuing bank.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                  <xsd:enumeration value="231">
                    <xsd:annotation>
                      <xsd:documentation>The account number is invalid.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                  <xsd:enumeration value="234">
                    <xsd:annotation>
                      <xsd:documentation>There is a problem with your merchant configuration for the payment vendor.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                  <xsd:enumeration value="400">
                    <xsd:annotation>
                      <xsd:documentation>The fraud score exceeds your threshold.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                  <xsd:enumeration value="480">
                    <xsd:annotation>
                      <xsd:documentation>The order is marked for review by Decision Manager.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                  <xsd:enumeration value="481">
                    <xsd:annotation>
                      <xsd:documentation>The order is rejected by Decision Manager.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ProductFee">
        <xsd:annotation>
          <xsd:documentation>This is information that is used by back-office systems and/or legacy billing for certain fees that are charged when actions result from a customer purchasing an offering.  For example, activation fees or rate plan change fees.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="name" type="xsd:normalizedString" minOccurs="0"/>
          <xsd:element name="description" type="csom:Description" minOccurs="0"/>
          <xsd:element name="code" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The product catalog's short-hand name for the fee.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="feeDetail" minOccurs="0">
            <xsd:complexType>
              <xsd:sequence minOccurs="0">
                <xsd:element name="amount" type="csom:Amount" minOccurs="0"/>
                <xsd:choice minOccurs="0">
                  <xsd:element name="oneTimeCharge" type="xsd:boolean" minOccurs="0"/>
                  <xsd:sequence minOccurs="0">
                    <xsd:element name="recurringFeeFrequency" type="csom:BillingFrequencyCode"
                       minOccurs="0"/>
                    <xsd:element name="recurringFeeChargeInstances" type="xsd:int"
                       minOccurs="0"/>
                  </xsd:sequence>
                </xsd:choice>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0"/>
          <xsd:element name="feeType" type="csom:ServiceProductFeeType" minOccurs="0"/>
          <xsd:element name="feeGroup" type="csom:ServiceProductFeeGroup" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="feeId" type="xsd:token"/>
      </xsd:complexType>
      <xsd:complexType name="BillingArrangement">
        <xsd:annotation>
          <xsd:documentation>Options for billing the customer or creating an invoice.  The billing arrangement contains information such as how payment is made (credit card, check, or bank account) and whether the customer receives a paper and/or electronic bill. </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="currencyCode" type="csom:CurrencyCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Currency Code for this Billing Profile. Bills created from this Billing Profile will have this currency as the primary currency code.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="languageCode" type="csom:LanguageCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    The language per ISO 639-1 Code: en for English, es for espagnol (Spanish)</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="billTypeCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the Bill is Detail or summary</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="SUMMARY"/>
                <xsd:enumeration value="DETAIL"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="mediaTypeCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The type of media on which the bill is to be presented. For example, printed, electronic transmission or electromagnetic (tape, diskette, and so on).</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="ELECTRONIC"/>
                <xsd:enumeration value="ELECTROMAGNETIC"/>
                <xsd:enumeration value="PAPER"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="mediaFormatCode" type="csom:MediaFormatCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An identifier for the type of media such as tape, CD, or diskette on which the bill is to be copied for distribution to the customer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="numberOfCopies" type="xsd:integer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Number of copies (printed or copies of the media) that are to be made at the time of printing.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:TimePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Effective Period of this Billing Profile</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:BillingArrangementStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Status of the Billing Arrangement.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="billingContact" type="csom:Contact" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Billing Proflie Contact</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="csom:paymentProfile" minOccurs="0"/>
          <xsd:element name="billingTypeCode" type="csom:BillingTypeCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A shorthand identifying name for the type of billing settlement: Prepaid or Postpaid.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="billColorIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the bill is printed in grayscale or in color</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="iBillSubscriptionIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicator for sending Universal Bill XML files to I-Bill</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="largeBillIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicator for DST to print Bill on large piece of paper (for disabled people)</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="csom:BillingArrangementActionCode"/>
        <xsd:attribute name="billingArrangementId" type="csom:BillingArrangementId">
          <xsd:annotation>
            <xsd:documentation>Unique identifier of the billing arrangement.  Assigned by the system of record.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="parentURI" type="csom:ReferenceURI"/>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="CardNumber">
        <xsd:annotation>
          <xsd:documentation>Payment card number</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:token">
            <xsd:attribute name="maskingType" type="csom:CCMaskingType">
              <xsd:annotation>
                <xsd:documentation>The type of masking to be applied to the credit card number for security and privacy.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="AssignedProduct">
        <xsd:annotation>
          <xsd:documentation>A Product Offering that has been procured by a customer, or other interested interested party.  The Assigned Product includes information about Parties who may have an interest arising from ownership/lease, usage, maintenance management, billing etc. It is an instance of the Product Specification, for a specific customer.  The Assigned Product may include relationships to other Assigned Products such as a like component or substitute.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:AssignedProductReference">
            <xsd:sequence minOccurs="0">
              <xsd:element name="processingStatus" type="csom:AssignedProductProcessingStatus"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Status of the processing of this assigned product.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="unitPurchasePrice" type="csom:Amount" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The price paid per unit of the assigned product
                                   </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="purchaseTime" type="xsd:dateTime" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation> 
    The datetime when the assigned product was purchased
                                    </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="parentAssignedProductId" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Reference to the parent assigned product</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="relatedAssignedProductId" type="xsd:token"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Other assigned products to which the assigned product may be related - the relationship may be a substitute/backup, a device related to a financial product, etc.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="componentAssignedProductId" type="xsd:token"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The components that make up the assigned product. The components are assigned product themselves.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="sharedAssignedProductId" type="xsd:token" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>other assigned products that are shared by this assigned product.  For example, this assigned product could share a bucket of data.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
                 minOccurs="0" maxOccurs="unbounded"/>
              <xsd:element name="balanceSummaryGroup" type="csom:BalanceSummaryGroup"
                 minOccurs="0" maxOccurs="unbounded"/>
              <xsd:element name="percentFinanced" type="xsd:decimal" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Percent of product purchase price financed under installment plan.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="deviceManufacturer" type="csom:Manufacturer"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Manufacturer of the customer owned device.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="deviceModel" type="xsd:normalizedString" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Model of the customer owned device.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="oldServiceProvider" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Communications carrier from which a trade in or unlocked device originated.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="eligibilityEvaluation" type="csom:EligibilityEvaluation"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Result of evaulation of eligibility criteria, as specified in the eligibility type.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="warranty" type="csom:Warranty" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Details of the warranty in effect for customer-owned equipment (device or accessory).</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="reason" type="xsd:normalizedString" minOccurs="0"/>
              <xsd:element name="mobileOS" type="xsd:normalizedString" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Operating system of the mobile device</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="firmware" type="xsd:normalizedString" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Firmware of the device</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="deviceRooting" type="csom:DeviceRooting" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Device rooting details</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="theftLock" type="csom:TheftLock" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Theft lock details</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="lastUseDate" type="xsd:date" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The date when the device was last used in TMO network</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="firstUseDate" type="xsd:date" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The date when the device was first used in TMO network.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="lastTMODiagnosticDate" type="xsd:date" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The latest date when the device is diagnosed by TMO</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="isPoweredOn" type="csom:Indicator" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>To indicate if the device is currently powered on</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="isScreenCracked" type="csom:Indicator" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>To indicate if the device screen is cracked.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="isLiquidDamaged" type="csom:Indicator" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>To indicate whether the device has been damaged by any liquid.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="returnChargeStatus" type="csom:ReturnChargeStatus"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Charge status for the returned device</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="AddressList">
        <xsd:sequence minOccurs="0">
          <xsd:element name="addressCommunication" type="csom:ExtendedAddressCommunication"
             minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="CustomerAccountReference">
        <xsd:annotation>
          <xsd:documentation>Contains the identifying information for a Customer's primary account. 
    The Customer Account captures all aspects of a customer's account including care, financial, ordering, billing, etc. 
    May include multiple financial accounts that are the resopnsibility of this customer.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="customerAccountNumber" type="csom:CustomerAccountNumber"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Customer Account Number is the public identifer for the account that represents the sum of all of the business that a given customer is doing with our company.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:CustomerAccountStatus" minOccurs="0"/>
          <xsd:element name="segmentationId" type="csom:SegmentationId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Segmentation Id represents the customer active data center; this is used to route messages to appropriate data center. All financial accounts under customer will be at the same data center.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="key" type="csom:DomainKey" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Unique keys that identify the customer account in systems other than the system of record.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="csom:businessUnit" minOccurs="0"/>
          <xsd:element name="programMembership" type="csom:ProgramMembership"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Program(s) in which the customer is participating, e.g. Advantage, Lifeline, TMUS Employee, Rural Healthcare, etc.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Group of named value pairs</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Specifies the action to be taken with the customer account object in a given transaction.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="customerAccountId" type="csom:CustomerAccountId">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the Customer Account within the billing system.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="parentURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>The URI which can be used to access the parent of this object. In the case of Customer Account, the parent is the Customer.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>The URI which can be used to access information about this Customer Account.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="CustomerReference">
        <xsd:sequence minOccurs="0">
          <xsd:element name="key" type="csom:DomainKey" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Unique keys that identify the customer in systems other than the system of record.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="customerType" type="csom:CustomerType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Type of Customer Account. Some examples are Internal customers, Revenue generating external customers etc.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:CustomerStatus" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Status of the customer party (e.g., Active, Inactive etc.)</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="levelCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>New customer, prospect, regular,  short term and temporary</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="creditClassification" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The calculated credit classification of the customer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="party" type="csom:Party" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Legal entity that may be either a person or an organization.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="customerGroup" type="csom:CustomerGroup" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The following customer groups are being considered for customer segmentation: Consumer, Business, Special/Other, Government.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Specifies the action to be taken with the Customer object as a result of an operation with the action code as an input.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="customerId" type="csom:CustomerId">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the customer.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>The URI that can be used to return information about this Customer.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="FinancialAccount">
        <xsd:annotation>
          <xsd:documentation>A formal business arrangement providing for regular dealings or services, and involving the establishment and maintenance of a record of debit and credit entries to cover transactions involving a particular Customer.  
    An invoice is prepared relative to a single financial account for a given bill cycle.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:FinancialAccountBase">
            <xsd:sequence minOccurs="0">
              <xsd:element name="lineOfService" type="csom:LineOfServiceReference"
                 minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="OrderExtension">
        <xsd:annotation>
          <xsd:documentation>This extension of the Order object includes order tasks, and accompanying Port requests in the Order Lines.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:OrderBase">
            <xsd:sequence minOccurs="0">
              <xsd:element name="billTo" type="csom:BillTo" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Name and address of the party to be billed for the order.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="shipping" type="csom:Shipping" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Details of the shipping arrangements for the order.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="businessUnitName" type="csom:BusinessUnit"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The business unit within the context of which the Customer Order is defined.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderSalesCommission" type="csom:SalesCommission"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Commissioning details associated with this order.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderLine" type="csom:OrderLineExtension" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Order lines provide the details of the product offerings being ordered. An order line may be for a product assembly or a component. 
    Additional order lines for components may be associated with the order line for the assembly.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderCharge" type="csom:Charge" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Charges and fees applied at the order level, not based on a specific line item.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderDeduction" type="csom:Deduction" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Discounts and rebates applied at the order level, not based on a specific line item.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderTax" type="csom:Tax" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Taxes applied at the order level, not based on a specific line item.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderPayment" type="csom:OrderPayment" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Payment information associated with the order. For complex orders payment information may be located on individual order schedules, reflecting partial shipments.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderAttachment" type="csom:OrderAttachment"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>References to additional documentation for the order.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderTypeVersionID" type="xsd:normalizedString"
                 minOccurs="0"/>
              <xsd:element name="versionName" type="xsd:normalizedString" minOccurs="0"/>
              <xsd:element name="orderTypeID" type="xsd:token" minOccurs="0"/>
              <xsd:element name="lastUpdateDate" type="xsd:dateTime" minOccurs="0"/>
              <xsd:element name="orderStatus" type="csom:OrderStatus" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Details about the status of the Customer Order.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="cartId" type="csom:CartId" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Unique identifier for the Cart from which this Order was built.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="addressList" type="csom:AddressList" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>List of addresses used in this Order.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="activityInstance" type="csom:ActivityInstance"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Activities or tasks associated with the order</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderSummary" type="csom:OrderSummary" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>An aggregation of order data according to the scope indicated in the summary scope.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="validationMessage" type="csom:ValidationMessage"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Order validation message</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ProductOffering">
        <xsd:annotation>
          <xsd:documentation>A proposition to sell one or more products to a customer. A product offering applies a price to a product, and is associated with eligibility rules in the product catalog.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:ProductOfferingBase">
            <xsd:sequence minOccurs="0">
              <xsd:element name="offeringVariant" type="csom:OfferingVariant"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Describes the detailed information for each variation of the offer.  For example, a device in various memory or color options.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="productOfferingComponent" type="csom:ProductOfferingComponent"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Product offering components are product offerings that are included within another product offering.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="updateContext" type="csom:AuditContext" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Latest update context</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="productSpecification" type="csom:ProductSpecificationBase"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Product specifications that are included within a product offering.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="standAloneOfferIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Defines whether an offer can be purchased standalone or only as a part of a package.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="advertizedPrice" type="csom:Amount" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The advertized price is the lead-in price for a packaged offer component.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="breakableIndicator" type="csom:Indicator" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>A Breakable package allows the customer to initially place the package in their shopping cart, but remove the component offers of the package individually. A Non-breakable package does not allow the customer to remove the individual component offers from the shopping cart; if one component offer is removed from their cart, all component offers must be removed or removed automatically before the order is accepted</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ProductSpecificationReference">
        <xsd:sequence minOccurs="0">
          <xsd:element name="name" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The common name of the product specification, as defined in the product catalog.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="shortName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The short name of the product specification, as defined in the product catalog.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="displayName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The display name of the product specification, as defined in the product catalog.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:DescriptionByChannel" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The common description of the product specification, as defined in the product catalog.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="shortDescription" type="csom:DescriptionByChannel"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The short description of the product specification, as defined in the product catalog.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="longDescription" type="csom:DescriptionByChannel"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The long description of the product specification, as defined in the product catalog.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="keyword" type="base:SpecificationValue" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A named value pair to provide additional details about the product specification.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="productType" type="csom:ProductType" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Categorizes the different types of product offered for sale: Service, Device, Accessory, or Financial.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="productSubType" type="csom:ProductSubType" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Categorizes the different subtypes of product offered for sale.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="sku" type="csom:SKU" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Stock Keeping Unit (SKU) used to track the specified product in inventory.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Effective period for the specification reference.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="productSpecificationType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Categorization of product specification.It can be customer-facingsoft good bundles,lower-level soft goods features (such as a feature code specification for Call Waiting) or hard goods family.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="SoftGoodFeatureBundle"/>
                <xsd:enumeration value="SoftGoodFeature"/>
                <xsd:enumeration value="HardGoodFamily"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="csom:ProductSpecificationActionCode">
          <xsd:annotation>
            <xsd:documentation>Specifies the action to take with the product specification</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="productSpecificationId" type="csom:ProductSpecificationId">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies a product specification.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>A URI that can be used to retrieve details about this Product Specification.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="OrderBase" abstract="true">
        <xsd:sequence minOccurs="0">
          <xsd:element name="totalAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total value of the items on this sales order including taxes or charges.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalTaxAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total tax of the items on this sales order. This is the sum of all the line item Tax Amounts</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="extendedAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total value of the items on this sales order not including taxes or charges. This is the sum of all the line item Extended Amounts
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="orderTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the date the order was submitted.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="orderLocation" type="csom:Store" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The geographic location where the order was placed. May be needed for tax purposes.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paymentMethodCode" type="csom:PaymentMethodCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the default payment method applicable for the sales order This is not the actual payment information. It is just information about how the sales order will be paid. 
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="reason" type="csom:OrderReason" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Reason code for changing the order. For cancelling the order, the reason will be identified in Status.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="orderType" type="csom:OrderType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the type of this Order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="salesChannel" type="csom:SalesChannelReference"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>This indicates the channel of the order: Web, Call Center, Distributor A, Retail.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="note" type="csom:Note" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation> 
    Notes about the order.
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="creditVerificationSuccess" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Flag to indicate whether Customer's Credit Check passed or not
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="customerPurchaseOrder" type="xsd:normalizedString"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the default customer purchase order associated with this order. Primarily used for B2B.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="applicationName" type="csom:Application" minOccurs="0"/>
          <xsd:element name="statusHistory" type="csom:OrderStatusHistory" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A record of the status changes that have occurred during the life of the order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The effective period of the Order allows future ordering, or an expiration date after which the order is no longer valid for submission.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="promotion" type="csom:Promotion" minOccurs="0"
             maxOccurs="unbounded"/>
          <xsd:element name="orderSpecification" type="csom:SpecificationGroup"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Name value pairs for additional order information, such as for fraud check.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="relatedOrder" type="csom:RelatedOrder" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Existing order that is related to this order, according to the specified relationship type.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="marketCode" type="csom:MarketCode" minOccurs="0"/>
          <xsd:element name="ipAddress" type="csom:IPAddress" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Internet protocol address where the order was submitted online.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="deviceFingerprintId" type="csom:DeviceFingerprintId"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Uniquely identifies the device that was used to submit the order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="backOrderAllowedIndicator" type="csom:Indicator"
             minOccurs="0"/>
          <xsd:element name="termsAndConditionsDisposition" type="csom:TermsAndConditionsDisposition"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Disposition of the terms and conditions: whether they were accepted, how, and when.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="bypassPaymentProcessing" type="csom:Indicator"
             minOccurs="0"/>
          <xsd:element name="customerPurchaseOrderDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date when the customer purchase order was approved.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalAmountExcludingTax" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total monetary amount of the order excluding taxes.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="deliveryBlockIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Inidcates that delivery is being blocked, as in the case of a pre-order or backorder scenario.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="freightCharge" type="csom:Charge" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Freight (shipping) charge associated with this order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="fraudCheckResponseIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>UI checked out order to OMS via ESP now OMS is doing CC authorization and response from VESTA come as fraud check review and UI end they will do what ever necessary step required (asking fraud related questions/calling fraud care rep ) then try to confirm the Order again to OMS..this time UI will pass flag either true/false to update OMS (based on this flag OMS will decide to submit order again or cancel it).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="bypassInventoryCheck" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>UI checked out order to OMS via ESP now OMS is doing inventory check get response from SAP that inventory is not available/back order and UI end they will do what ever necessary step required (removing that item from cart/ update the quantity/ get concent from customer to submit order with backorder SKU/item) then try to confirm the Order again to OMS..this time UI will pass flag either true/false to update OMS (based on this flag OMS will skip the inventory check).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalDueAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total payment amount due at the time the order was placed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalRecurringDueAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total recurring charge that resulted on the customer's billing account after placing this order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="originalOrderId" type="csom:OrderId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The original order id in the case of a return or exchange.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="modeOfExchange" type="csom:ModeOfExchange" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Mode of Exchange (Advance, Post, Immediate). Advance exchange means customer sends in their device before receiving the replacement. Post means new device is esnt first and Customer sends in old one after receiving the new one. Immediate means the exchange is made in store with rep. </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalRecurringTaxAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total recurring tax amount resulting from this order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalRecurringFeeAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total recurring fee amount resulting from this order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="reasonDescription" type="csom:Description" minOccurs="0"/>
          <xsd:element name="pointOfNoReturnReached" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicate if the order is reached point of no return.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="fraudCheckStatus" type="csom:FraudCheckStatus"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Status of fraud check for the payment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="salesperson" type="csom:PersonReference" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the saesperson credited with the order. Normally, the salesperson who entered it.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalDeltaDueMonthlyAmount" type="csom:Amount"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Difference between currentRecurringChargeAmmount and totalRecurringChargeAmount</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalSoftGoodDueNowAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Portion of totalDueAmount that applies to softgoods</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalHardGoodDueNowAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Portiion of totalDueAmount that applies to hardgoods</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalDuePayNowAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The amount the customer has to pay now.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="bypassFraudCheck" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates to bypass fraud check during the processing flow.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="orderSubType" type="csom:OrderSubType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Order sub type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="csom:OrderActionCode">
          <xsd:annotation>
            <xsd:documentation>The action to be taken with the order data.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="orderId" type="csom:OrderId">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies this order.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="Tax">
        <xsd:sequence minOccurs="0">
          <xsd:element name="code" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Tax Code</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="taxJurisdiction" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Code used to identify the taxing authority that is applicable for a specific location</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="amount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Tax Amount in the transactional currency</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="typeCode" type="csom:TaxTypeCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identify the type of Tax. An example is type State, with ID as MA</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice>
            <xsd:element name="basisAmount" type="csom:Amount" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>The amount on which the tax is calculated. This may be different from the line amount</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="taxExemption" type="csom:TaxExemptionIndicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the customer is tax exempt and identifies the pertinent exemtption.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="recurringIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the tax is applied on a periodic basis.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="taxableItemIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the item was determined to be taxable with respect to this particular tax type.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="jurisdictionType" type="csom:JurisdictionType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The type of tax jurisdiction for the taxing authority</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the tax record.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="Note">
        <xsd:sequence minOccurs="0">
          <xsd:element name="entryTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date and time when the note was entered/created.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="noteType" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An extensive list of note types (500+) are defined for systematic use. Business rules determine which note to apply in a given situation.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="content" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Addiitonal descriptive information entered by the author to supplement the system text.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="author" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the author of the note.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Unique identifier assigned by the system of record.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="language" type="csom:LanguageCode">
          <xsd:annotation>
            <xsd:documentation>Identifier for the language used in the description.  Value is the ISO 639-1 Code, a two letter code such as   (en-US,en-CA,en-GB)defined at www.w3.org/standards </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="ClaimReference">
        <xsd:annotation>
          <xsd:documentation>Basic reference information for an insurance claim.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="claimTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date when the claim was initiated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="lossDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date when the loss occurred.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="serialNumber" type="csom:SerialNumber" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The serial number of the device, which may be the IMEI for a GSM device.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="sku" type="csom:SKU" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The SKU or Stock Keeping Unit of the device.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="claimStatus" type="csom:ClaimStatus" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The status of the claim.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="lossDescription" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A description of the loss incurred.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="problemDescription" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A description of the problem with the device.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="xsd:token"/>
        <xsd:attribute name="claimId" type="csom:ClaimId"/>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="LineOfServiceStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="csom:LineOfServiceStatusCode">
              <xsd:annotation>
                <xsd:documentation>Code for the status of the Line Of Service.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode" type="xsd:token"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ServiceProductFeeType">
        <xsd:annotation>
          <xsd:documentation>A categorization of fees, used by UIs for the sales process.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="name" type="xsd:normalizedString" minOccurs="0"/>
          <xsd:element name="description" type="xsd:normalizedString" minOccurs="0"/>
          <xsd:element name="code" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The product catalog's short-hand name for the fee type.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="feeTypeId" type="xsd:token"/>
      </xsd:complexType>
      <xsd:complexType name="ProductInstance">
        <xsd:sequence minOccurs="0">
          <xsd:choice>
            <xsd:element name="serialNumber" type="csom:SerialNumber" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Serial Number of the item</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="imei" type="csom:IMEI" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>International Mobile Equipment Identifier.  Unique identifier for a mobile device on a network.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="imsi" type="csom:IMSI" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>An International Mobile Subscriber Identifier or IMSI is a unique number associated with all GSM and UMTS network mobile phone users.  It is stored in the SIM card, and can be used to uniquely identify the SIM card..</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="description" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Description of the item instance</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="OfferingComponentBase">
        <xsd:sequence minOccurs="0">
          <xsd:element name="minimumComponentQuantity" type="xsd:int" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The minimum quantity of the offering component that must be included in the bundled offer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="maximumComponentQuantity" type="xsd:int" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The maximum quantity of the offering component that may be included in the bundled offer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="componentIsDefault" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Flags whether the offering component is part of the default configuration of the offer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="offeringComponentId" type="csom:OfferingComponentId">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies an offering component within a bundled offer.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="parentURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>URI providing a reference to the bundled offer containing the offering component.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="PreferredLanguage">
        <xsd:annotation>
          <xsd:documentation>The preferred language of communication in the specified usage context.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="csom:LanguageCode"/>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="Address">
        <xsd:annotation>
          <xsd:documentation>A place where a person or organization may be communicated with.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="addressFormatType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A code specifying the format of this address(STREET_ADDRESS, POBOX_ADDRESS,MILITARY_ADDRESS,FOREIGN_ADDRESS,HWY_ADDRESS,RURALROUTE_ADDRESS)</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="STREET"/>
                <xsd:enumeration value="POBOX"/>
                <xsd:enumeration value="MILITARY"/>
                <xsd:enumeration value="FOREIGN"/>
                <xsd:enumeration value="HIGHWAY"/>
                <xsd:enumeration value="RURALROUTE"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="addressLine1" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The first free form line, expressed as text, of an address</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="addressLine2" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The second free form line, expressed as text, of an address</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="addressLine3" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The third free form line, expressed as text, of an address</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="addressLine4" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The fourth free form line, expressed as text, of an address</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="cityName" type="csom:CityName" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name, expressed as text, of the city, town or village of this address</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="stateName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name of the state in which  the city falls, where applicable</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="stateCode" type="csom:StateCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Code of the state in which the city falls, where applicable(WA, TX, GA)</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="countyName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>County Name, if applicable</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="countryCode" type="csom:CountryCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The unique identifier of a country for this address (Reference ISO 3166 and UN/ECE Rec 3)</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="countryName" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of a country for this address (Reference ISO 3166 and UN/ECE Rec 3)</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="attentionOf" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name, expressed as text, of a person or department in the organization to whom incoming mail is marked with words such as 'for the attention of' or 'FAO' or 'ATTN' for this address</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="csom:postalCode" minOccurs="0"/>
          <xsd:element name="postalCodeExtension" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An extension to the postalcode of the address, e.g., in 94070-3541, the extension is 3541.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="geoCodeID" type="csom:GeoCodeId" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A unique code that represents a geospatial coordinate measurement of an exact geographic location and time at, below, or above the surface of the earth</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="uncertaintyIndicator" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Uncertainty code for geoCode: 'FULL'-Full Address, 'ZIP'-Rural Zip+4, 'E'-Rural Zip+2, 'RURAL'-Rural Zip, 'D'-Street Zip+4, 'L'-Street Zip+2, 'STREET'-Street zip</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="inCityLimitIndicator" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether or not this address is within the taxing boundary of the jurisdiction that's provided in the address.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="geographicCoordinates" type="csom:GeographicCoordinates"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The latitude and longitude of the geographic location that corresponds to the address.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="countyCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A numeric code identifying the county from the Federal Information Processing Standard (FIPS).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="key" type="csom:DomainKey" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="residentialIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if the address is for residential or business</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="BankAccount">
        <xsd:annotation>
          <xsd:documentation>An account held by a party at a financial institution.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="name" type="xsd:normalizedString" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Name on the Bank Account</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="accountNumber" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Bank Account Number</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="IBAN" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>International Bank Account Number</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="currencyCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>CurrencyCode in which the account is maintained</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="typeCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Account Type. Examples are Checking, Savings etc</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="bankAccountHolder" type="csom:Party" minOccurs="0"/>
          <xsd:element name="bank" type="csom:Bank" minOccurs="0"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="Preference">
        <xsd:annotation>
          <xsd:documentation>Determines the relative preference of a list of objects.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="preferred" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicator if this is a preferred communication. If false, this communication should be less used.
    					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="Person">
        <xsd:annotation>
          <xsd:documentation>A person is an individual human acting as a separate legal entity.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="personName" type="csom:PersonName" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name that the person is known by.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="birthPlace" type="csom:BirthPlace" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Place of birth for the person.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="csom:ssn" minOccurs="0"/>
          <xsd:element name="preferredLanguage" type="csom:PreferredLanguage"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The preferred language of communication. ISO 639-1 Code (en-US,en-CA,en-GB)
    					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="addressCommunication" type="csom:ExtendedAddressCommunication"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A person's addresses for residence or business, to be used for deliveries or postal mail.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="birthDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date of birth of the person.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="phoneCommunication" type="csom:PhoneCommunication"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A person's Phone Numbers for voice, fax or text communication.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="emailCommunication" type="csom:EmailCommunication"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A person's email addresses.
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="websiteCommunication" type="csom:WebsiteCommunication"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A person's personal web sites.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="driversLicense" type="csom:DriversLicense" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A person's Drivers License details.
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="nationalIdentityDocument" type="csom:NationalIdentityDocument"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A person's national IDs, such as green cards.
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="personImage" type="csom:Attachment" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A graphical image of the person may be attached for easier recognition. This could also be used to attach biometric data.
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="citizenship" type="csom:Citizenship" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A person's citizenships contain information about the country or countries of which the person is a citizen.
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="passport" type="csom:Passport" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A passport data for each of the person's citizenships
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="visa" type="csom:Visa" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A person's immigration documents, such as visas and work permits.
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="employment" type="csom:Employment" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Employment records of the person.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="gender" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the gender of the person.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="motherMaidenName" type="csom:PersonName" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Family name of the mother of the person, prior to any marriage.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="maritalStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Marital status of the person.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="MARRIED"/>
                <xsd:enumeration value="SINGLE"/>
                <xsd:enumeration value="DIVORCED"/>
                <xsd:enumeration value="WIDOWED"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="ethnicity" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Describe ethnic groups to which individuals belong, identify with, or belong in the eyes of the community.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="HISPANIC">
                  <xsd:annotation>
                    <xsd:documentation>A person of Cuban, Mexican, Puerto Rican, South or Central American, or other Spanish culture or origin, regardless of race.</xsd:documentation>
                  </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="NATIVE AMERICAN">
                  <xsd:annotation>
                    <xsd:documentation>A person having origins in any of the original peoples of North and South America (including Central America) who maintains cultural identification through tribal affiliation or community attachment.</xsd:documentation>
                  </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="ALASKA NATIVE">
                  <xsd:annotation>
                    <xsd:documentation>A person having origins in any of the original peoples of Alaska.</xsd:documentation>
                  </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="ASIAN">
                  <xsd:annotation>
                    <xsd:documentation>A person having origins in any of the original peoples of the Far East, Southeast Asia, or the Indian Subcontinent, including, for example, Cambodia, China, India, Japan, Korea, Malaysia, Pakistan, the Philippine Islands, Thailand, and Vietnam.</xsd:documentation>
                  </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="AFRICAN AMERICAN">
                  <xsd:annotation>
                    <xsd:documentation>A person having origins in any of the black racial groups of Africa.</xsd:documentation>
                  </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="NATIVE HAWAIIAN">
                  <xsd:annotation>
                    <xsd:documentation>A person having origins in any of the original peoples of Hawaii</xsd:documentation>
                  </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="PACIFIC ISLANDER">
                  <xsd:annotation>
                    <xsd:documentation>A person having origins in any of the original peoples of Guam, Samoa, or other Pacific Islands.</xsd:documentation>
                  </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="WHITE">
                  <xsd:annotation>
                    <xsd:documentation>A person having origins in any of the original peoples of Europe, the Middle East, or North Africa.</xsd:documentation>
                  </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="OTHER"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="socialCommunication" type="csom:SocialCommunication"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="activeDutyMilitary" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the Person is an active duty military member. This would normally be supported by a military identification card.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="securityProfile" type="csom:SecurityProfile" minOccurs="0"/>
          <xsd:element name="identityDocumentVerification" type="csom:IdentityDocumentVerification"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Information about verification on person's identity document such as passport, SSN, driver's license.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="personId" type="csom:PersonId">
          <xsd:annotation>
            <xsd:documentation>Enterprise-wide unique identifier of the Person.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>The URI that can be used to return information about this Person.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="FinancialAccountStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="INCOLLECTION"/>
                  <xsd:enumeration value="ACTIVE"/>
                  <xsd:enumeration value="SUSPENDED"/>
                  <xsd:enumeration value="CLOSED"/>
                  <xsd:enumeration value="CANCELLED"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="OrderStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="NEW"/>
                  <xsd:enumeration value="INPROCESS"/>
                  <xsd:enumeration value="CANCELED"/>
                  <xsd:enumeration value="COMPLETE"/>
                  <xsd:enumeration value="PROVISIONED"/>
                  <xsd:enumeration value="RECEIVED"/>
                  <xsd:enumeration value="ERROR"/>
                  <xsd:enumeration value="INPROGRESSBEFOREPNR"/>
                  <xsd:enumeration value="CANCELINPROGRESS"/>
                  <xsd:enumeration value="LOCKEDFORUPDATE"/>
                  <xsd:enumeration value="LOCKEDFORCANCEL"/>
                  <xsd:enumeration value="POSTVOIDED"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="Promotion">
        <xsd:annotation>
          <xsd:documentation>A proposition to entice a customer to purchase one or more offers.  The promotion can provide discounted pricing or additional products as the incentive to purchase the offer(s).  The discount or additional product is called the promotion benefit.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:PromotionReference">
            <xsd:sequence minOccurs="0">
              <xsd:element name="eligibilityCriteria" type="csom:EligibilityCriteria"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Eligibility criteria under which the customer may qualify for this promotional benefit. This is in addition to the product requirement specified elsewhere.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="promotionalBenefit" type="csom:Deduction" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Describes the dollar or percent discount that may be applied as part of this promotion.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="oneTimeOnlyIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates whether this promotion may be applied more than once for the same customer. True if it may only be used once.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="mutuallyExclusiveIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates whether this prmotion may be applied with other promotions. True if it may NOT be used with other promotions.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="recurringIndicator" type="csom:Indicator" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates that the promotion may be applied on a recurring basis for the specified duration.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="promotionDuration" type="xsd:duration" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Duration for which the promotion may continue after having been applied to the customer's account.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="promotionalActionType" type="csom:PromotionalActionType"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Action to be applied as part of the promotion: Discount, Allowance, ProductAddition.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="productBenefit" type="csom:ProductOfferingReference"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Product offering that is included as a benefit of the promotion. (BOGO)</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="quantityGiven" type="csom:Quantity" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Product quantity or size of allowance included as a benefit.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="productEligible" type="csom:ProductOfferingReference"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Product offering that is eligible to receive a benefit from the promotion, such as a discount.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="quantityRequired" type="csom:Quantity" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Quantity of the affected product that must be purchased or used to qualify for the promotion.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderBehavior" type="csom:OrderBehavior" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Parameters controlling the sale of an offer variant in a specifc channel. If no channel is specified, the parameters apply to all channels.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="promotionCode" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Code for this promotion. It is different from promotion ID.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="serializedPromotionCoupon" type="csom:SerializedPromotionCoupon"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>To identifies a Product Catalog Promotion.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="promotionClassification" type="csom:Classification"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Classification of promotion, e.g. Deal</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="productVariantEligible" type="csom:OfferingVariant"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Product variants that are eligible for promotion</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="displayPriority" type="csom:Ranking" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Display priority or ranking</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="EmailCommunication">
        <xsd:complexContent>
          <xsd:extension base="csom:CommunicationBase">
            <xsd:sequence minOccurs="0">
              <xsd:element ref="csom:emailAddress" minOccurs="0"/>
              <xsd:element name="emailFormat" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates that the communication is preferred	in HTML format, Plain Text, Rich Text.
    							</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                  <xsd:restriction base="xsd:token">
                    <xsd:enumeration value="HTML"/>
                    <xsd:enumeration value="PLAINTEXT"/>
                    <xsd:enumeration value="RICHTEXT"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="usageContext" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Identifies the circumstances for which we are to use this email address for communication.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                  <xsd:restriction base="xsd:token">
                    <xsd:enumeration value="PRIMARY"/>
                    <xsd:enumeration value="SECONDARY"/>
                    <xsd:enumeration value="BILLING"/>
                    <xsd:enumeration value="HOME"/>
                    <xsd:enumeration value="WORK"/>
                    <xsd:enumeration value="DATANOTIFY"/>
                    <xsd:enumeration value="OTHER"/>
                    <xsd:enumeration value="SECURITY"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="communicationStatus" type="csom:EmailCommunicationStatus"
                 minOccurs="0"/>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="Organization">
        <xsd:annotation>
          <xsd:documentation>A generic organization: partner, vendor, supplier, B2B customer, corporation, etc.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:OrganizationReference">
            <xsd:sequence minOccurs="0">
              <xsd:element name="stockSymbol" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Corporate Stock Symbol, e.g. ORCL  </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="salutation" type="xsd:normalizedString" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>This is the salutation that needs to be used to refer to the organization. Ex. M/S ( Messrs)</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="incorporation" type="csom:Incorporation" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Details that apply only to organizations that are legally incorporated.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="DUNSInquiryIdentifier" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Identifier used to query the Dun and Bradstreet data. If a location is a branch, then in some countries the headquarters DUNS number is used to do the query. Otherwise it is the same as the DUNS number</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="dunBradstreetCreditRating" type="xsd:integer"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Dun and Bradstreet credit rating   </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="taxPayerRegistrationNumberId" type="xsd:token"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Taxpayer registration number, also known as the VAT number.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="registrationCode" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Type of registration, such as nonprofit  </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="preferredFunctionalCurrencyCode" type="csom:CurrencyCode"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Organization's default currency code    </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="subsidiaryIndicator" type="xsd:boolean" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates if organization is a parent or subsidiary location.  If true, the organization is a subsidiary of another organization.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="privateOwnershipIndicator" type="xsd:boolean"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>If true, indicates if the organization is privately owned.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="internalIndicator" type="xsd:boolean" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>If true, indicates this is an internal T-Mobile organization.   </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="operationalScopeCode" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The physical class of business in which a business participates. For example, local, national, or international   </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="localBusinessId" type="xsd:token" minOccurs="0">
                <xsd:annotation/>
              </xsd:element>
              <xsd:element name="localActivityClassificationCode" type="xsd:token"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Local activity classification code</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="taxationClassificationCode" type="xsd:token"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Taxation classification for corporation entities. For example, Chapter S in the US</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="similarSoundingBusiness" type="xsd:normalizedString"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates existence of similarly named business   </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="homeCountryCode" type="csom:CountryCode" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Home Country    </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="chiefExecutiveOfficerName" type="csom:PersonName"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Name of the person who is the chief executive officer.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="principalPersonName" type="csom:PersonName"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Name of the person who is a principal in the organization.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="bestCallTimeNote" type="xsd:normalizedString"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Best time to contact the organization</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="organizationLocation" type="csom:OrganizationLocation"
                 minOccurs="0" maxOccurs="unbounded"/>
              <xsd:element name="organizationContact" type="csom:Contact" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The URI that can be used to return information about this organization's point of contact.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="organizationCreditRating" type="csom:OrganizationCreditRating"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Credit ratings of an organization</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="importIndicator" type="xsd:boolean" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates if this company is an importer   </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="organizationEmploymentStatistics" type="csom:OrganizationEmploymentStatistics"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Employment statistics of an organization                        </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="organizationCertification" type="csom:Certification"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Certifications of an organization</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="smallBusinessIndicator" type="xsd:boolean"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>If true, indicates this company is considered to be a small business.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="disadvantagedIndicator" type="xsd:boolean"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates if this company is considered disadvantaged by the US governement under Title 8A. This typically includes woman and minority owned businesses, but is a specific certification.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="womanOwnedIndicator" type="xsd:boolean" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>If true, indicates this company is primarily owned by women.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="minorityOwnedType" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The type of minority-owned firm, such as black, hispanic, native american, alaska native, asian american.    </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="minorityOwnedIndicator" type="xsd:boolean"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>If true, indicates this company is primarily owned by ethnic or racial minorities.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="exportIndicator" type="xsd:boolean" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates if this company is an exporter    </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="SalesChannel">
        <xsd:annotation>
          <xsd:documentation>Describes the manner in which contact is made between T-Mobile and the customer.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:SalesChannelReference">
            <xsd:sequence minOccurs="0">
              <xsd:element name="salesChannelStatus" type="csom:SalesChannelStatus"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>State of the channel, during the time period defined in the status.   For example, active/inactive.   Multiple status allow support for historical status information.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="channelType" type="csom:SalesChannelType" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Defines whether the channel is Direct T-Mobile or Indirect.  </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="servicingOrganization" type="csom:ServicingOrganization"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>An organization that uses this Channel in order to service a customer. The organization could be TMobile,  partner, dealer, retailer, etc...  Example, organizations  are TMobile, BestBuy, T-Star...</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="businessFunction" type="xsd:token" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The primary function of the business unit that uses this Channel.  Examples are Assisted Care, Self Care, Sales.    </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="contactMethod" type="xsd:token" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The way that a customer interacts with TMobile (or a partner organization) when using this Channel.  Examples are Web, MobileWeb, Telephone, Retail.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="channelApplication" type="csom:Application"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>TMobile.com, myTMobile.com, RPX, CRM, DUS, Samson;  the Application that is being used by the channel.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="platform" type="xsd:token" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The TMobile middleware platform being used with this Channel.  Examples are ESOA, RSP, Tibco, .NET</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="NetworkResource">
        <xsd:annotation>
          <xsd:documentation>SIM, mobile number, or IMEI used by the customer to gain access to services.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:choice minOccurs="0">
            <xsd:element name="imei" type="csom:IMEI" minOccurs="0"/>
            <xsd:element name="sim" type="csom:SIM" minOccurs="0"/>
            <xsd:element name="mobileNumber" type="csom:MobileNumber" minOccurs="0"/>
          </xsd:choice>
          <xsd:element name="resourceStatus" type="csom:ResourceStatus" minOccurs="0"
             maxOccurs="unbounded"/>
          <xsd:element ref="csom:billingSystem" minOccurs="0"/>
          <xsd:element name="brand" type="csom:Brand" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The business name under which a TMUS product is sold.  A Customer can have accounts in any Brand, within the same Business Unit. For example, a T-Mobile customer may also have a branded account with a national retailer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="csom:businessUnit" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="xsd:token"/>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
        <xsd:attribute name="resourceId" type="xsd:token"/>
      </xsd:complexType>
      <xsd:complexType name="Deduction">
        <xsd:sequence minOccurs="0">
          <xsd:element name="typeCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identify the type of deduction</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="RECURRING"/>
                <xsd:enumeration value="ONETIME"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:choice>
            <xsd:element name="basisAmount" type="csom:Amount" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>The amount on which the deduction is calculated. This may be different from the line amount.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="amount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Deduction Amount in the transactional currency</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice>
            <xsd:element name="calculationPercent" type="xsd:float" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>The percentage applied to the Basis Amount or Basis Quantity to calculate the deduction. Either the CalculationPercent or the CalculationNumericRate can be specified</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="reason" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Reason for applying the deduction</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="OVERRIDE"/>
                <xsd:enumeration value="PROMOTION"/>
                <xsd:enumeration value="VOLUME"/>
                <xsd:enumeration value="GOODWILL"/>
                <xsd:enumeration value="CORRECTION"/>
                <xsd:enumeration value="SCORE"/>
                <xsd:enumeration value="WAIVE"/>
                <xsd:enumeration value="PRICEDISCOUNT"/>
                <xsd:enumeration value="REFUND">
                  <xsd:annotation>
                    <xsd:documentation>Use this enumerated value in context other than cart/order</xsd:documentation>
                  </xsd:annotation>
                </xsd:enumeration>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded"/>
          <xsd:element name="recurringFrequency" type="csom:CalendarFrequency"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Recurring frequency for the deduction</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="promotion" type="csom:PromotionReference" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Promotion associated with the deduction</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="chargeCode" type="csom:BillingChargeCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Billing charge code</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="productOfferingPriceId" type="csom:ProductOfferingPriceId"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Product offering price identification</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="name" type="csom:Name" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name of the deduction</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="taxInclusiveIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if the tax is included for the deduction.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="specificationValue" type="base:SpecificationValue"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Named value pairs</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="realizationMethod" type="csom:DiscountRealizationMethod"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The way that the deduction is delivered to the customer.   Only applicable when the method is is something other than a reduction in the charges for the transaction.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="proratedIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if the deduction is prorated</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="deductionId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the deduction.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="LineOfServiceReference">
        <xsd:sequence minOccurs="0">
          <xsd:element ref="csom:financialAccountNumber" minOccurs="0"/>
          <xsd:element name="activationDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date when subscription's initial activation took place.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="subscriberContact" type="csom:SubscriberContact"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contact information for the customer that is the primary user of this line of service.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="networkResource" type="csom:NetworkResource" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A logical or physical entity that allows a subscriber to use the T-Mobile network. A resource is identified by a managed numbering system.   Examples are MSISDN, IMEI, SIM card number, IMSI. </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="pin" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Personal Identification Number</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="lineOfServiceStatus" type="csom:LineOfServiceStatus"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Statuses of the line of service in their specified time periods or usage contexts.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="primaryLineIndicator" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates if the line is the primary, secondary or additional line of service (contract). The numeric value can be used to sequence the lines of service on the account.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="lineAlias" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Alias or nickname for the line of service, set by customer, to be used in user interface.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="lineSequence" type="xsd:positiveInteger" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Sequence or order of the line in the account.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="csom:LineOfServiceActionCode">
          <xsd:annotation>
            <xsd:documentation>Indicates the type of change that has been made to this version, as compared to the previous version.  </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="lineOfServiceId" type="csom:LineOfServiceId">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the subscription.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="parentURI" type="csom:ReferenceURI"/>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="TaxExemption">
        <xsd:sequence minOccurs="0">
          <xsd:element name="reason" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Reason for the tax exemption</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="certificateNumber" type="xsd:normalizedString"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Tax Exemption Certificate Number</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="issuedTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date Time at which the exemption was issued</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="expirationTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>DateTime of expiration of the exemption</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:TaxExemptionStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The status of the tax exemption.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:TimePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The period during which the tax exemption is being used by the Customer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Description of the tax exemption</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="percentage" type="xsd:float" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Percentage of tax from which this customer is exempted.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="exemptionJurisdiction" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the taxation jurisdiction to which this exemption applies.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="taxTypeCode" type="csom:TaxTypeCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Defines the type of tax to which this exemption applies.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="jurisdictionType" type="csom:JurisdictionType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The type of tax jurisdiction that the customer has received an exemption for.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="taxCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Vertex tax code that maps to this exemption.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the tax exemption.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="PeriodicUnitPrice">
        <xsd:sequence minOccurs="0">
          <xsd:element name="amountPerPeriod" type="csom:Amount" minOccurs="0"/>
          <xsd:element name="periodLengthMeasurementUnit" minOccurs="0">
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="HOUR"/>
                <xsd:enumeration value="DAY"/>
                <xsd:enumeration value="WEEK"/>
                <xsd:enumeration value="MONTH"/>
                <xsd:enumeration value="QUARTER"/>
                <xsd:enumeration value="YEAR"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="minimumNumberOfPeriods" type="xsd:decimal" minOccurs="0"/>
          <xsd:element name="maximumNumberOfPeriods" type="xsd:decimal" minOccurs="0"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="AccountType">
        <xsd:annotation>
          <xsd:documentation>The type of customer.  Examples are Individual, Government, Business. </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:normalizedString">
            <xsd:attribute name="id" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>Identifies a unique combination of account type and subtype.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="subTypeCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>Code specifying the account subtype.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="typeCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>The account type code.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="AccessorySpecification">
        <xsd:annotation>
          <xsd:documentation>DEPRECATED</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:ProductSpecificationBase">
            <xsd:sequence minOccurs="0"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="TaxExemptionStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="code" type="xsd:token"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="Amount">
        <xsd:annotation>
          <xsd:documentation>A number of monetary units specified in a currency where the unit of the currency is explicit or implied</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:decimal">
            <xsd:attribute name="currencyCode" type="csom:CurrencyCode">
              <xsd:annotation>
                <xsd:documentation>For a monetary value, the currency code as defined by the iso 4217 Currency Code standard. For United States dollars, the code is USD. Reference http://www.iso.org/iso/home/standards/currency_codes.htm</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="EffectivePeriod">
        <xsd:annotation>
          <xsd:documentation>The effective period for the object that contains an element of this type. The period may define the duration for a warning period associated with a service for example.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:TimePeriod">
            <xsd:sequence minOccurs="0">
              <xsd:element name="usageContext" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The context within which this effective period applies and should be used.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="Indicator">
        <xsd:simpleContent>
          <xsd:extension base="xsd:token">
            <xsd:attribute name="name" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>An Indicator is a property of an object that can be assigned a value.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="Visa">
        <xsd:sequence minOccurs="0">
          <xsd:element name="numberID" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Control number appearing on the visa</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="typeCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Foreign key for the type of visa or work permit; available values are based on the country, e.g., R for regular, D for diplomat
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="issuingCountryCode" type="csom:CountryCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    The country which issued the visa or work permit
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="validIndicator" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Indicates whether or not the visa is valid
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="issuedDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    The date on which the visa or work permit was issued
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="expirationDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    The date on which the visa or work permit expires
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="entryDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    The date on which the person used the visa to enter the country
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:VisaStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    The status of the visa
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="SubscriptionPromotion">
        <xsd:annotation>
          <xsd:documentation>A record of a Promotion applied to a Subscription in the past, present, or future.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="key" type="csom:DomainKey" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Alternative identifiers for the application of this promotion to this subscription.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="promotionId" type="csom:PromotionId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Uniquely identifies the definition of the promotion being applied to this subscription.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="amount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The currency amount being applied to this subscription by this promotion.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="channel" type="csom:SalesChannelReference" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The channel through which this promotion was applied to the subscription.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A text description of the promotion being applied to the subscription.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The time period during which this promotion will be applied to this subscription.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="note" type="csom:Note" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional information about the way this promotion was applied to the subscription.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:SubscriptionPromotionStatus"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The status of the promotion as it applies to this subscription.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="productOfferingRelationship" type="csom:ProductOfferingRelationship"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Products that influenced the eligibility of this subscription for this promotion.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="balanceSummary" type="csom:BalanceSummary" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Balances related to the promotion being applied to this subscription.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="activityHistory" type="csom:ActivityInstance" minOccurs="0"
             maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Specifies the action to be taken with this promotion relative to this subscription. </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="subscriptionPromotionId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the application of the promotion to this subscription.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="ActivityInstance">
        <xsd:annotation>
          <xsd:documentation>A business activity that causes a change in the status of the object. For example new service activation, feature addition, cancel service.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="activity" type="csom:Activity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Uniquely identifies the business activity definition.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="activityPeriod" type="csom:TimePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Time period that the activity occurred, is occurring or will occur.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="reason" type="xsd:token" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Code identifying the reason for the activity</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="activityInstanceStatus" type="csom:ActivityInstanceStatus"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="performedBy" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The user id of the person that performed this activity.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Group of named value pairs.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Action code</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Unique identifier assigned by the system of record for the occurrence of the business activity.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="PaymentMethod">
        <xsd:annotation>
          <xsd:documentation>The method of payment to be used.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="paymentMethodCode" type="csom:PaymentMethodCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Code identifying the current payment method specified for the payment instrument.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paymentInstrument" type="csom:PaymentInstrument"
             minOccurs="0"/>
          <xsd:element name="previousPaymentMethodCode" type="csom:PaymentMethodCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Code identifying the previous payment method specified for the payment instrument.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Status of the Payment Method.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:complexContent>
                <xsd:extension base="csom:StatusBase">
                  <xsd:attribute name="statusCode">
                    <xsd:annotation>
                      <xsd:documentation>Code identifying the status of the payment method.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="Valid"/>
                        <xsd:enumeration value="Invalid"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:attribute>
                  <xsd:attribute name="subStatusCode">
                    <xsd:annotation>
                      <xsd:documentation>Code identifying the substatus of the payment method.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="AboutToExpire"/>
                        <xsd:enumeration value="Blocked"/>
                        <xsd:enumeration value="Expired"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:attribute>
                </xsd:extension>
              </xsd:complexContent>
            </xsd:complexType>
          </xsd:element>
          <xsd:element name="nickname" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Nickname for the stored payment method.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="isStoredPaymentMethodIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate if the payment method is stored in the system.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Code specifying the action to be taken with this Payment Method in a given transaction, if any.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="paymentMethodId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies a stored payment method.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>The URI that can be used to access information from this payment method.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="ActivityStatus">
        <xsd:annotation>
          <xsd:documentation>Condition of an Activity, at the time expressed in the effectivePeriod.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase"/>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="StatusBase" abstract="true">
        <xsd:annotation/>
        <xsd:sequence minOccurs="0">
          <xsd:element name="name" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The user-friendly label that defines the state of the object, as the point in time expressed by effectivePeriod.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>An explanation of the status.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:TimePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The time during which the object is/was in this Status.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="reasonCode" type="xsd:token" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Specifies the reason for the change in status.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="OrderPayment">
        <xsd:complexContent>
          <xsd:extension base="csom:Payment">
            <xsd:sequence minOccurs="0">
              <xsd:element name="receivedPaymentId" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Unique identifier for the payment transaction.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="receiptMethodCode" type="csom:ReceiptMethodCode"
                 minOccurs="0"/>
              <xsd:element name="cardPresentIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Card is physically present for Credit card and Debit card transactions.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="delayPaymentIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Delay payment for B2B and existing customer.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="storePaymentMethodIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicator if set to true will store the Credit card and return a permanent token </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="isTemporyToken" type="xsd:boolean" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>DEPRECATED. Use OrderPayment/bankPayment/chargeAccountNumberIndicator or OrderPayment/creditCardPayment/creditCard/chargeAccountNumberIndicator or OrderPayment/debitCardPayment/debitCard/chargeAccountNumberIndicator</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="settlementIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicator if the authorization and settlement needs to happen in the same transaction.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="termsAndConditionsDisposition" type="csom:TermsAndConditionsDisposition"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Disposition of the terms and conditions: whether they were accepted, how, and when.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="terminalEntryCapability" type="csom:TerminalEntryCapability"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>An identifier used to indicate the entry mode capability of the terminal.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="terminalType" type="csom:TerminalType" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>An identifier used to describe the type of terminal being used for the transaction.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="terminalEntryMode" type="csom:TerminalEntryMode"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates how the account number was entered on the transaction at Point of Sale (POS).</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="cardCaptureCapability" type="csom:CardCaptureCapability"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>This field represents the terminal capability to capture the card data.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="posConditionCode" type="csom:POSConditionCode"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>An identifier used to indicate the transaction condition at the Point-of-Sale (POS).</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="posId" type="csom:POSId" minOccurs="0"/>
              <xsd:element name="paymentTransactionType" type="csom:FinancialTransactionType"
                 minOccurs="0"/>
              <xsd:element name="retailPinlessFlag" type="xsd:boolean" minOccurs="0"/>
              <xsd:element name="trackData" type="csom:TrackData" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Contains the unaltered track data from the magnetic stripe of the payment card.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="electronicAuthenticationCapability" type="csom:ElectronicAuthenticationCapability"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>A code indicating the electronic (PIN) authentication capability at the Point of Sale (POS).</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="tokenization" type="csom:Tokenization" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>This section contains fields used in TransArmor transactions in which the card number or track data are encrypted and/or tokenized.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="terminalID" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Terminal identifier</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="isStoredPaymentMethodIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>To indicate if the payment method is stored in the system.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="fundingGroupId" type="csom:FundingGroupId"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Identifer of funding group</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="Passport">
        <xsd:sequence minOccurs="0">
          <xsd:element name="passportNumber" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    The person's passport number
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="typeCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    The type code of the Person Passport
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="OrderLineBase" abstract="true">
        <xsd:sequence minOccurs="0">
          <xsd:element name="quantity" type="csom:Quantity" default="1" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The quantity of the product being ordered on this line. Defaults to 1.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A description of this line of the order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="parentOrderLineID" type="csom:OrderLineId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the parent order line that this order line is a component of. Parent line may represent a bundle or kit. In the case of services, the parent line may represent the bundled offering that includes this feature.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="rootParentOrderLineID" type="csom:OrderLineId"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the configuration top parent line. In the case of an activation, the root parent line identifies the requested mobile number or other network service access point.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="relatedOrderLineID" type="csom:OrderLineId" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Identifies the order line that this order line is related to. May be used to link a financial product to the related device, if not part of a bundled offering.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="componentOrderLineID" type="csom:OrderLineId" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Identifies the order lines components that make up the Order. The components are order lines themselves.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="promotion" type="csom:Promotion" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Represents the promotion under which the order item is being purchased.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="orderLineStatus" type="csom:OrderLineStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The current status of this line of the order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="transactionType" type="csom:OrderLineTransactionType"
             minOccurs="0"/>
          <xsd:element name="transactionSubType" type="csom:OrderLineTransactionSubType"
             minOccurs="0"/>
          <xsd:element name="affectsEligibility" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>This flag is set by EPC when the offer eligibility is evaluated. This warns the UI that if this item is removed from the order, the offer may no longer be valid.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="inventoryStatus" type="csom:InventoryStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Status of the selected product in inventory, as reported to the customer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="bypassInventoryCheck" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>UI checked out order to OMS via ESP now OMS is doing inventory check get response from SAP that inventory is not available/back order and UI end they will do what ever necessary step required (removing that item from cart/ update the quantity/ get concent from customer to submit order with backorder SKU/item) then try to confirm the Order again to OMS..this time UI will pass flag either true/false to update OMS (based on this flag OMS will skip the inventory check).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="originalOrderLineId" type="csom:OrderLineId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The original order line id in the case of a return or exchange.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="deviceDiagnostics" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate that a device diagnostics was performed on the device being returned.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="reasonCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Order reason code</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="reasonDescription" type="csom:Description" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Description of reason code</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="returnAuthorizationType" type="csom:ReturnAuthorizationType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Return authorization type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="fundingGroupId" type="csom:FundingGroupId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifier of funding group</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="parentURI" type="csom:ReferenceURI"/>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="ServiceProductFeeGroup">
        <xsd:sequence minOccurs="0">
          <xsd:element name="name" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the fee group.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The description of the fee group.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="code" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The code identifying the fee group for billing.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="feeGroupId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the fee group.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="ProductOfferingReference">
        <xsd:sequence minOccurs="0">
          <xsd:element name="name" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The commonly used name of the Proudct Offering as defined in the Product Catalog.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="shortName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The brief name of the Proudct Offering as defined in the Product Catalog.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="displayName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the Proudct Offering to be used for presentation to the Customer, as defined in the Product Catalog.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:DescriptionByChannel" minOccurs="0"
             maxOccurs="unbounded"/>
          <xsd:element name="shortDescription" type="csom:DescriptionByChannel"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="longDescription" type="csom:ComplexDescription"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The long description of the product offering to be used when space allows.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="keyword" type="base:SpecificationValue" minOccurs="0"
             maxOccurs="unbounded"/>
          <xsd:element name="offerType" type="csom:OfferType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Primary categorization of Product Offering.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="offerSubType" type="csom:OfferSubType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Secondary categorization of Product Offering.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="offerLevel" type="csom:OfferLevel" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Level in the customer hierarchy to which the offer should be applied.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="offeringStatus" type="csom:OfferingStatus" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Deprecated. The status of the Product Offering in the product catalog.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The time period during which the offer is valid.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="offeringClassification" type="csom:Classification"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="brand" type="csom:Brand" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The business name under which a TMUS product is sold.  A Customer can have accounts in any Brand, within the same Business Unit. For example, a T-Mobile customer may also have a branded account with a national retailer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="businessUnit" type="csom:BusinessUnit" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The different T-Mobile business units require complete separation of assets. Separate companies that comprise TMUS corporation.  The system restricts a Customer to one Business Unit.  In other words, a Customer in one Business Unit is not visible in any of the other  Business Units of the corporation. For example, a Metro PCS customers billing address cannot be looked up by a Magenta representative.  There is no capability to override this in the system.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="coreOfferFamily" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Core offer family</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="extendedOfferFamily" type="xsd:normalizedString"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Extended offer family</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="productType" type="csom:ProductType" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Product type that is included in the product offering.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="productSubType" type="csom:ProductSubType" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Product subtype that is included in the product offering.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="key" type="csom:DomainKey" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Alternate identifier for the offer. For example the offer id in RPX or Samson.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="csom:ActionCode"/>
        <xsd:attribute name="productOfferingId" type="csom:ProductOfferingId">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies a Product Offering in the Product Catalog.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>A URI that can be used to retrieve details about this Product Offering.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="Market">
        <xsd:annotation>
          <xsd:documentation>A market is a geographic area which is managed as a business unit, and within which the product offerings of a company are uniform and consistent. 
    Other markets may offer different products, or the same products at different prices or different times, or through different promotions.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="marketName" type="xsd:normalizedString" minOccurs="0"/>
          <xsd:element name="marketCode" type="csom:MarketCode" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:token"/>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="AssignedProductReference">
        <xsd:annotation>
          <xsd:documentation>A Product Offering that has been procured by a customer, or other interested interested party.  The Assigned Product includes information about Parties who may have an interest arising from ownership/lease, usage, maintenance management, billing etc. It is an instance of the Product Specification, for a specific customer.  The Assigned Product may include relationships to other Assigned Products such as a like component or substitute.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="key" type="csom:DomainKey" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Alternate identifiers for the assigned product.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="productOffering" type="csom:ProductOffering" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the product in the product catalog.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:AssignedProductStatus" minOccurs="0"/>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation> 
    The effective period when the assigned product is valid
                                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="customerOwnedIndicator" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Indicates whether the customer is the owner of the assigned product. In the case of a lease, the assigned product should be owned by a business unit within the deploying enterprise
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="serialNumber" type="csom:SerialNumber" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Unique serial number for identifying an assigned product - this is applicable only for assigned products based on a serializable product. Quantity will always be 1 if serial number is specified
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="productOfferingPrice" type="csom:ProductOfferingPrice"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="deviceBlock" type="csom:DeviceBlock" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Device block details</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="serialNumberType" type="csom:SerialNumberType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Type of serial number</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="csom:AssignedProductActionCode">
          <xsd:annotation>
            <xsd:documentation>Specifies the action to be taken with the AssignedProduct object as a result of an operation with the action code as an input.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="assignedProductId" type="csom:AssignedProductId"/>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="OrderLineExtension">
        <xsd:annotation>
          <xsd:documentation>This extension of the Order Line object includes accompanying Port request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:OrderLine">
            <xsd:sequence minOccurs="0">
              <xsd:element ref="csom:port" minOccurs="0"/>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ProductOfferingPrice">
        <xsd:annotation>
          <xsd:documentation>A reusable price component: charge, fee, or discount.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="name" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the Product Offering Price.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0"/>
          <xsd:element name="productChargeType" type="csom:ProductChargeType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Charge types that are used for product offerings.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="productChargeIncurredType" type="csom:ProductChargeIncurredType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Specifies the timing for when the first charge is incurred for the product: immediate, on usage, or on demand.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice minOccurs="0">
            <xsd:element name="discountAmount" type="csom:Amount" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>The Amount by which the price is discounted</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="discountPercent" type="xsd:float" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>The percentage by which the price is discounted</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="basisAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The amount on which the price is calculated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="amount" type="csom:Amount" minOccurs="0"/>
          <xsd:element name="quantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>If the price is not paid with money, but with points or some other non-monetary means of exchange, the quantity is captured here.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="amountPerPeriod" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>DEPRECATED</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="periodLengthEnd" type="xsd:positiveInteger" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The end of the range during which this price applies. Sometimes a price changes over the life of the product, so different price components are required for each time slice.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="periodLengthBegin" type="xsd:positiveInteger" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The beginning of the range during which this price applies. Sometimes a price changes over the life of the product, so different price components are required for each time slice.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="periodLengthMeasurementUnit" type="csom:PeriodLengthMeasurementUnit"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The time measurement unit used for the time range indicated in periodLengthBegin and periodLengthEnd.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="chargeCode" type="csom:BillingChargeCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The billing system charge code that corresponds to this price component when it is added to the customer's bill.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="minimumNumberOfPeriods" type="xsd:decimal" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The minimum number of time periods that the </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="amountPerUse" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>DEPRECATED. Use amount.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="maximumNumberOfPeriods" type="xsd:decimal" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The maximum number of periods in which this price component can be applied. Can be used to control duration of limited time discount.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="usageMeasurementUnit" type="csom:UsageMeasurementUnit"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The unit to be used for measuring usage of this consumable product, usually a service.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="minimumUsageOccurrences" type="xsd:nonNegativeInteger"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The minimum number of times the product must be used before this price component applies.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="maximumUsageOccurrences" type="xsd:nonNegativeInteger"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The maximum number of times the product may be used while this price component applies.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="feeCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The product catalog's short-hand name for the fee.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="feeGroup" type="csom:ServiceProductFeeGroup" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>If the price component is a fee, the fee group to which the fee belongs.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="feeType" type="csom:ServiceProductFeeType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The type of fee.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="billingCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>DEPRECATED The sku, soc or other identifier for the system where the charge for the fee is mastered.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="oneTimeCharge" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Flags a charge as being applied once, when the product is first acquired.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="recurringFeeFrequency" type="csom:BillingFrequencyCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies whether the charge is recurring, regular, or an installment payment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="recurringFeeChargeInstances" type="xsd:int" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Specifies the number of times (biiling cycles) the customer should be charged the fee.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="taxInclusiveIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether the price reflects the total after tax, as for Metro PCS pricing.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="specificationValue" type="base:SpecificationValue"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A named value pair containing additional information about the price component.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="promotion" type="csom:PromotionReference" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Promotion associated with the product offering price</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="displaySummarizationIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To enable UI to identify if the price &amp; discount need to be displayed individually or not</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="alternateDescription" type="csom:ComplexDescription"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Alternative description for product offering price component</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="capacityType" type="csom:UsageType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Type of capacity, or usage type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="capacitySubType" type="csom:UsageSubType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Capacity sub type, or usage sub type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="productOfferingPriceId" type="csom:ProductOfferingPriceId">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies a product offering price, which is a price component within a price list line.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>A URI that can be used to reference information about the Product Offering Price.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="OrganizationReference">
        <xsd:annotation>
          <xsd:documentation>Identifying details for a generic organization.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="fullName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name of the organization</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="legalName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Formal name of the organization for legal purposes, such as contracts or taxes.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="fein" type="csom:FEIN" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Federal Employer Identification Number</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="sicCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Version of Standard Industry Classification (SIC) code    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="organizationCode" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Publicly used code that uniquely identifies the organization in business transactions.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="organizationId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Enterprise-wide unique identifier of the Organization.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>The URI that can be used to return information about this Organization.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="Tenure">
        <xsd:annotation>
          <xsd:documentation>The number of units of time that something has lasted.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:integer">
            <xsd:attribute name="measurementUnit" type="csom:MeasurementUnit"
               default="months">
              <xsd:annotation>
                <xsd:documentation>Measurement unit for the unit of time that tenure is being measured in.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="OrderLineStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="NEW"/>
                  <xsd:enumeration value="INPROCESS"/>
                  <xsd:enumeration value="SHIPPED"/>
                  <xsd:enumeration value="BACKORDER"/>
                  <xsd:enumeration value="INSTALLED"/>
                  <xsd:enumeration value="PARTIAL"/>
                  <xsd:enumeration value="PREORDER"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="BillTo">
        <xsd:sequence minOccurs="0">
          <xsd:element name="customerAccount" type="csom:OrderAccount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Information about the customer's business relationship with the company.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="customer" type="csom:OrderCustomer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Information about the party that placed the order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="NationalIdentityDocument">
        <xsd:sequence minOccurs="0">
          <xsd:element name="nationalIdentityDocumentIdentifier" type="xsd:token"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The unique identifier for the national identity document.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="issuingCountryCode" type="csom:CountryCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The country that issued the person's National ID</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="typeCode" type="csom:IdentityDocumentType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>key for the type of the person's national ID
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="issuingAuthority" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Authority that issued the identification document as stated on the document, i.e. tribal council, county for voter registration, or state for state identification.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="csom:ActionCode"/>
      </xsd:complexType>
      <xsd:complexType name="Order">
        <xsd:annotation>
          <xsd:documentation>An order placed by the customer for goods and/or services. May imply the creation of a new customer account. Potentially includes activation of new or additional lines, handset upgrade, change rate plan, change SIM, change MSISDN, port ins, etc.
    </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:OrderBase">
            <xsd:sequence minOccurs="0">
              <xsd:element name="billTo" type="csom:BillTo" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Name and address of the party to be billed for the order.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="shipping" type="csom:Shipping" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Details of the shipping arrangements for the order.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="businessUnitName" type="csom:BusinessUnit"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The business unit within the context of which the Customer Order is defined.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element ref="csom:orderLine" minOccurs="0" maxOccurs="unbounded"/>
              <xsd:element name="orderCharge" type="csom:Charge" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Charges and fees applied at the order level, not based on a specific line item.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderDeduction" type="csom:Deduction" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Discounts and rebates applied at the order level, not based on a specific line item.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderTax" type="csom:Tax" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Taxes applied at the order level, not based on a specific line item.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderPayment" type="csom:OrderPayment" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Payment information associated with the order. For complex orders payment information may be located on individual order schedules, reflecting partial shipments.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderAttachment" type="csom:OrderAttachment"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>References to additional documentation for the order.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="lastUpdateDate" type="xsd:dateTime" minOccurs="0"/>
              <xsd:element name="orderStatus" type="csom:OrderStatus" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Details about the status of the Customer Order.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="cartId" type="csom:CartId" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Unique identifier for the Cart from which this Order was built.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="addressList" type="csom:AddressList" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>List of addresses used in this Order.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="originalOrder" type="csom:OriginalOrder" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Original order</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ChargeDistribution">
        <xsd:annotation>
          <xsd:documentation>Describes how the charges for a product are to be distributed.  Different types of charges can be distributed to different accounts.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="payChannelId" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The PayChannel Id associated to this distribution</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="chargeType" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The type of charge, For Example RC, OC, UC, ALL</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:ChargeDistributionStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The current state of the account.  For example active, inactive.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Indicates the type of change that has been made to the charge distribution, as compared to the previous version of the charge distribution.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="chargeDistributionId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Unique identifier of the charge distribution.  Assigned by the system of record.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="Employment">
        <xsd:sequence minOccurs="0">
          <xsd:element name="employerOrganization" type="csom:Contact" minOccurs="0"/>
          <xsd:element name="employeeNumber" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Unique identifier for the employee within their employer's organization.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="positionTitle" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The title of the position held by the person during this period of employment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="AddressExtension">
        <xsd:complexContent>
          <xsd:extension base="csom:Address">
            <xsd:sequence>
              <xsd:element name="building" type="xsd:normalizedString" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>A building name or number. An office park or campus may have several buildings that are distinguished by the building name or number.
    					</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="floor" type="xsd:normalizedString" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>A floor in a building
    					</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="area" type="xsd:normalizedString" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Refers to an area of the building such as left wing, right wing etc.  
    					</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="ruralRoute" type="xsd:normalizedString" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Rural Route Number for the address, if applicable</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="countrySubDivisionID" type="xsd:normalizedString"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>An identification of a country sub-division of this address such as a department in France. To be used only if there is no named term that represents the countrysubdivision concept being expressed</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="citySubDivisionName" type="xsd:normalizedString"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>A name, expressed as text, of a sub-division of a city for this address, for example a district or borough.  To be used only if there is no named term that represents the citysubdivision concept being expressed</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="timeZone" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Value indicating the time zone of the address, e.g. Eastern, Pacific</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="houseNumber" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The number identifying a specific locaton on a street, usually associated with a building or complex, as parsed out the address line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="streetName" type="xsd:normalizedString" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The name of the street or road, as parsed from the address line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="streetSuffix" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The suffix for the street name, as parsed from the address line. Example values are Ave, St, Rd, Blvd, Pl, Ct.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="trailingDirection" type="csom:Direction" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The direction (N, S, E, W, NE, NW, SE, SW) that follows the street name, as parsed from the address line, if present.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="unitType" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The type of unit parsed from the address line, if present. Examples are Apt or Ste.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="unitNumber" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The apartment or suite number as parsed from the address line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="streetDirection" type="csom:Direction" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The direction (N, S, E, W, NE, NW, SE, SW) that precedes the street name, as parsed from the address line, if present.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="houseNumberSuffix" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The suffix for the house number when an alpha suffix is used, as parsed from the address line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="urbanization" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>An urbanization denotes an area, sector, or residential development within a geographic area. Commonly used in Puerto Rican urban areas, it is an important part of the addressing format, as it describes the location of a specific street. In Puerto Rico, identical street names and address number ranges can be found within the same ZIP Code. In these cases, the urbanization name is the only element that correctly identifies the location of a particular address. Generally, the abbreviation URB is placed before the urbanization name. </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="deliveryPointCode" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>A three digit field used in CASS (Coding Accuracy Support System) along with ZIP, ZIP+4 and CRRT Code to automate the postal sortation process and maximize postage discounts. First two digits identify a unique delivery point. Third digit is a check digit.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                  <xsd:restriction base="xsd:token">
                    <xsd:pattern value="[0-9]{3}"/>
                    <xsd:minLength value="2"/>
                    <xsd:maxLength value="3"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="confidenceLevel" type="csom:Ranking" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Level of confidence in the address as supplied by the address validation routine.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="carrierRoute" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The postal carrier route code identifies a group of addresses within a single ZIP code to aid in sorting mail for delivery. The code consists of a letter followed by 3 digits.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                  <xsd:restriction base="xsd:token">
                    <xsd:length value="4"/>
                    <xsd:pattern value="[A-Z]{1}[0-9]{3}"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="overrideIndicator" type="csom:Indicator" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates that the address supplied by the customer has been overridden. Implies that the original address was deemed to be invalid, but a corrected address was derived from it.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="postOfficeBox" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The post office box number, as parsed from the address line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="observesDaylightSavingsIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates whether daylight savings time is observed at the location of the address.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="matchIndicator" type="csom:Indicator" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Value added by address validation software to indicate the address match (Exact, Partial, Alternative, Default).</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="PaymentSchedule">
        <xsd:sequence minOccurs="0">
          <xsd:element name="totalScheduledAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The total monetary amount being paid through this Payment Arrangement.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="totalPlannedPaymentCount" type="csom:Quantity"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total number of planned payments defined under this payment arrangement.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="plannedPayment" type="csom:PlannedPayment" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A Planned Payment defines a payment installment promised by the customer as part of a Payment Arrangement.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="interestRate" type="csom:Rate" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Interest rate being charged for this extended payment schedule.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="BundledProductOffering">
        <xsd:annotation>
          <xsd:documentation>DEPRECATED</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:ProductOfferingBase">
            <xsd:sequence minOccurs="0">
              <xsd:element name="simpleOfferingComponent" type="csom:SimpleOfferingComponent"
                 minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="PaymentInstrument">
        <xsd:annotation>
          <xsd:documentation>A payment instrument is the financial vehicle used to make a payment.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:choice minOccurs="0">
            <xsd:element name="check" type="csom:Check" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>A check is a negotiable instrument drawn against deposited funds, to pay a specific entity a specific amount of funds on demand.  The payee's bank issues funds to the payee, and then by the payee's bank to the payer's bank. The payer's bank then shifts funds from the payer's account to the payee's bank, thereby settling all accounts.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="electronicFundsTransfer" type="csom:ElectronicFundsTransfer"
               minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Electronic Funds Transfer (EFT) is a system of transferring money from one bank account directly to another without any paper money changing hands.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="bankDraft" type="csom:BankDraft" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>A bank draft is a bill of exchange drawn by a bank on itself, or on a correspondent bank in another city or country. Bank drafts are commonly used by banks in dealings with other banks, or when a creditor or seller is unwilling to accept an ordinary check from a debtor or buyer in another city or country.  Because a draft is certain to be paid, it is generally accepted as a cash equivalent.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="creditCard" type="csom:CreditCard" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>holders draw on a credit limit approved by the card-issuer such as a bank, store, or service provider. Cardholders normally must pay for credit card purchases at a later date as determined by their agreement with the card-issuer.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="debitCard" type="csom:DebitCard" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>A debit card provides the cardholder electronic access to his or her bank account(s) at a financial institution.  Some cards may bear a stored value with which a payment is made, while most relay a message to the cardholder's bank to withdraw funds from a payer's designated bank account.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="prepaidCard" type="csom:PrepaidCard" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>A Prepaid Card is a type of payment card that accesses a limited funding source. Commonly used to provide banking services to those without access to a bank account or credit card.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="cashIndicator" type="xsd:boolean" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Indicates that the payment instrument used is cash.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="paymentCard" type="csom:PaymentCard" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>The Payment Card provides the core elements that are shared in common between Credit Card, Debit Card, and Prepaid Card. It can be used when it is not necessary to differentiate between those three.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="financialInstrument" type="csom:FinancialInstrument"
               minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Financial Instrument provides the core elements used in common between Check, Bank Draft, and Electronic Funds Transfer. It can be used in situations where it is not necessary to differentiate between those three.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="issuedDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date when the payment instrument was issued.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="storedPaymentToken" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Token used as an alias to retrieve stored payment method from T-Safe or other secured system.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="defaultPaymentMethodIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To identify if the current payment method will be used as default payment method</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Code specifying the action to be taken with this Payment Instrument in a given transaction, if any.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="paymentInstrumentId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the system's record of this payment instrument.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>The URI that can be used to access information from this payment instrument.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="ExtendedAddressCommunication">
        <xsd:complexContent>
          <xsd:extension base="csom:CommunicationBase">
            <xsd:sequence>
              <xsd:element name="address" type="csom:AddressExtension" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The mailing (postal) or physical address.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="usageContext" type="csom:AddressUsageContext"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The Address Usage Context specifies a way in which the address should be used.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="communicationStatus" type="csom:AddressCommunicationStatus"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The status of the address for communication.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="specialInstruction" type="csom:Description"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Instruction to identify additional address information.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="Manufacturer">
        <xsd:annotation>
          <xsd:documentation>The basic information about the manufacturer of a product.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="manufacturerName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The manufacturer name that is to be used for display to a user.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="manufacturerOrganization" type="csom:Organization"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Organization that is a manufacturer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="manufacturerAddress" type="csom:AddressCommunication"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Address of the manufacturer.  Populated only when the Organization has a different address as a manufacturer than the Organization itself.  For example Acme Phones, Inc. might be a customer of T-Mobile, but also manufactures phones that T-Mobile sells.  The address that is relevant to the manufacturing arm of Acme Phones is likely to be different than customer address.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="manufacturerWebsite" type="csom:WebsiteCommunication"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Website of the manufacturer.  Populated only when the Organization has different website information as a manufacturer than the Organization itself.  For example Acme Phones, Inc. might be a customer of T-Mobile, but also manufactures phones that T-Mobile sells.  Website information that is relevant to the manufacturing arm of Acme Phones is likely to be different than customer website information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="manufacturerPhone" type="csom:PhoneCommunication"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The phone number of the manufacturer.  Populated only when the Organization has different phone number as a manufacturer than the Organization itself.  For example Acme Phones, Inc. might be a customer of T-Mobile, but also manufactures phones that T-Mobile sells.  The phone number that is relevant to the manufacturing arm of Acme Phones is likely to be different than a customer phone number.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="manufacturerEmail" type="csom:EmailCommunication"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The email address of the manufacturer.  Populated only when the Organization has a different email address as a manufacturer than the Organization itself.  For example Acme Phones, Inc. might be a customer of T-Mobile, but also manufactures phones that T-Mobile sells.  The email address that is relevant to the manufacturing arm of Acme Phones is likely to be different than the customer email address.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="manufacturerContact" type="csom:Contact" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contact information about the manufacturer organization.  Populated only when the Organization has different contact information as a manufacturer than the Organization itself.  For example Acme Phones, Inc. might be a customer of T-Mobile, but also manufactures phones that T-Mobile sells.  Contact information that is relevant to the manufacturing arm of Acme Phones is likely to be different than customer contact information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="manufacturerStatus" type="csom:ManufacturerStatus"
             minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="manufacturerId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Enterprise-wide unique identifier of the Manufacturer. </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="RMAReference">
        <xsd:sequence minOccurs="0">
          <xsd:element name="rmaNumber" type="xsd:token" minOccurs="0"/>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0"/>
          <xsd:element name="rmaStatus" type="csom:RMAStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Status of the Return Merchandise Authorization (RMA).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="QuotationLine">
        <xsd:annotation>
          <xsd:documentation>A quote line item defines a specific offer that a customer has the option of purchasing at a particular price. A quote line item will typically contain a product offering and a list price. </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:QuotationLineReference">
            <xsd:sequence>
              <xsd:element name="note" type="csom:Note" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Notes about the quotation line</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="overridePriceAllowedIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates whether price override is allowed for this quoatation line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="currencyCode" type="csom:CurrencyCode" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Currency used for this quotation line. Applies to all quotation schedules associated with this line unless specified within the schedule.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="parentQuotationLineId" type="csom:QuotationLineId"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Identifies the immediate parent quotation line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="rootParentQuotationLineId" type="csom:QuotationLineId"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Identifies the top parent quotation line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="contractReference" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>References a contract associated with the quotation line, if applicable. Usually only used for commercial accounts.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="assignedProduct" type="csom:AssignedProduct"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Identifies an instance of a product offering already owned or subscribed to by the customer that this quotation line is associated with. May also be used for device trade in details.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="originalProductOfferingId" type="csom:ProductOfferingId"
                 minOccurs="0"/>
              <xsd:element name="quotationSchedule" type="csom:QuotationSchedule"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>A shipment schedule defines specific scheduled shipment dates, shipping attributes, source of supply, and other shipping terms for a single quote line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The specification group allows for extended attributes defined for a particular quotation line. It can be used to capture the configurated definition of the particular product being quoted.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element ref="csom:lineOfService" minOccurs="0"/>
              <xsd:element name="productOffering" type="csom:ProductOffering"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Identifies the product offering that is being quoted on the quotation line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="rmaReference" type="csom:RMAReference" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Reference to the Return Merchandise Authorization (RMA) associated with the quotation line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="quantity" type="csom:Quantity" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Quantity of the product offering quoted in the quotation line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="promotion" type="csom:Promotion" minOccurs="0"
                 maxOccurs="unbounded"/>
              <xsd:element name="transactionType" type="csom:OrderLineTransactionType"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>This field categorizes the type of order transaction being performed on the quotation line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="claimReference" type="csom:ClaimReference"
                 minOccurs="0"/>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="OrderLineOfService">
        <xsd:complexContent>
          <xsd:extension base="csom:LineOfService">
            <xsd:sequence minOccurs="0">
              <xsd:element name="handsetTenure" type="csom:Tenure" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Duration of the life of the device on the line of service.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="ratePlanTenure" type="csom:Tenure" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Duration of the life of the rate plan on the line of service.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="totalRecurringDueAmount" type="csom:Amount"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Total recurring due amount</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="MobileNumber">
        <xsd:sequence minOccurs="0">
          <xsd:element name="msisdn" type="csom:MSISDN" minOccurs="0"/>
          <xsd:element name="numberGroup" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Toll area containing one or more area codes.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="portIndicator" type="csom:Indicator" minOccurs="0"/>
          <xsd:element name="releaseDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date to reserve/release a mobile number.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="portReason" type="xsd:token" minOccurs="0"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="Customer">
        <xsd:annotation>
          <xsd:documentation>Details concerning the person or organization that is a customer.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:CustomerReference">
            <xsd:sequence>
              <xsd:element name="customerAccount" type="csom:CustomerAccount"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Account held by this customer.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="customerTaxExemption" type="csom:TaxExemption"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Tax Exemptions that this Customer Party is eligible for.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="bankruptcyOpenIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Set when bankruptcy is filed by customer.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="bankruptcyDischargeIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Set when bankruptcy is accepted by the Judge. This information is viewable from the UI.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="OrderLine">
        <xsd:annotation>
          <xsd:documentation>Order lines provide the details of the product offerings being ordered. An order line may be for a product assembly or a component. 
    Additional order lines for components may be associated with the order line for the assembly.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:OrderLineBase">
            <xsd:sequence minOccurs="0">
              <xsd:element name="productOffering" type="csom:ProductOffering"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The offering being purchased in the sales order.  The offering can be bundled or simple. A bundled offering may combine multiple simple offerings into a single package.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="assignedProduct" type="csom:AssignedProduct"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The assigned product that is being created, modified or deleted by this order line. May also be used for device trade in details.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderSchedule" type="csom:OrderSchedule" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Destination, pricing and quantity details for the order line. Multiple schedules may be required if the order line is divided into multiple shipments.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderLineAttachment" type="csom:OrderAttachment"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Binary, URI or other attachments that apply to the Order Line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="networkResource" type="csom:NetworkResource"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The resource that is being created, modified, or deleted by this order line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="lineOfService" type="csom:OrderLineOfService"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The line of service affected by this order line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="lastUpdateDate" type="xsd:dateTime" minOccurs="0"/>
              <xsd:element name="rmaReference" type="csom:RMAReference" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Reference to the Return Merchandise Authorization (RMA) associated with the order line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="claimReference" type="csom:ClaimReference"
                 minOccurs="0"/>
              <xsd:element name="deviceConditionQuestions" type="csom:VerificationQuestionSet"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Questions and answers about the condition of a device being traded in, exchanged, returned, or replaced.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="revisionReason" type="xsd:normalizedString"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Reason for remorse period override for a exchange or return order.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="pointOfNoReturnReached" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicate if the order line is reached the point of no return.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="priceChangedIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates that a price has changed for the order line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="financialAccount" type="csom:FinancialAccountReference"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The customer finance account associated with the order line</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderType" type="csom:OrderType" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Order type for the order line</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="extendedAmount" type="csom:Amount" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Charge amount without tax or fee</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="totalChargeAmount" type="csom:Amount" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Sum of charges in item (excluding fees)</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="totalDiscountAmount" type="csom:Amount" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>sum of all discounts in item</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="totalFeeAmount" type="csom:Amount" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>sum of all fees in item</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="totalTaxAmount" type="csom:Amount" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>sum of all sales tax in item</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="totalDueNowAmount" type="csom:Amount" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Total amount customer has to pay now for the item</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="totalDueMonthlyAmount" type="csom:Amount" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Total due monthly for the item</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderSubType" type="csom:OrderSubType" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Order sub type</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="actionCode" type="csom:OrderLineActionCode">
              <xsd:annotation>
                <xsd:documentation>The action to be taken with the order line data.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="orderLineId" type="csom:OrderLineId">
              <xsd:annotation>
                <xsd:documentation>Uniquely identifies this order line within the order. Useful for specifying relationships between order lines.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="PhoneCommunication">
        <xsd:complexContent>
          <xsd:extension base="csom:CommunicationBase">
            <xsd:sequence minOccurs="0">
              <xsd:element name="phoneType" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates the type of Phone Communication. </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                  <xsd:restriction base="xsd:token">
                    <xsd:enumeration value="LANDLINE"/>
                    <xsd:enumeration value="MOBILE"/>
                    <xsd:enumeration value="FAX"/>
                    <xsd:enumeration value="SATELLITE"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element ref="csom:phoneNumber" minOccurs="0"/>
              <xsd:element name="areaCode" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The area dialing code for a communication number
    							</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="usageContext" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Identifies the circumstances for which we are to use this phone number for communication.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                  <xsd:restriction base="xsd:token">
                    <xsd:enumeration value="PRIMARY"/>
                    <xsd:enumeration value="SECONDARY"/>
                    <xsd:enumeration value="BILLING"/>
                    <xsd:enumeration value="HOME"/>
                    <xsd:enumeration value="WORK"/>
                    <xsd:enumeration value="DATANOTIFY"/>
                    <xsd:enumeration value="OTHER"/>
                    <xsd:enumeration value="SECURITY"/>
                    <xsd:enumeration value="SKIPTRACE"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="Check">
        <xsd:annotation>
          <xsd:documentation>A check is a negotiable instrument drawn against deposited funds, to pay a specific entity a specific amount of funds on demand.  The payee's bank issues funds to the payee, and then by the payee's bank to the payer's bank. The payer's bank then shifts funds from the payer's account to the payee's bank, thereby settling all accounts.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:FinancialInstrument">
            <xsd:sequence minOccurs="0">
              <xsd:element name="IBAN" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>International bank account number. Used internationally to uniquely identify the account of a customer at a financial institution</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="checkNumber" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The number uniquely identifying the check within the the set of checks written against the customer's bank account.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="Description">
        <xsd:annotation>
          <xsd:documentation>A structure that provides a description for a certain usage and language. For example, a description in Spanish for web display.
    			</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="csom:Text"/>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="Citizenship">
        <xsd:sequence minOccurs="0">
          <xsd:element name="countryCode" type="csom:CountryCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    The country of the person's citizenship
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="PayChannel">
        <xsd:annotation>
          <xsd:documentation>A Pay Channel is a conduit for charges that are to be collected and applied to a single payment instrument.  A pay channel belongs to one billing arrangement.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="preferredIndicator" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates whether this is the Primary Payment profile for this Customer</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paymentMethodCode" type="csom:PaymentMethodCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Specifies the Payment Type. Example values can be: Card, Cash, Direct Debit, Draft, Letter of Credit, GIRO, Check ( Send me a bill)</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="currencyCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Currency code for this Payment Profile</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="clearingPeriod" type="xsd:duration" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Specifies the number of  days before receipts that belong to this customer (or site) can be cleared by the Automatic Clearing program. This overrides the value of the payment method or bank account.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="autoPayEnabledIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates that this Pay Channel is enabled to support automatic payments according to the defined clearing period.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="autoPayMaximumWithdrawalAmount" type="csom:Amount"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Captures maximum amount to be processed by automatic payment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:TimePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Effective Period of this Payment Profile</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:PayChannelStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Status as of Effective Date</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="note" type="csom:Note" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Note</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="paymentInstrument" type="csom:PaymentInstrument"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Captures the payment detail information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="contact" type="csom:Contact" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Customer Party Payment Profile contact information</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="automaticPaymentType" type="csom:AutomaticPaymentType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Categorizes the type of automatic payment configured by this Pay Channel.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="autoRefillAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The amount configured by the customer for the automatic refill payment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="additionalPaymentAmount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An additional amount to be added to the automatic payment. This is in addition to the autoRefillAmount, or the MRC amount for AutoPay or EasyPay.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="additionalPaymentDescription" type="csom:Description"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Describes the additional amount being added to the automatic payment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="autoRefillFrequency" type="csom:CalendarFrequency"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Describes the frequency that the automatic refill should be applied, as requested by the customer. For AutoPay and EasyPay, the frequency is implied by the Billing Method and Bill Cycle.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="firstPaymentDay" type="xsd:integer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The day of the month when the first automatic refill should occur, for a month-based frequency. For AutoPay and EasyPay the day is implied by the assigned Bill Cycle.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="secondPaymentDay" type="xsd:integer" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The day of the month when the second automatic refill should occur, for a semi-monthly frequency. </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="refillBalanceThresholdAmount" type="csom:Amount"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies a balance amount that acts as a threshold for triggering an automatic refill.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="refillDayOfWeek" type="csom:DayOfWeek" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The day of the week when a weekly or bi-weekly automatic refill event should occur.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="setupOrderId" type="csom:OrderId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Uniquely identifies the Order that setup up this automatic payment rule.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="setupOrderTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date and time of the Order setting up the automatic payment rule.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="csom:PayChannelActionCode">
          <xsd:annotation>
            <xsd:documentation>Indicates the type of transaction that is being applied to the pay channel, relative to the previous version of the pay channel.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="parentURI" type="csom:ReferenceURI"/>
        <xsd:attribute name="payChannelId" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies a Pay Channel.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="Quantity">
        <xsd:simpleContent>
          <xsd:extension base="xsd:decimal">
            <xsd:attribute name="measurementUnit" type="csom:MeasurementUnit">
              <xsd:annotation>
                <xsd:documentation>Unit of quantity</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="Contact">
        <xsd:annotation>
          <xsd:documentation>A resource serving as a go-between, messenger, connection, or source of special information.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="personName" type="csom:PersonName" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Name of the person to be contacted.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="preference" type="csom:Preference" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Precedence, advantage, or choice of one contact over another</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="addressCommunication" type="csom:AddressCommunication"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="phoneCommunication" type="csom:PhoneCommunication"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="emailCommunication" type="csom:EmailCommunication"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="websiteCommunication" type="csom:WebsiteCommunication"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="preferredLanguage" type="csom:PreferredLanguage"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The preferred language of communication in the specified usage context. ISO 639-1 Code (en-US,en-CA,en-GB)</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="preferredCommunicationChannel" type="csom:PreferredCommunicationChannel"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Precedence, advantage, or choice of one communication channel, for example  email vs phone</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="socialCommunication" type="csom:SocialCommunication"
             minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the Contact.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>The URI that can be used to return information about this Contact.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="Bank">
        <xsd:annotation>
          <xsd:documentation>The financial institution.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="bankName" type="xsd:normalizedString" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Name of the bank</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="routingNumber" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Bank routing number or routing transit number (RTN) is a nine digit number used to identify a financial institution in a transaction. Most common types of transaction being ACH and wire transfers. Some banks and credit unions may have multiple routing numbers serving different purposes, geographical regions or branches. Routing numbers are printed on the bottom of a check. They can also be found on online banking sites of the financial institution.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="Location">
        <xsd:annotation>
          <xsd:documentation>The location of an entity such as a store or warehouse, including address and phone numbers.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="locationName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the entity located at the given address.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="locationType" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The type of entity at this location.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="address" type="csom:Address" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The physical address of the entity.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="phoneCommunication" type="csom:PhoneCommunication"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Phone numbers associated with the location, possibly including fax numbers as well as voice lines.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>The unique  identifier of the location.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>The URI that can be used to return information about this Location.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="RelatedOrder">
        <xsd:sequence minOccurs="0">
          <xsd:element name="orderId" type="csom:OrderId" minOccurs="0"/>
          <xsd:element name="relationshipType" type="csom:OrderRelationshipType"
             minOccurs="0"/>
          <xsd:element name="orderStatus" type="csom:OrderStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Status of related order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="BalanceSummary">
        <xsd:sequence minOccurs="0">
          <xsd:element name="balanceType" type="csom:BalanceType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The type of balance information given. For example, Initial, Transferred or Remaining.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0"/>
          <xsd:element name="dueDate" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Captured the date associated with the subscription balance details (ie. payment due date).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="expirationDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date on which the given balance expires, as in a use it or lose it scenario.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice minOccurs="0">
            <xsd:element name="amount" type="csom:Amount" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Captures the amount associated with the subscription balance type.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="quantity" type="csom:Quantity" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Captures the quantity associated with the subscription balance type, such as messages or MB.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="Name">
        <xsd:annotation>
          <xsd:documentation>A structure that provides name for a certain usage and language. For example, a name that is used for display vs an internal name.
    			</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="csom:Text"/>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="SimpleOfferingComponent">
        <xsd:annotation>
          <xsd:documentation>DEPRECATED</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:OfferingComponentBase">
            <xsd:sequence minOccurs="0">
              <xsd:element name="simpleOffering" type="csom:SimpleProductOfferingReference"
                 minOccurs="0"/>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="AddressCommunication">
        <xsd:complexContent>
          <xsd:extension base="csom:CommunicationBase">
            <xsd:sequence minOccurs="0">
              <xsd:element name="address" type="csom:Address" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The mailing (postal) or physical address.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="usageContext" type="csom:AddressUsageContext"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The Address Usage Context specifies a way in which the address should be used.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="communicationStatus" type="csom:AddressCommunicationStatus"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The status of the address for communication.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="specialInstruction" type="csom:Description"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Instruction to identify additional address information.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ProductSpecificationBase">
        <xsd:complexContent>
          <xsd:extension base="csom:ProductSpecificationReference">
            <xsd:sequence minOccurs="0">
              <xsd:element name="msrp" type="csom:UnitPrice" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Manufacturer's suggested retail price for the specified product.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="productRelationship" type="csom:ProductSpecificationRelationship"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Describes the relationship between this product and another specified product.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="additionalSpecification" type="csom:SpecificationGroup"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>A group of named value pairs to provide additional information about the product specification.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="resourceFacingSpecification" type="csom:SpecificationGroup"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Describes the details of how the configuration of a service can be changed, with respect to service capability, usage record specifications, normalized usage service parameters. Provides the mapping from specific network protocol terminology to a standard terminology.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="productClassification" type="csom:Classification"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Classifies the product.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="rankedSpecificationGroup" type="csom:RankedSpecificationGroup"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>A ranked specification group captures name value pairs, but adds the ability to rank both the groups and the values in the list.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="marketingSpecificationGroup" type="csom:RankedSpecificationGroup"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Holds the list of Specifications provided for marketing and display purposes for this product. Contents should be displayed in the order specified by the ranking.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="CommunicationBase" abstract="true">
        <xsd:annotation>
          <xsd:documentation>An abstract object that is extended to create a different types of communication ifnormation about customers and other parties that T-Mobile does business with.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="preference" type="csom:Preference" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Precedence, advantage, or choice of one communication over another</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="privacyProfile" type="csom:PrivacyProfile" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Privacy controls for this communication, indicating the circumstances in which this communication should be
    used.
    					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="uiAddedIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>To indicate that the entity is added from user interface.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="csom:ActionCode">
          <xsd:annotation>
            <xsd:documentation>Specifies the action to be taken with the communication channel</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the communication pathway described by this structure.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="PriceListLine">
        <xsd:annotation>
          <xsd:documentation>A price list line contains a valid price point for a given product</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="name" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A name for the Price List Line.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded"/>
          <xsd:element name="productOfferingPrice" type="csom:ProductOfferingPrice"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A price component included in the price list line. A product may have price components that are one time, recurring, or specail fees. In addition, discount components may be part of the price list line.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="msrp" type="csom:UnitPrice" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Manufacturer suggested retail price. Only applicable to equipment (devices and accessories).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="priceListLineEligibility" type="csom:PriceListLineEligibility"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>In order to be eligible for this price, ALL of the criteria in ONE of the price list line elibigibilities must be satisfied.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="eligibleOfferVariant" type="csom:EligibleOfferVariant"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The variant(s) to which this price list line can be applied.  Applicable only to hard goods offers.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="priceAssumesAdditionalOfferIndicator" type="csom:Indicator"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>If true this price represents the price when this offer is placed on the order with an addional offer.  For example, if this is a phone offer, a financial offer such as EIP must be placed with the order to achieve this price.  The additional offer is referenced in the 'additionalOffer' element.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="additionalOfferId" type="csom:ProductOfferingId"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Another offer that must be ordered in conjunction with this one, in order to achieve this price.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="offeringPriceType" type="csom:OfferingPriceType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Product offering price type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="alternateDescription" type="csom:ComplexDescription"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Alternative description for a price list line (price point).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="parentURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>A URI to reference the parent, which in the case of Price List Line is the Price List.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="priceListLineId" type="csom:PriceListLineId">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies a Price List Line.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI">
          <xsd:annotation>
            <xsd:documentation>A URI that can be used to reference information about the Price List Line.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="ProductOfferingBase">
        <xsd:annotation>
          <xsd:documentation>The common parts of a ProductOffering, irrespective of the type of ProductOffering.   This is meant to be an abstract type that is extended for specific types of product offerings.
    </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:ProductOfferingReference">
            <xsd:sequence minOccurs="0">
              <xsd:element name="relatedOffering" type="csom:ProductOfferingRelationship"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Related offerings are Product Offerings that may be offered to the Customer to go along with the current offering, or that may wish to consider instead the current offering. Relationship type and reason can be used to control how to present the related offerings.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
                 minOccurs="0" maxOccurs="unbounded"/>
              <xsd:element name="applicableChannel" type="csom:SalesChannelReference"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>DEPRECATED</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="offeringPrice" type="csom:PriceListLine" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>A set of price elements associated with the Product Offering, as embodied in a Price List Line.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="offeringEligibility" type="csom:EligibilityCriteria"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Rules for determining a customer's eligibility to receive this offer. In order to be able to buy this offer, ALL criteria present in ONE of these offeringEligibility must be satisfied by the customer.  For example, if there are two offeringEligibility.  The first offeringEligibilty has eligibleState of WA,MI and excludedChannel WEB.  A second offeringEligibility has eligibilePostalCode 10098 and eligibleState eligibileChannel RETAIL.  The offer is availble to customers who either: are in WA, MI and buying in any channel that is not web OR are in zip code 10098 and buying via the retail channel.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="orderBehavior" type="csom:OrderBehavior" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Parameters controlling the sale of an offer in a specifc channel. If no channel is specified, the parameters apply to all channels.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="image" type="csom:ProductOfferingImageAttachment"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>A link to an image of the product to be used in the user interface.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="marketingMessage" type="csom:MarketingMessage"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Marketing messages for the given sales channel appear in a list in the specified sequence.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="equipmentCharacteristics" type="csom:EquipmentCharacteristics"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Chracterisitics of the product offering that are only used for equipment (device or accessory).</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="serviceCharacteristics" type="csom:ServiceCharacteristics"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Chracterisitics of the product offering that are only used for network services.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="requiredOffering" type="csom:RequiredOffering"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Required offering list</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="compatibleOffering" type="csom:ScopedOfferingRelationship"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Compatible offering list</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="conflictingOffering" type="csom:ScopedOfferingRelationship"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Conflicting offering list</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="SalesCommission">
        <xsd:annotation>
          <xsd:documentation>Details associated with calculation of commission for salesperson.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="typeCode" type="xsd:token" minOccurs="0">
            <xsd:annotation/>
          </xsd:element>
          <xsd:element name="percentage" type="xsd:float" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Percentage used to calculate the commission amount</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="quantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Commission quantity which could be in monetary, points, etc.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="amount" type="csom:Amount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Commission amount, this field is deprecated in FP2.5 in order to be consistent with other related concepts such as SalesQuota which uses a quantity field to capture the commission or quota either it is in monetary amount, points, or others.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="quotaCreditQuantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Quantity of quota relief or quota credit calculated in the processing currency, points, etc.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="salesperson" type="csom:PersonReference" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identifies the salesperson being credited with this commission allocation.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="commissionsCode" type="csom:CommissionsCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The commissions code may be a dealer code or a rep id, depending on the context.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode">
          <xsd:annotation>
            <xsd:documentation>Specifies the action to be taken with the sales commission record.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:token">
              <xsd:enumeration value="ADD"/>
              <xsd:enumeration value="REMOVE"/>
              <xsd:enumeration value="UPDATE"/>
              <xsd:enumeration value="RESET"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the sales commission record.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="referenceURI" type="csom:ReferenceURI"/>
      </xsd:complexType>
      <xsd:complexType name="SimpleProductOffering">
        <xsd:annotation>
          <xsd:documentation>DEPRECATED</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:choice minOccurs="0">
            <xsd:element name="serviceOffering" type="csom:ServiceOfferingReference"
               minOccurs="0"/>
            <xsd:element name="deviceOffering" type="csom:DeviceOfferingReference"
               minOccurs="0"/>
            <xsd:element name="ratePlanOffering" type="csom:RatePlanOfferingReference"
               minOccurs="0"/>
            <xsd:element name="accessoryOffering" type="csom:AccessoryOfferingReference"
               minOccurs="0"/>
            <xsd:element name="financialProductOffering" type="csom:FinancialProductOfferingReference"
               minOccurs="0"/>
            <xsd:element name="premiumContentOffering" type="csom:PremiumContentOfferingReference"
               minOccurs="0"/>
            <xsd:element name="otherProductOffering" type="csom:OtherProductOfferingReference"
               minOccurs="0"/>
          </xsd:choice>
          <xsd:element name="offeringPrice" type="csom:PriceListLine" minOccurs="0"/>
          <xsd:element name="productSpecification" type="csom:ProductSpecificationBase"
             minOccurs="0"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="person" type="csom:Person">
        <xsd:annotation>
          <xsd:documentation>A person is an individual human acting as a separate legal entity.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="searchOrderRequest">
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="base:BaseRequest">
              <xsd:sequence>
                <xsd:element name="searchCriteria" minOccurs="0">
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="orderId" type="csom:OrderId" minOccurs="0"/>
                      <xsd:element name="orderType" type="csom:OrderType" minOccurs="0"/>
                      <xsd:element name="orderStatus" type="csom:OrderStatus"
                         minOccurs="0"/>
                      <xsd:element name="orderDateRanges" minOccurs="0">
                        <xsd:complexType>
                          <xsd:sequence>
                            <xsd:element name="creationDate" minOccurs="0">
                              <xsd:complexType>
                                <xsd:attribute name="from" type="xsd:date"/>
                                <xsd:attribute name="to" type="xsd:date"/>
                              </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="scheduledDate" minOccurs="0">
                              <xsd:complexType>
                                <xsd:attribute name="from" type="xsd:date"/>
                                <xsd:attribute name="to" type="xsd:date"/>
                              </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="startExecutionDate" minOccurs="0">
                              <xsd:complexType>
                                <xsd:attribute name="from" type="xsd:date"/>
                                <xsd:attribute name="to" type="xsd:date"/>
                              </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="completionDate" minOccurs="0">
                              <xsd:complexType>
                                <xsd:attribute name="from" type="xsd:date"/>
                                <xsd:attribute name="to" type="xsd:date"/>
                              </xsd:complexType>
                            </xsd:element>
                          </xsd:sequence>
                        </xsd:complexType>
                      </xsd:element>
                      <xsd:element name="orderClassification" type="xsd:token"
                         minOccurs="0"/>
                      <xsd:element name="lineOfService" type="csom:LineOfService"
                         minOccurs="0"/>
                      <xsd:element ref="csom:MSISDN" minOccurs="0"/>
                      <xsd:element name="portInIndicator" type="xsd:boolean"
                         minOccurs="0"/>
                      <xsd:element ref="csom:imei" minOccurs="0"/>
                      <xsd:element ref="csom:simNumber" minOccurs="0"/>
                      <xsd:element ref="csom:imsi" minOccurs="0"/>
                      <xsd:element name="sku" type="csom:SKU" minOccurs="0"/>
                      <xsd:element name="salesPerson" type="xsd:token" minOccurs="0"/>
                      <xsd:element name="agentId" type="xsd:token" minOccurs="0"/>
                      <xsd:element name="salesChannel" type="csom:SalesChannelReference"
                         minOccurs="0"/>
                      <xsd:element name="dealerCode" type="xsd:token" minOccurs="0"/>
                      <xsd:element name="storeId" type="csom:StoreId" minOccurs="0"/>
                      <xsd:element ref="csom:financialAccountNumber" minOccurs="0"/>
                      <xsd:element ref="csom:customerId" minOccurs="0"/>
                      <xsd:element name="customerName" type="csom:PersonName"
                         minOccurs="0"/>
                      <xsd:element ref="csom:phoneNumber" minOccurs="0"/>
                      <xsd:element ref="csom:emailAddress" minOccurs="0"/>
                      <xsd:element name="address" type="csom:ExtendedAddressCommunication"
                         minOccurs="0" maxOccurs="unbounded"/>
                      <xsd:element name="fulfillmentMethod" type="csom:Name"
                         minOccurs="0"/>
                      <xsd:element name="carrierTrackingNumber" type="xsd:token"
                         minOccurs="0"/>
                      <xsd:element ref="csom:ssn" minOccurs="0"/>
                      <xsd:element name="taxId" type="xsd:token" minOccurs="0"/>
                      <xsd:element name="organizationName" type="xsd:token"
                         minOccurs="0"/>
                      <xsd:element name="createdBy" type="xsd:token" minOccurs="0"/>
                      <xsd:element name="paymentMethod" type="csom:PaymentMethod"
                         minOccurs="0"/>
                      <xsd:element name="cardLast4" type="csom:CardNumber" minOccurs="0"/>
                      <xsd:element name="terminalID" type="xsd:token" minOccurs="0"/>
                      <xsd:element name="transactionId" type="xsd:token" minOccurs="0"/>
                      <xsd:element name="receiptNumber" type="xsd:token" minOccurs="0"/>
                      <xsd:element name="fraudCheckStatus" type="csom:FraudCheckStatus"
                         minOccurs="0"/>
                      <xsd:element name="username" type="xsd:token" minOccurs="0"/>
                      <xsd:element name="pin" type="xsd:token" minOccurs="0"/>
                      <xsd:element name="issueNumber" type="xsd:token" minOccurs="0"/>
                    </xsd:sequence>
                  </xsd:complexType>
                </xsd:element>
                <xsd:element name="searchCart" type="xsd:boolean" minOccurs="0"/>
                <xsd:element name="resultSize" type="xsd:positiveInteger" minOccurs="0"/>
                <xsd:element name="pageNumber" type="xsd:positiveInteger" minOccurs="0"/>
                <xsd:element name="sortField" type="xsd:token" minOccurs="0"/>
              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="financialAccountId" type="csom:FinancialAccountId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies the Customer's financial (billing) account within the billing system.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ssn" type="csom:SSN">
        <xsd:annotation>
          <xsd:documentation>Social Security Number for this person, used for tax identification.   When used in a request, it must only be sent by secure transport. When used in a response, it should be blank or contain at most the last four digits.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="getOrderDetailsRequest">
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="base:BaseRequest">
              <xsd:sequence>
                <xsd:element name="orderId" type="csom:OrderId"/>
              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="getOrderDetailsResponse">
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="base:BaseResponse">
              <xsd:sequence>
                <xsd:element name="order" type="csom:OrderExtension" minOccurs="0"/>
              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="customer" type="csom:Customer">
        <xsd:annotation>
          <xsd:documentation>Details concerning the person or organization that is a customer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="simNumber" type="csom:SIMnumber"/>
      <xsd:element name="searchOrderResponse">
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="base:BaseResponse">
              <xsd:sequence>
                <xsd:element name="order" type="csom:OrderExtension" minOccurs="0"
                   maxOccurs="unbounded"/>
                <xsd:element name="totalResultCount" type="xsd:positiveInteger"
                   minOccurs="0"/>
                <xsd:element name="isSearchComplete" type="xsd:boolean" minOccurs="0"/>
                <xsd:element name="currentPage" type="xsd:positiveInteger" minOccurs="0"/>
              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="organization" type="csom:Organization">
        <xsd:annotation>
          <xsd:documentation>A generic organization: partner, vendor, supplier, B2B customer, corporation, etc.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="customerId" type="csom:CustomerId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies the customer party.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="postalCode" type="csom:PostalCode">
        <xsd:annotation>
          <xsd:documentation>A code specifying the postal code of the address</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="order" type="csom:Order">
        <xsd:annotation>
          <xsd:documentation>An order placed by the customer for goods and/or services. May imply the creation of a new customer account. 
    Potentially includes activation of new or additional lines, handset upgrade, change rate plan, change SIM, change MSISDN, port ins, etc.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="phoneNumber" type="csom:PhoneNumber">
        <xsd:annotation>
          <xsd:documentation>The complete number without separation between 	country code, area code etc
    							</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="getOrderLogRequest">
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="base:BaseRequest">
              <xsd:sequence>
                <xsd:element name="orderLogMilestoneRequest">
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="orderId" type="csom:OrderId"/>
                      <xsd:element name="transactionDateRange" type="csom:TimePeriod"
                         minOccurs="0"/>
                      <xsd:element name="resultSize" type="xsd:integer" minOccurs="0"/>
                    </xsd:sequence>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="orderLine" type="csom:OrderLine">
        <xsd:annotation>
          <xsd:documentation>Order lines provide the details of the product offerings being ordered. An order line may be for a product assembly or a component. 
    Additional order lines for components may be associated with the order line for the assembly.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="paymentProfile" type="csom:PaymentProfile">
        <xsd:annotation>
          <xsd:documentation>Specifies the Payment Profile to be used. If the Payment Profile is defined elsewhere, for instance in the Customer's Payment Profile, this may simply refer to it by id.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="pairing" type="csom:Pairing">
        <xsd:annotation>
          <xsd:documentation>Holds the network resource pairing and locking attributes.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="salesChannel" type="csom:SalesChannel">
        <xsd:annotation>
          <xsd:documentation>Describes the manner in which contact is made between T-Mobile and the customer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="getOrderLogResponse">
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="base:BaseResponse">
              <xsd:sequence>
                <xsd:element name="orderLogMilestones" minOccurs="0">
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="orderLogMilestone" type="csom:OrderStatus"
                         minOccurs="0" maxOccurs="unbounded"/>
                    </xsd:sequence>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="networkResource" type="csom:NetworkResource">
        <xsd:annotation>
          <xsd:documentation>SIM, mobile number, or IMEI used by the customer to gain access to services</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="imei" type="csom:IMEI">
        <xsd:annotation>
          <xsd:documentation>International Mobile Equipment Identifier.  Unique identifier for a mobile device on a network.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="emailAddress" type="csom:EmailAddress">
        <xsd:annotation>
          <xsd:documentation>URI in the case of email communication
    							</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="billingSystem" type="csom:BillingSystemCode">
        <xsd:annotation>
          <xsd:documentation>The T-Mobile billing system that is involved in the transaction.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="imsi" type="csom:IMSI">
        <xsd:annotation>
          <xsd:documentation>An International Mobile Subscriber Identifier of IMSI is a unique number associated with all GSM and UMTS network mobile phone users.  It is stored in the SIM card.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MSISDN" type="csom:MSISDN">
        <xsd:annotation>
          <xsd:documentation>Mobile Subscriber Integrated Services Digital Network Number. A number uniquely identifying a subscription in a GSM or a UMTS mobile network. Simply put, it is the telephone number to the SIM card in a mobile/cellular phone.
    			</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="lineOfService" type="csom:LineOfService">
        <xsd:annotation>
          <xsd:documentation>An arrangement for making use of services on a continuing or periodic basis. It serves as the collection point for the record of service offerings and resources that enable a line of service.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="financialAccountNumber" type="csom:FinancialAccountNumber">
        <xsd:annotation>
          <xsd:documentation>Customer Billing Account Number</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:simpleType name="SerialNumber">
        <xsd:restriction base="xsd:normalizedString"/>
      </xsd:simpleType>
      <xsd:simpleType name="SalesChannelCode">
        <xsd:annotation>
          <xsd:documentation>Code defining the manner in which contact is made between T-Mobile and the customer.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="StoreId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies a store location.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="SKU">
        <xsd:annotation>
          <xsd:documentation>Stock Keeping Unit for inventory purposes.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:maxLength value="50"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="OrderType">
        <xsd:annotation>
          <xsd:documentation>Identifies the type of this Order.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="OrderId">
        <xsd:annotation>
          <xsd:documentation>Enterprise-wide unique identifier for any order.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="LineOfServiceId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies a line of service</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="DealerId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies a dealer</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:complexType name="ChargeDistributionStatus">
        <xsd:annotation>
          <xsd:documentation>Condition of a ChargeDistribution, at the time expressed in the effectivePeriod.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the object, at the point in time specified by the effectivePeriod.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A finer division of code.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:simpleType name="DiscountRealizationMethod">
        <xsd:annotation>
          <xsd:documentation>Defines the way a discount is delivered.  For example by gift card, rebate.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="Rebate">
            <xsd:annotation>
              <xsd:documentation>The discount is given in as a rebate directly from a third party to a customer.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="GiftCard">
            <xsd:annotation>
              <xsd:documentation>The discount is given in as an open-loop gift card, such as a prepaid visa card.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Voucher">
            <xsd:annotation>
              <xsd:documentation>The discount is given in as an TMUS voucher, also known as a refill card.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="ProgramMembership">
        <xsd:annotation>
          <xsd:documentation>Program(s) in which the customer is participating, e.g. Advantage, Lifeline, TMUS Employee, Rural Healthcare, etc.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="programCode" type="csom:ProgramCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Code in common use to identify the Program to which the account belongs.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="csom:Description" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Describes the program membership.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="effectivePeriod" type="csom:EffectivePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The effective period during which the program membership applies.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="csom:StatusBase" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The status of the program membership.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="note" type="csom:Note" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Notes about the customer's program memebership.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="partyRelationship" type="csom:PartyRelationship"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Party relationship upon which the program membership is based.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="AccountHolder">
        <xsd:complexContent>
          <xsd:extension base="csom:CustomerReference">
            <xsd:attribute name="accountHolderId" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>Unique identifier of the account holder.  Assigned by the system of record.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="CustomerAccount">
        <xsd:annotation>
          <xsd:documentation>The entity that captures all aspects of a customer's account including care, financial, ordering, billing, etc. 
    May include multiple financial accounts that are the resopnsibility of this customer.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:CustomerAccountBase">
            <xsd:sequence minOccurs="0">
              <xsd:element name="financialAccount" type="csom:FinancialAccountReference"
                 minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="CustomerAccountBase">
        <xsd:annotation>
          <xsd:documentation>The Customer Account captures all aspects of a customer's account including care, financial, ordering, billing, etc. </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:CustomerAccountReference">
            <xsd:sequence minOccurs="0">
              <xsd:element name="accountHolder" type="csom:AccountHolder" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The owner of the account.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="currencyCode" type="csom:CurrencyCode" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation> 
    Default Billing Currency for this financial account
                                    </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="accountTenure" type="csom:Tenure" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Duration of the life of the account.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="personalGuarantor" type="csom:PersonalGuarantor"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>A personal guarantor is a party that has guarateed payment of another party's account, enabling that other party to establish an account with T-Mobile based on the credit profile of the personal guarantor rather than on the credit of the account holder.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element ref="csom:paymentProfile" minOccurs="0" maxOccurs="unbounded"/>
              <xsd:element name="balanceSummaryGroup" type="csom:BalanceSummaryGroup"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>A named group of account balances categorized by type of balance. Aggregated to the Customer Account level.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="preferredCommunicationChannel" type="csom:PreferredCommunicationChannel"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>A preferred type of communication for the specified usage context, ranked according to preference.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="preferredLanguage" type="csom:PreferredLanguage"
                 minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The preferred language of communication in the specified usage context. ISO 639-1 Code (en-US,en-CA,en-GB)</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="effectivePeriod" type="csom:EffectivePeriod"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The time period when the customerAccount is active.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="eSignConsentIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates whether the customer has agreed to allow electronic signatures for agreements and transactions related to their account.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="fraudIndicator" type="csom:Indicator" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Fraud indicator.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="fraudLock" type="csom:Indicator" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>This flag allows setting of fraud lock at customer account level. This must prevent any changes at the customer account level and all billing accounts and contracts under the customer account.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="idVerificationIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>ID Verification will be for accounts that have not yet run credit and will be an enabler attribute so that in the future customers who have not run credit can have their ID validated systemically,  we can then offer additional services such as Simple Global (B52) offerings. If a customer does choose to run credit instead of just offering an ID, this will remain a No as it is specifically speaking to a Systemic ID Verification process.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="corporateAffiliationProgram" type="xsd:token"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The name of the Corporate Affiliation Program with which the Customer Account is associated.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="strategicAccountIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates whether the customer account is considered strategic.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="preferredTimeToCall" type="xsd:normalizedString"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The time period when the account holder would prefer to receive calls.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="paymentNotificationIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates whether the customer should be notified when a payment is made.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="accountChangeNotificationIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates whether the customer should be notified when the account is changed.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="accountStatusNotificationIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates whether the customer should be notified when the account status is changed.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="cpniNotificationIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates whether the customer should be notified when CPNI is changed.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="accountSupportIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates whether the account receives special support.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="incidentTrackingIndicator" type="csom:Indicator"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Indicates whether the account is currently involved in incident tracking.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="fraudLockEffectivePeriod" type="csom:EffectivePeriod"
                 minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Specifies the time period during which the customer's accounts are (or were) locked for suspected fraud.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="PersonalGuarantor">
        <xsd:complexContent>
          <xsd:extension base="csom:CustomerReference">
            <xsd:sequence minOccurs="0">
              <xsd:element name="creditProfile" type="csom:CreditProfile" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The credit profile of the personal guarantor.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:element name="customerAccountId" type="csom:CustomerAccountId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies the Customer Account within the billing system.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:simpleType name="ProgramCode">
        <xsd:annotation>
          <xsd:documentation>Code in common use to identify the Program to which the account belongs.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="CustomerAccountId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies the Customer Account within the billing system.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="AccountClassification">
        <xsd:annotation>
          <xsd:documentation>Classifies the Account for processing and segmentation purposes.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="BillingMethod">
        <xsd:annotation>
          <xsd:documentation>The Billing Method specifies the billing experience that the customer has qualified for and requested.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="BillInAdvance">
            <xsd:annotation>
              <xsd:documentation>Customer is billed on a recurring basis for the coming period; services are provided on good faith.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="PayInAdvance">
            <xsd:annotation>
              <xsd:documentation>Customer is billed on a recurring basis, but payment must be recieved before services are rendered.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="OnDemand">
            <xsd:annotation>
              <xsd:documentation>A non-recurring billing cycle for prepaid.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="BillCurrent"/>
          <xsd:enumeration value="BillInArrears"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="CustomerAccountNumber">
        <xsd:annotation>
          <xsd:documentation>Customer Account Number is the public identifer for the account that represents the sum of all of the business that a given customer is doing with our company.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="BillingSystemCode">
        <xsd:annotation>
          <xsd:documentation>The T-Mobile billing system that is involved in the transaction.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="RPX"/>
          <xsd:enumeration value="WHOLESALE"/>
          <xsd:enumeration value="SAMSON"/>
          <xsd:enumeration value="ACC"/>
          <xsd:enumeration value="MVNEACC"/>
          <xsd:enumeration value="METROPCS">
            <xsd:annotation>
              <xsd:documentation>The legacy Metro PCS billing system.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="TBILL">
            <xsd:annotation>
              <xsd:documentation>Ericsson CBiO stack (BSCS).</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="SegmentationId">
        <xsd:annotation>
          <xsd:documentation>Segmentation Id represents the customer active data center; this is used to route messages to appropriate data center. All financial accounts under customer will be at the same data center.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:complexType name="ShipToContact">
        <xsd:annotation>
          <xsd:documentation>Name and address of the party to whom the order is to be shipped.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:Contact">
            <xsd:sequence minOccurs="0">
              <xsd:element name="useForBillTo" type="xsd:boolean" minOccurs="0"/>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:simpleType name="ServiceLevelCode">
        <xsd:annotation>
          <xsd:documentation>Shipping carrier priority code such as 2nd day, next day, priority, etc.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="2ND DAY"/>
          <xsd:enumeration value="NEXT DAY"/>
          <xsd:enumeration value="PRIORITY"/>
          <xsd:enumeration value="STORE PICKUP"/>
          <xsd:enumeration value="GROUND"/>
          <xsd:enumeration value="SATURDAY"/>
          <xsd:enumeration value="EXPRESS"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="FreightCarrier">
        <xsd:restriction base="xsd:normalizedString">
          <xsd:enumeration value="DHL"/>
          <xsd:enumeration value="FEDEX"/>
          <xsd:enumeration value="USPS"/>
          <xsd:enumeration value="UPS"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ModeOfTransportCode">
        <xsd:annotation>
          <xsd:documentation>Shipping method such as air, sea, or ground.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="AIR"/>
          <xsd:enumeration value="SEA"/>
          <xsd:enumeration value="GROUND"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="FOBPointCode">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="FOB Origin">
            <xsd:annotation>
              <xsd:documentation>Buyer is responsible for freight charges. Buyer owns goods in transit.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="FOB Destination">
            <xsd:annotation>
              <xsd:documentation>Seller is responsible for freight charges. Seller owns goods in transit.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="FOB Origin Freight Collect">
            <xsd:annotation>
              <xsd:documentation>Buyer pays and bears freight charges. Buyer owns goods in transit.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="FOB Origin Freight Prepaid">
            <xsd:annotation>
              <xsd:documentation>Seller pays and bears freight charges. Buyer owns goods in transit.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="FOB Origin Freight Prepaid and Add">
            <xsd:annotation>
              <xsd:documentation>Seller pays and invoices Buyer for freight charges. Buyer owns goods in transit.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="FOB Destination Freight Collect">
            <xsd:annotation>
              <xsd:documentation>Buyer pays and bears the freight charges. Seller owns goods in transit.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="FOB Destination Freight Prepaid">
            <xsd:annotation>
              <xsd:documentation>Seller pays and bears the freight charges. Seller owns goods in transit.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="FOB Destination Freight Collect and Allowed">
            <xsd:annotation>
              <xsd:documentation>Buyer pays freight charges and deducts the amount from Seller's invoice. Seller owns goods in transit.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="FOB Destination Freight Prepaid and Add">
            <xsd:annotation>
              <xsd:documentation>Seller pays the freight and adds the freight charges to its invoice to the Buyer. Seller owns goods in transit.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="FreightTermCode">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="EXW">
            <xsd:annotation>
              <xsd:documentation>Ex Works - Seller makes goods available at the seller's premises.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="FCA">
            <xsd:annotation>
              <xsd:documentation>Free Carrier - Seller delivers goods to freight carrier's place of operaiton</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="CPT">
            <xsd:annotation>
              <xsd:documentation>Carriage Paid To</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="CIP">
            <xsd:annotation>
              <xsd:documentation>Carriage and Insurance Paid</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="DAT">
            <xsd:annotation>
              <xsd:documentation>Delivered At Terminal</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="DAP">
            <xsd:annotation>
              <xsd:documentation>Delivered At Place</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="DDP">
            <xsd:annotation>
              <xsd:documentation>Delivered Duty Paid</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ShippingId">
        <xsd:annotation>
          <xsd:documentation>Identifier of shipping information</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="CommissionsCode">
        <xsd:annotation>
          <xsd:documentation>The commissions code may be a dealer code or a rep id, depending on the context.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:complexType name="BirthPlace">
        <xsd:annotation>
          <xsd:documentation>Place of birth for the person.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="birthCountryCode" type="csom:CountryCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    Country of birth. Validate from lookup.
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="birthRegionName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
    						State/Province of the birth country.
    					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="birthCityName" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation> 
    City of birth. 
                                    </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="SSN">
        <xsd:annotation>
          <xsd:documentation>Social Security Number for this person, used for tax identification.  When used in a request, it must only be sent by secure transport. When used in a response, it should be blank or contain at most the last four digits.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:token">
            <xsd:attribute name="maskingType" type="csom:MaskingType">
              <xsd:annotation>
                <xsd:documentation>The type of masking to be applied to a social security number for security and privacy.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="IdentityDocumentVerification">
        <xsd:annotation>
          <xsd:documentation>A record of the verification of an identity document.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="id" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identification number of the identity document.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="identityDocumentType" type="csom:IdentityDocumentType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Type of identity document verified.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="validPeriod" type="csom:DatePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date range during which the identity document was valid.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="verificationEvent" type="csom:AuditTrail" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Information about when and where the id verification took place.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="verificationStatus" type="csom:VerificationStatus"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Identity document verification status.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="VerificationStatus">
        <xsd:annotation>
          <xsd:documentation>The status of the verification, and reasons for the result.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode"/>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="VisaStatus">
        <xsd:annotation>
          <xsd:documentation>Condition of a Visa, at the time expressed in the effectivePeriod.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the object, at the point in time specified by the effectivePeriod.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A finer division of code.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="PersonReference">
        <xsd:annotation>
          <xsd:documentation>Identifying information about the person. For more information, use SecurityProfile or Person structures.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="userName" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Logon identifier for the salesperson.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="personName" type="csom:PersonName" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Name of the salesperson.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="PersonId">
        <xsd:annotation>
          <xsd:documentation>Enterprise-wide unique identifier of a Person.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:simpleType name="IdentityDocumentType">
        <xsd:annotation>
          <xsd:documentation>Defines the types of personal identity documents that are accepted.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="MILITARY"/>
          <xsd:enumeration value="TRIBAL"/>
          <xsd:enumeration value="STATE"/>
          <xsd:enumeration value="VOTER"/>
          <xsd:enumeration value="ALIEN"/>
          <xsd:enumeration value="DISABILITY">
            <xsd:annotation>
              <xsd:documentation>Person with a disability identification card.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="FEDERAL"/>
          <xsd:enumeration value="MATRICULACONSULAR"/>
          <xsd:enumeration value="PASSPORT"/>
          <xsd:enumeration value="SSN"/>
          <xsd:enumeration value="VISA"/>
          <xsd:enumeration value="DRIVERSLICENSE"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="EligibilityEvaluation">
        <xsd:annotation>
          <xsd:documentation>Result of evaulation of eligibility criteria, as specified in the eligibility type.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="eligibilityType" type="csom:EligibilityType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The type of eligibility being evaluated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="eligibilityDate" type="xsd:date" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date when eligibility was evaluated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ineligibilityReason" type="csom:NamedText" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A reason that the evaluation of eligibility returned a negative result.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional information associated with the eligibility evaluation.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="overrideIndicator" type="csom:Indicator" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Override indicator</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="eligibilityStatus" type="csom:EligibilityStatusCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The resulting status of the eligibility evaluation.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="managerId" type="xsd:token" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Approving manager or supervisor identification</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="eligibilityPeriod" type="csom:DatePeriod" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date range of eligibility.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="EligibilityCriteria">
        <xsd:annotation>
          <xsd:documentation>The criteria that is used to determine eligibility for something.  For example EligibilityCriteria can be specified for an offer for an offer or a price.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="postalCodeCriteria" type="csom:PostalCodeCriteria"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The criteria requires the postal code be in the eligible list or not in the excluded list.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="countyCriteria" type="csom:CountyCriteria" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The criteria requires the county be in the eligible list or not in the excluded list.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="stateCriteria" type="csom:StateCriteria" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The criteria requires the state  be in the eligible list or not in the excluded list.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="countryCriteria" type="csom:CountryCode" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The criteria requires one of the countries in this list. If no country is specified, all countries are eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="billingTypeCriteria" type="csom:BillingTypeCode"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The criteria requires one of the settlement arrangements in this list.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="eligibleAccountTypeCriteria" type="csom:AccountType"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The criteria requires one of the account type/subtype combinations in this list. This criteria is supplied for backward compatibility with legacy systems, but customer segment criteria should be used instead for new rules.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="customerSegmentCriteria" type="csom:CustomerSegmentCriteria"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Customers in the included segments, but not in the excluded segments are eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="channelCriteria" type="csom:ChannelCriteria" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The criteria requires the channel be in the eligible list or not in the excluded list.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="creditWorthinessCriteria" type="csom:CreditWorthinessCriteria"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The criteria requires the credit worthiness be in the eligible list or not in the excluded list.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="cityCriteria" type="csom:CityCriteria" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The criteria requires the city be in the eligible list or not in the excluded list.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="storeCriteria" type="csom:StoreCriteria" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The criteria requires the store ID to be in the eligible list, or not in the excluded list. If neither list is populated, all stores are eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="programCode" type="csom:ProgramCode" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Lists the programs whose members are eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="accountClassification" type="csom:AccountClassification"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Lists the account classifications that are eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="customerGroup" type="csom:CustomerGroup" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Lists the customer groups that are eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="marketCode" type="csom:MarketCode" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A three-letter code that identifies an eligible market.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A group of named value pairs.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="tacCode" type="csom:TacCode" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Specifies a TAC Code that is eligible. The Type Allocation Code (TAC) is the initial eight-digit portion of the 15-digit IMEI and 16-digit IMEISV codes used to uniquely identify wireless devices.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="billingMethod" type="csom:BillingMethod" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The Billing Method specifies the billing experience that the customer has qualified for and requested.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="orderTypeCriteria" type="csom:OrderTypeCriteria"
             minOccurs="0"/>
          <xsd:element name="contractTypeCriteria" type="csom:ContractTypeCriteria"
             minOccurs="0"/>
          <xsd:element name="dealerCodeCriteria" type="csom:DealerCodeCriteria"
             minOccurs="0"/>
          <xsd:element name="lineLimitCriteria" type="csom:LineLimitCriteria"
             minOccurs="0"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="EligibilityType">
        <xsd:annotation>
          <xsd:documentation>The type of eligibility being evaluated.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:complexType name="OtherProductOfferingReference">
        <xsd:annotation>
          <xsd:documentation>DEPRECATED</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:SimpleProductOfferingReference"/>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="EmailCommunicationStatus">
        <xsd:annotation>
          <xsd:documentation>Condition of the EmailCommunication, at the time expressed in the effectivePeriod.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the object, at the point in time specified by the effectivePeriod.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="VALIDATED"/>
                  <xsd:enumeration value="NONVALIDATED"/>
                  <xsd:enumeration value="DELIVERED"/>
                  <xsd:enumeration value="UNDELIVERABLE"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode">
              <xsd:annotation>
                <xsd:documentation>A finer division of code.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="OVERWRITTEN"/>
                  <xsd:enumeration value="HARDBOUNCE"/>
                  <xsd:enumeration value="SOFTBOUNCE"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="SocialCommunication">
        <xsd:complexContent>
          <xsd:extension base="csom:CommunicationBase">
            <xsd:sequence minOccurs="0">
              <xsd:element name="usageContext" type="xsd:token" minOccurs="0"
                 maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The Social Communication Usage Context specifies a way in which the social media should be used.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="communicationStatus" type="csom:SocialCommunicationStatus"
                 minOccurs="0"/>
              <xsd:element name="socialMediaType" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The social media type identifies the type of social media used by this communication channel.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                  <xsd:restriction base="xsd:token">
                    <xsd:enumeration value="FACEBOOK"/>
                    <xsd:enumeration value="TWITTER"/>
                    <xsd:enumeration value="GOOGLEPLUS"/>
                    <xsd:enumeration value="INSTAGRAM"/>
                    <xsd:enumeration value="YOUTUBE"/>
                    <xsd:enumeration value="FOURSQUARE"/>
                    <xsd:enumeration value="MYSPACE"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="socialMediaHandle" type="xsd:token" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The handle or identifiying token used by the person in communication via this social media channel.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="customerSentiment" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>The prevailing attitude of the person towards T-Mobile as expressed via this social media communication channel.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                  <xsd:restriction base="xsd:token">
                    <xsd:enumeration value="POSITIVE"/>
                    <xsd:enumeration value="NEGATIVE"/>
                    <xsd:enumeration value="NEUTRAL"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="PreferredCommunicationChannel">
        <xsd:annotation>
          <xsd:documentation>A preferred type of communication for the specified usage context, ranked according to preference.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="communicationType" type="csom:CommunicationType"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The type of communication for example EmailCommunication or PhoneCommunication</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="preference" type="csom:Preference" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Determines the relative preference of the communication channel as compared to others listed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>the Id of the communication channel</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="usageContext">
          <xsd:annotation>
            <xsd:documentation>Identifies the circumstances for which we are to use this communication channel as the preferred channel.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:token">
              <xsd:enumeration value="SECURITY"/>
              <xsd:enumeration value="USAGE"/>
              <xsd:enumeration value="ROAMING"/>
              <xsd:enumeration value="MARKETING"/>
              <xsd:enumeration value="ORDERING"/>
              <xsd:enumeration value="3RDPARTYPURCHASES"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="SocialCommunicationStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the object, at the point in time specified by the effectivePeriod.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>A finer division of code.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="WebsiteCommunicationStatus">
        <xsd:annotation>
          <xsd:documentation>Condition of the WebsiteCommunication, at the time expressed in the effectivePeriod.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:annotation>
                <xsd:documentation>A short hand identifier for the state of the object, at the point in time specified by the effectivePeriod.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="AVAILABLE"/>
                  <xsd:enumeration value="NOTFOUND"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode">
              <xsd:annotation>
                <xsd:documentation>A finer division of code.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="UNDERCONSTRUCTION"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="WebsiteCommunication">
        <xsd:complexContent>
          <xsd:extension base="csom:CommunicationBase">
            <xsd:sequence minOccurs="0">
              <xsd:element name="websiteURI" type="xsd:anyURI" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>URI or URL for the communication
    							</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="usageContext" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>The Website Usage Context specifies a way in which the website should be used.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                  <xsd:restriction base="xsd:token">
                    <xsd:enumeration value="PERSONAL"/>
                    <xsd:enumeration value="BUSINESS"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="communicationStatus" type="csom:WebsiteCommunicationStatus"
                 minOccurs="0"/>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:simpleType name="CommunicationChannelCode">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="PHONE"/>
          <xsd:enumeration value="EMAIL"/>
          <xsd:enumeration value="SMS"/>
          <xsd:enumeration value="FACEBOOK"/>
          <xsd:enumeration value="TWITTER"/>
          <xsd:enumeration value="LINKEDIN"/>
          <xsd:enumeration value="GOOGLEPLUS"/>
          <xsd:enumeration value="CHAT"/>
          <xsd:enumeration value="OTHER"/>
          <xsd:enumeration value="FOURSQUARE"/>
          <xsd:enumeration value="MYSPACE"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="AddressUsageContext">
        <xsd:annotation>
          <xsd:documentation>The Address Usage Context specifies a way in which the address should be used.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="HOME"/>
          <xsd:enumeration value="WORK"/>
          <xsd:enumeration value="MAILING"/>
          <xsd:enumeration value="SHIPPING"/>
          <xsd:enumeration value="BILLING"/>
          <xsd:enumeration value="PHYSICAL"/>
          <xsd:enumeration value="E911"/>
          <xsd:enumeration value="PPU"/>
          <xsd:enumeration value="SKIPTRACE"/>
          <xsd:enumeration value="PRIMARY"/>
          <xsd:enumeration value="PREVIOUS"/>
          <xsd:enumeration value="OTHER"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="EmailAddress">
        <xsd:restriction base="xsd:normalizedString"/>
      </xsd:simpleType>
      <xsd:simpleType name="CommunicationType">
        <xsd:annotation>
          <xsd:documentation>Type of communication, such as email, phone, sms, mail.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="EMAIL"/>
          <xsd:enumeration value="SMS"/>
          <xsd:enumeration value="MAIL"/>
          <xsd:enumeration value="PHONE"/>
          <xsd:enumeration value="PUSHNOTIFICATION"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="PhoneNumber">
        <xsd:restriction base="xsd:normalizedString"/>
      </xsd:simpleType>
      <xsd:complexType name="ClaimStatus">
        <xsd:annotation>
          <xsd:documentation>The status of the claim.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>The code identifying the status of the claim.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:simpleType name="ClaimId">
        <xsd:annotation>
          <xsd:documentation>Uniquely identifies the claim.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:complexType name="RelatedClassification">
        <xsd:annotation/>
        <xsd:sequence minOccurs="0">
          <xsd:element name="relatedClassificationId" type="xsd:token" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The Classification that this Classification is related to.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="relationshipEffectivePeriod" type="csom:EffectivePeriod"
             minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="relationshipCode" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Describes the type of relationship between the classifications. </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="Classification">
        <xsd:annotation/>
        <xsd:sequence minOccurs="0">
          <xsd:element name="key" type="csom:DomainKey" minOccurs="0"/>
          <xsd:element name="relatedClassification" type="csom:RelatedClassification"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Other Classification objects related to this instance.  A Classification may be have many different types of relationships to other Classifications.  </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="classificationCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The label that describes this category. For example the BillingArrangementType.  The  classificationCode is a governed value.  Recognized values of the classificationCode are specific to the object that uses the Classification.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="nameValue" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The classification attribute called Name.  Name is a user-friendly label for the classification value.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="descriptionValue" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The classification attribute called Description.  Description is an explanation of the classification value.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="codeIdValue" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The classification attribute called Code.  Code is a short-hand unique identifier of the classification.  The code id is only guaranteed to be unique within the scope of  a classificationCode.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="classificationAttribute" type="base:SpecificationValue"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional data attributes for this classification (other than the Name, Description, and Code);  for example SortOrder or LongDescription. Each attribute is represented as a Name/Value pair, therefore many Name/Value pairs can be attached to this object instance.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Unique identifier assigned by the system of record.
    				</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="PortStatus">
        <xsd:complexContent>
          <xsd:extension base="csom:StatusBase">
            <xsd:attribute name="statusCode">
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="New"/>
                  <xsd:enumeration value="InProgress"/>
                  <xsd:enumeration value="Error"/>
                  <xsd:enumeration value="Cancelled"/>
                  <xsd:enumeration value="Complete"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="subStatusCode">
              <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                  <xsd:enumeration value="ReadyToActivate"/>
                  <xsd:enumeration value="ReadyToDeactivate"/>
                  <xsd:enumeration value="ResolutionRequired"/>
                  <xsd:enumeration value="Concurrence"/>
                  <xsd:enumeration value="Jeopardy"/>
                  <xsd:enumeration value="ActivationComplete"/>
                  <xsd:enumeration value="DeactivationComplete"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:element name="port" type="csom:Port"/>
      <xsd:simpleType name="PortActionCode">
        <xsd:annotation>
          <xsd:documentation>The action code defines what should be done with the port in request. 
                </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="SUBMIT"/>
          <xsd:enumeration value="COMPLETE"/>
          <xsd:enumeration value="CANCEL"/>
          <xsd:enumeration value="FALLOUTUPDATE"/>
          <xsd:enumeration value="CUSTOMERUPDATE"/>
          <xsd:enumeration value="PORTDUEDATEUPDATE"/>
          <xsd:enumeration value="RESOLVE"/>
          <xsd:enumeration value="NOTIFY"/>
          <xsd:enumeration value="CONFIRM"/>
          <xsd:enumeration value="VALIDATE"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="VirtualServiceProvider">
        <xsd:annotation>
          <xsd:documentation>MVNO or other service partner.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token"/>
      </xsd:simpleType>
      <xsd:complexType name="PremiumContentOfferingReference">
        <xsd:annotation>
          <xsd:documentation>DEPRECATED A product offering for premium content, such as ring tones, Rhapsody, Lookout Premium.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="csom:ProductOfferingReference"/>
        </xsd:complexContent>
      </xsd:complexType>
    </xsd:schema>

    <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xml="http://www.w3.org/XML/1998/namespace"
       elementFormDefault="qualified" attributeFormDefault="unqualified" targetNamespace="http://services.tmobile.com/base"
       xmlns:base="http://services.tmobile.com/base">
      <xsd:annotation>
        <xsd:documentation>Common interface elements</xsd:documentation>
      </xsd:annotation>
      <xsd:complexType name="TargetSystemIdentity">
        <xsd:all>
          <xsd:element name="systemId" type="xsd:token"/>
          <xsd:element name="userId" type="xsd:token"/>
        </xsd:all>
      </xsd:complexType>
      <xsd:complexType name="Sender">
        <xsd:sequence>
          <xsd:element name="senderId" type="xsd:token">
            <xsd:annotation/>
          </xsd:element>
          <xsd:element name="channelId" type="xsd:token">
            <xsd:annotation/>
          </xsd:element>
          <xsd:element name="applicationId" type="xsd:token">
            <xsd:annotation/>
          </xsd:element>
          <xsd:element name="applicationUserId" type="xsd:token" minOccurs="0"/>
          <xsd:element name="sessionId" type="xsd:token" minOccurs="0">
            <xsd:annotation/>
          </xsd:element>
          <xsd:element name="workflowId" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
    						A value populated by the Sender used to track their workflow
    						within the confines of a session.
    						Senders are encouraged to develop a
    						useful scheme.
    						Do not use customer-sensitive information.
    							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="activityId" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
    						A value populated by the Sender used to track a unit of work
    						within a workflow.
    						Senders are
    						encouraged to develop a
    						useful scheme.
    						Do not use customer-sensitive information.
    							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="timestamp" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
    						A timestamp provided by Sender to track their workflow.
    						Usually set to the moment when a request
    						is made.
    					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="storeId" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
    						For tracking purposes only. Unique identifier for the store.
    					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="dealerCode" type="base:DealerCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>For tracking purposes only. Code that uniquely identifies the dealer.
    					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="scope" type="xsd:token" minOccurs="0">
            <xsd:annotation>
